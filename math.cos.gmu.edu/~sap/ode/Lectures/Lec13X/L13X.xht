                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   It follows that
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>n</mml:mi>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>n</mml:mi>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mi>a</mml:mi>
                 <mml:mrow>
                   <mml:mi>n</mml:mi>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mi>r</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>r</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mi>a</mml:mi>
                 <mml:mi>n</mml:mi>
               </mml:msub>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext mathcolor="black">&emsp;for&ensp;</mml:mtext>
       </mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>n</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>2</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>&hellip;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Finally
   solve for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mrow>
           <mml:mi>n</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mrow>
           <mml:mi>n</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>n</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>n</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>n</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>r</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>r</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mi>a</mml:mi>
           <mml:mi>n</mml:mi>
         </mml:msub>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Simplify the numerator as
   follows:<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mi>r</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>r</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi>n</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mi>n</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>r</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>r</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msup>
                         <mml:mi>n</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>r</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>n</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>r</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mi>r</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mi>r</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>n</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>r</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>n</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>r</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>n</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mi>r</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Then
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(13.24)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>a</mml:mi>
             <mml:mrow>
               <mml:mi>n</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
           </mml:msub>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mi>r</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>r</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>n</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mi>a</mml:mi>
               <mml:mi>n</mml:mi>
             </mml:msub>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext mathcolor="black">&emsp;for&ensp;</mml:mtext>
       </mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>n</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>2</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>&hellip;</mml:mi>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Observe that the coefficients
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   are not specified. Since the ODE must have a pair of linearly independent
   solutions, those coefficients will correspond to the arbitrary weights for the
   general solution. To see how this works we need to compute the remaining
   coefficients
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mn>3</mml:mn>
       </mml:msub>
       <mml:mo form="infix">,</mml:mo>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mn>4</mml:mn>
       </mml:msub>
       <mml:mo form="infix">,</mml:mo>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mn>5</mml:mn>
       </mml:msub>
       <mml:mo form="infix">,</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>a</mml:mi>
           <mml:mrow>
             <mml:mn>6</mml:mn>
             <mml:mtext>,</mml:mtext>
           </mml:mrow>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mi>a</mml:mi>
           <mml:mn>7</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>&hellip;</mml:mi>
     </mml:mrow>
   </mml:math>
   from the recursion relation Eqn. (13.24). Calculate
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="center center center center center center left center left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>n</mml:mi>
               <mml:mo form="infix">=</mml:mo>
