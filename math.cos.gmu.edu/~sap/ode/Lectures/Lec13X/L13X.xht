                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&mu;</mml:mi>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mi>a</mml:mi>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi>&mu;</mml:mi>
                       <mml:mo>&InvisibleTimes;</mml:mo>
