                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&plusmn;</mml:mo>
               <mml:msqrt>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mn>5</mml:mn>
                 </mml:mrow>
               </mml:msqrt>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo form="infix">&plusmn;</mml:mo>
               <mml:msqrt>
                 <mml:mn>16</mml:mn>
               </mml:msqrt>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&plusmn;</mml:mo>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>With
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&alpha;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&beta;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>2</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the basis on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&infin;</mml:mi>
     </mml:mrow>
   </mml:math>
   is given by 
   
<span class="QQQHyperLink-BodyText"><a href="#Case1b"> <span class="QQQHyperLink-BodyText">Case 1b</span></a></span>    
   
  
</p>
