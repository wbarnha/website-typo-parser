<!-- MSISEMCOM \FRAME{itbpF}{0.7766in}{0.7766in}{0in}{}{}{lighthouse10.png}{\special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 0.7766in;  height 0.7766in;  depth 0in;  original-width 0.8518in;  original-height 0.8518in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "11";  file-properties "XNPEU"; }} -->
