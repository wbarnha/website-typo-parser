                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>or
   in component form
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&theta;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>c</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="left">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>&theta;</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>&omega;</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>&pi;</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>c</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>&theta;</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>&omega;</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
