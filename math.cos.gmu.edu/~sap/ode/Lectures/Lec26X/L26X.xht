                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:msup>
                         <mml:mi>u</mml:mi>
                         <mml:mn>3</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mrow>
                             <mml:mn>4</mml:mn>
                             <mml:mo form="postfix">!</mml:mo>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mrow>
                             <mml:msup>
                               <mml:mi>d</mml:mi>
                               <mml:mn>4</mml:mn>
                             </mml:msup>
                             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>h</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                             <mml:mi>d</mml:mi>
                             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:msup>
                               <mml:mi>u</mml:mi>
                               <mml:mn>4</mml:mn>
                             </mml:msup>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:msup>
                         <mml:mi>u</mml:mi>
