   </mml:math><span class="QQQSansSerifItalic16">for
   all</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&Element;</mml:mo>
       <mml:mi>I</mml:mi>
     </mml:mrow>
   </mml:math>.
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <span class="QQQSansSerifItalic16">Given</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:msup>
           <mml:mi fontstyle="upright">&Ropf;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">we say that</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a
   </span><span class="QQQSansSerifBoldItalic16">solution to the initial value
   problem
   (IVP)</span><mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(26.3)</mtext></mtd>
	    <mml:mrow>

       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mspace width="negativethinmathspace" />
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">h</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mi>t</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mn>0</mml:mn>
         </mml:msub>
       </mml:mrow>

      </mml:mrow>
	 </mlabeledtr>
    </mtable>	

   </mml:math><span class="QQQSansSerifItalic16">on
   an interval</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>I</mml:mi>
       <mml:mo form="infix">:</mml:mo>
       <mml:mrow>
         <mml:mi>&alpha;</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&beta;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">containing</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">provided</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a solution to</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math><strong>h</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:msub>
           <mml:mi>t</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQSpecialText">
   <hr class="hrulefill" />
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <br/>We can interpret Eqn. (26.2) as a pair of parametric equations that trace
   out a curve in the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>-plane.
   Each value of the parameter
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   generates a point in the plane with coordinates
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mrow>
             <mml:mi>y</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>Like the
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec10X/L10X.xht#Theorem10.03"> <span class="QQQHyperLink-BodyText">autonomous
   scalar ODE</span></a></span>, the time translates of a solution to an
   autonomous system are themselves solutions to that system. This important
   result is rephrased in mathematically precise terms in the following theorem.
   For simplicity we will assume its interval of definition is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Its proof is left as an exercise.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="theorem">
   <span class="leadin">Theorem</span><p class="theorem-FirstPara">
      26.2 [Time Translations of Solutions are Solutions]:<a id="Theorem26.02"></a>
   </p>
</div>
<p>
   <br/><span class="QQQSansSerifItalic16">Suppose</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a solution to</span>
</p>
<p class="QQQTheoremText">
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(26.4)</mtext></mtd>
	    <mml:mrow>

       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">h</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>

      </mml:mrow>
	 </mlabeledtr>
    </mtable>	

   </mml:math>
</p>
<p class="QQQTheoremText">
   <br/>
</p>
<p>
   <span class="QQQSansSerifItalic16">on</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">Then for every</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>s</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>s</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a solution to Eqn. (26.4) on</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">It follows that</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>s</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">define the same orbit.</span>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p class="QQQSpecialText">
   <strong>The orbit of an autonomous system does not depend on when the clock is
   started. Shifting the initial time
   </strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong> does
   not change the orbit. Think of the path you take on your daily run. If your
   pace is identical each day and you take the same route, then no matter what
   time you start, at
   </strong><mml:math display="inline">
     <mml:mrow>
       <mml:mi>m</mml:mi>
     </mml:mrow>
   </mml:math>
   <strong>minutes into your run, you will always be at the same point on your
   route each day. </strong>
</p>
<p>
   <br/>
</p>
<p>
   Like
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec02X/L02X.xht#EU"> <span class="QQQHyperLink-BodyText">scalar
   ODEs</span></a></span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   or
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec23X/L23X.xht#EU"> <span class="QQQHyperLink-BodyText">linear
   systems</span></a></span><strong> </strong><mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>Ar</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   existence and uniqueness of an IVP must be established before we investigate
   solution behavior. Typically, nonlinear systems do not admit solution formulas
   for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>y</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Even though we cannot write down such formulas, the following theorem will
   guarantee unique solutions for all the examples and applications in this
   Lecture 26 (this lecture) and Lecture 27. (See
   <span class="QQQNote-BodyText"> <a target="_blank" href="L26X__note00.xht"><span class="QQQNote-BodyText">Perko</span></a> </span> for a proof.)
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="theorem">
   <span class="leadin">Theorem</span><p class="theorem-FirstPara">
      26.3 [Existence and Uniqueness - E&amp;U]<a id="EU"></a>
   </p>
</div>
<p>
   <br/><span class="QQQSansSerifItalic16">Suppose</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>g</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and their partial derivatives</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>f</mml:mi>
         <mml:mi>x</mml:mi>
       </mml:msub>
       <mml:mo form="infix">,</mml:mo>
       <mml:msub>
         <mml:mi>f</mml:mi>
         <mml:mi>y</mml:mi>
       </mml:msub>
       <mml:mo form="infix">,</mml:mo>
       <mml:msub>
         <mml:mi>g</mml:mi>
         <mml:mi>x</mml:mi>
       </mml:msub>
       <mml:mo form="infix">,</mml:mo>
       <mml:msub>
         <mml:mi>g</mml:mi>
         <mml:mi>y</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">are defined and continuous
   throughout</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">Given any</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&Element;</mml:mo>
       <mml:msup>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifItalic16">,
   the IVP</span>
</p>
<p class="QQQCenteredBodyText">
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mover accent="true">
                   <mml:mi>x</mml:mi>
                   <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                 </mml:mover>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mi>f</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mi>y</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">,</mml:mo>
               <mml:mtext>&emsp;</mml:mtext>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:msub>
                       <mml:mi>t</mml:mi>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mover accent="true">
                   <mml:mi>y</mml:mi>
                   <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                 </mml:mover>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mi>g</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mi>y</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">,</mml:mo>
               <mml:mtext>&emsp;</mml:mtext>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>y</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:msub>
                       <mml:mi>t</mml:mi>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:msub>
                   <mml:mi>y</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifItalic16">or
   in vector
   form</span><span style="width:1em"><span style="padding-left:1em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math><strong>h</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
     </mml:mrow>
   </mml:math><strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:msub>
           <mml:mi>t</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <span class="QQQSansSerifItalic16">has a unique solution defined on some
   interval</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>I</mml:mi>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">containing</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>.
   <span class="QQQSansSerifItalic16">(A system that satisfies these continuity
   assumptions is called <strong>smooth</strong>.)</span>
</p>
<p class="QQQTheoremText">
   <br/>
</p>
<p class="QQQTheoremText">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>Not withstanding the absence of solution formulas, the easiest way to
   view the behavior of a two-dimensional autonomous system is to create a
   direction field and plot some representative integral curves.
</p>
<p>
   <br/>
</p>
<p class="QQQSubHead1">
   <br/>26.2 EQUILIBRIUM SOLUTIONS OF NONLINEAR SYSTEMS
</p>
<p>
   <br/>Many of the same features of autonomous two-dimensional systems mimic
   those of scalar autonomous ODEs as developed in Lecture 10.
    One such feature is that of an
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec10X/L10X.xht#EquilibriumSolutions"> <span class="QQQHyperLink-BodyText">equilibrium solution
  </span></a></span>, a solution
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   which satisfies
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   for all
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   As this implies no change in
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mspace width="thickmathspace" />
     </mml:mrow>
   </mml:math>it
   follows that an equilibrium solution to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is a constant solution and calculated by solving
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>The same idea extends to <strong>autonomous</strong> two-dimensional
   systems.
</p>
<p>
   <br/>
</p>
<p>
   <a id="Definition26.04"></a>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="definition">
   <span class="leadin">Definition</span><p class="definition-FirstPara">
      26.4 [Equilibrium Solution - Equilibrium Point]:
   </p>
</div>
<p>
   <br/><span class="QQQSansSerifItalic16">An <strong>equilibrium
   solution</strong> to the autonomous two-dimensional system</span>
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(26.5)</mtext></mtd>
	    <mml:mrow>

       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">h</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>

      </mml:mrow>
	 </mlabeledtr>
    </mtable>	

   </mml:math>
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <span class="QQQSansSerifItalic16">is a constant vector function</span>
</p>
<p class="QQQCenteredBodyText">
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">:=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="left">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="infix">&midast;</mml:mo>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>y</mml:mi>
                   <mml:mo form="infix">&midast;</mml:mo>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifItalic16">for
   all</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">.</mml:mo>
       <mml:mspace width="thickmathspace" />
     </mml:mrow>
   </mml:math>
</p>
<p>
   <span class="QQQSansSerifItalic16">Consequently</span>
</p>
<p class="QQQCenteredBodyText">
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mstyle fontweight="bold" fontstyle="upright">
               <mml:mo form="infix">&sdot;</mml:mo>
             </mml:mstyle>
           </mml:mrow>
         </mml:mover>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>0</strong><span style="width:1em"><span style="padding-left:1em"></span></span><span class="QQQSansSerifItalic16">for
   all</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifItalic16">so
   that</span><span style="width:1em"><span style="padding-left:1em"></span></span><strong>h</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>0</strong>
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <span class="QQQSansSerifItalic16">As</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mo form="infix">&midast;</mml:mo>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">represents a point in the plane, we
   call</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">an <strong>equilibrium point</strong> of
   the system. Note that</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">at an equilibrium point.</span>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   The preceding definition allows us to think of the roots of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   in one of two ways:
</p>
<p>
   <br/>
</p>
<p>
   <span style="width:2em"><span style="padding-left:2em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math> Algebraically
   as the common roots of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   or
</p>
<p>
   <span style="width:2em"><span style="padding-left:2em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math> Dynamically
   as an equilibrium solution to the pair of ODEs
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>y</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <br/><a id="Example26.05"></a>
</p>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      26.5 [Equilibrium Points]:
   </p>
</div>
<p class="QQQExampleText">
   Determine the equilibrium points for each of the following two-dimensional
   systems. Use software to create a corresponding direction field that includes
   the equilibrium solutions and some integral curves.
</p>
<p class="QQQExampleText">
   <br/><span style="width:2em"><span style="padding-left:2em"></span></span>System
   (a)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>y</mml:mi>
                   <mml:mn>3</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span style="width:2em"><span style="padding-left:2em"></span></span>System
   (b)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>a</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mrow>
                         <mml:mn>4</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>x</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>y</mml:mi>
                       </mml:mrow>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:msup>
                           <mml:mi>x</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>b</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mi>y</mml:mi>
                             <mml:mrow>
                               <mml:mn>1</mml:mn>
                               <mml:mo form="infix">+</mml:mo>
                               <mml:msup>
                                 <mml:mi>x</mml:mi>
                                 <mml:mn>2</mml:mn>
                               </mml:msup>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math><span style="width:1em"><span style="padding-left:1em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mi>a</mml:mi>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   are positive constants
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution: </span>Calculate all roots
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   by solving
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   for each system.
</p>
<p class="QQQExampleText">
   <br/>(a) Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>y</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>3</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>y</mml:mi>
         <mml:mn>3</mml:mn>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   in
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   to get
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>y</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mroot>
         <mml:mn>1</mml:mn>
         <mml:mn>4</mml:mn>
       </mml:mroot>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&plusmn;</mml:mo>
         <mml:mn>1.</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   The corresponding values of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   are
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix">&plusmn;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mn>3</mml:mn>
       </mml:msup>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&plusmn;</mml:mo>
         <mml:mn>1.</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   There are two equilibrium points:
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>See
   Fig. 26.1(a) for their location and the behavior of some orbits.
</p>
<p class="QQQExampleText">
   <br/>(b)Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msup>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mi>y</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:msup>
                       <mml:mi>x</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Solve the both equations for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   the first equation yields
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>a</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msup>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:mn>4</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and the second equation yields
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>y</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Equate these two expressions for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>
   to obtain
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mi>a</mml:mi>
               <mml:mn>5</mml:mn>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It follows that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>y</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mi>a</mml:mi>
                     <mml:mn>5</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Thus the only equilibrium point is :
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mi>a</mml:mi>
                   <mml:mn>5</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:msup>
                       <mml:mi>a</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                     <mml:mn>25</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Fig. 26.1(b) shows the location of the single equilibrium point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>5</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>a</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>10</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>4</mml:mn>
     </mml:mrow>
   </mml:math>
   along with an orbit.<a id="Figure26.01"></a>
</p>
<p class="QQQCenteredExampleText">
   
<!-- MSISEMCOM /FRAME{ftbpFU}{9.7464in}{5.3065in}{0pt}{}{}{fig26.png}{\special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 9.7464in;  height 5.3065in;  depth 0pt;  original-width 9.6669in;  original-height 5.2503in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "1";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L26X__1.png" alt="fig26.png" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 26.1</span>
         </td>
      </tr></tbody>
</table></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p class="QQQExampleText">
   <br/>Observe that in the vicinity of the equilibrium points of both systems,
   the behaviors of the integral curves resemble those of a
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Figure25.01"> <span class="QQQHyperLink-BodyText">linear
   system</span></a></span>. In Fig. 26.1(a), the point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   looks like a saddle and the point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   looks like a node. Likewise, in Fig. 26.1(b), the point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   looks very much like a focus. These resemblances will be validated later by
   the Hartman-Grobman theorem.
   <span class="QQQHyperLink-BodyText"><a href="#HartmanGrobman"> <span class="QQQHyperLink-BodyText">[Jump
   ahead if you are curious.]</span></a></span>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 26.5
   </p>
</div>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>By the term "resemblance" in the last example we mean that as orbits get
   close to an equilibrium point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>,
   the orbits mimic the geometry types of
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Figure25.01"> <span class="QQQHyperLink-BodyText">Fig.
   25.1</span></a></span>. To better visualize the "resemblance," consider the
   system
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.6)</mtext></mtd>     
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:msup>
                       <mml:mi>y</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>y</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mi>x</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <br/>The equilibrium points of Eqn. (26.6) are solutions to the (nonlinear)
   algebraic equations
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msup>
                     <mml:mi>y</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>y</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mi>x</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mn>2</mml:mn>
               </mml:msup>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>There only real valued solution to these equations is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>.
   This implies that there is a single isolated equilibrium point at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   Zoom in to the equilibrium point with the interact of Fig. 26.2 by dragging
   the Zoom-slider to the right. Notice how the integral curves morph into
   straight line emanating from
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   The resulting image sure looks like the singular node from
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Figure25.01"> <span class="QQQHyperLink-BodyText">Fig.
   25.1</span></a>. </span><a id="Figure26.02"></a>
</p>
<br/>

<p class="center">
<iframe src="https://www.wolframcloud.com/objects/39ce40d9-a80d-4f23-ba7f-2826771eccd7?_embed=iframe" width="750" height="750"></iframe>
</p>  



<p>
   <br/>Justification of this behavior may be found by first expanding Eqn.
   (26.6) to obtain
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
    <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.7)</mtext></mtd>     
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>y</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>y</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>When
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>y</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   gets close to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>,
   say,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>0.01</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           <mml:mi>y</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mn>0.01</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the nonlinear terms
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>y</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>
   get even smaller. For instance, at the point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>0.01</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0.01</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we might approximate Eqn. (26.7) with the linear system
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.8)</mtext></mtd>  
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   We see from the table below that when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0.01</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0.01</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   the two systems have directions that cannot be distinguished when plotted.
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis">
         <mml:mtr>
           <mml:mtd>
             <mml:mstyle fontstyle="italic">
               <mml:mtext mathcolor="black">Nonlinear System</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontstyle="italic">
               <mml:mtext mathcolor="black">Linear System</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd columnalign="left">
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mover accent="true">
                         <mml:mi>x</mml:mi>
                         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                       </mml:mover>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mover accent="true">
                         <mml:mi>y</mml:mi>
                         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                       </mml:mover>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mn>0.01</mml:mn>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mn>0.5</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mn>0.01</mml:mn>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mn>0.01</mml:mn>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mn>0.01</mml:mn>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                           <mml:mn>0.01</mml:mn>
                           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                           <mml:mn>0.01</mml:mn>
                           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:msup>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mn>0.01</mml:mn>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mn>0.01</mml:mn>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mn>0.009995</mml:mn>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mn>0.010201</mml:mn>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="1.000000in" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mover accent="true">
                       <mml:mi>y</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.01</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.01</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd columnalign="left">
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   The difference gets smaller when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>y</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is closer to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   So the closer the orbits of the nonlinear system are to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the more they approach the behavior of orbits of a
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Figure25.01"> <span class="QQQHyperLink-BodyText">singular
   node</span></a></span>.
</p>
<p>
   <br/>
</p>
<p>
   The matrix formulation of Eqn. (26.8) is given
   by<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">Ar,&emsp;</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">where</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">&emsp;A</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Example25.03"> <span class="QQQHyperLink-BodyText">It
   is evident that A has eigen-pairs</span></a></span>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&lambda;</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mspace width="thickmathspace" />
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&lambda;</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mspace width="thickmathspace" />
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   (i.e., repeated eigenvalues with linearly independent eigenvectors), so that
   the linear system is a singular node.
</p>
<p>
   <br/>
</p>
<p>
   The following example provides another illustration of how a nonlinear system
   resembles a linear system in a neighborhood of an equilibrium point. This
   example is based on a second-order nonlinear ODE.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      26.6: <a id="Example26.06"></a>
   </p>
</div>
<p class="QQQExampleText">
   Consider the second-order ODE
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" accent="true">&die;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>4</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Rewrite
   as the system:<a id="Eqn.(26.09)"></a>
</p>
<p class="QQQExampleText">
   <br/><mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.9)</mtext></mtd>     
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>4</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>The equilibrium points of Eqn. (26.9) are solutions to the (nonlinear)
   algebraic equations
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>4</mml:mn>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   The second of these equations can be factored as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>4</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>.
   Consequently, Eqn. (26.9) has equilibrium points at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>4</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   The quickest way to get an overview of the solutions to Eqn. (26.7) is to
   construct the associated direction field as in Fig. 26.3(a); Fig. 26.3(b)
   illustrates some of the orbits of the system through the indicated
   points.<a id="Figure26.03"></a>
</p>
<p class="QQQCenteredExampleText">
   
<!-- MSISEMCOM /FRAME{ftbpFU}{8.7761in}{4.4771in}{0pt}{}{}{figure26.03.png}{\special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 8.7761in;  height 4.4771in;  depth 0pt;  original-width 9.3062in;  original-height 4.7331in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "3";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L26X__2.png" alt="figure26.03.png" height="430px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 26.3:
            <mml:math display="inline">
     <mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>y</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>y</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="false" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>4</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math></span>
         </td>
      </tr></tbody>
</table></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p class="QQQExampleText">
   The orbits through
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>2</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   appear to be unbounded. Observe what seems to be a nested family of periodic
   orbits about
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>4</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   (We establish the periodicity of these orbits in Example 27.6 of Lecture 27.)
   Likewise, the unbounded orbits through
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>2</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   appear to collapse toward the particular closed curve that passes through
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>.
</p>
 See the interact of Fig. 26.4 below to view the integral curves of the
   system. <a id="Figure26.04"></a>


<br/><br/>
<p class="center">
<iframe src="https://www.wolframcloud.com/objects/4340a337-f9cb-466f-8f55-18ad9c6de54c?_embed=iframe" width="690" height="800"></iframe>
</p>  
<br/>

<p class="QQQExampleText">
   The equilibrium point at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is selected by default. Gradually drag the Zoom-slider to the right. Notice
   how the neighborhood around
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   morphs to a saddle. As you zoom in to the neighborhood of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the bolded orbits of Fig. 26.4 appear to straighten out to form
   lines, reflecting the behavior of a saddle in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Example25.02"> <span class="QQQHyperLink-BodyText">Lecture
   25</span></a></span>.
</p>
<p class="QQQExampleText">
   <br/>Click on the Reset button and select the equilibrium point at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   by clicking on the radio button for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">&midast;</mml:mo>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>4.</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   Gradually drag the Zoom-slider to the right. Notice how the neighborhood
   around
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>4</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   morphs to a collection of closed egg shaped curves. As you zoom in to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>the
   phase portrait appears to resemble a center. (We will confirm this in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec27X/L27X.xht#Example27.06"> <span class="QQQHyperLink-BodyText">Example
   27.6</span></a> </span>and again in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec27X/L27X.xht#Theorem27.14"> <span class="QQQHyperLink-BodyText">Theorem 27.14</span></a></span>.)
</p>
<p class="QQQExampleText">
   A quantitative comparison is helpful. When
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>y</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is close to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>,
   say
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mn>0.1</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="false" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>4</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mn>0.0025</mml:mn>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   For instance at the point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>0.1</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0.1</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we might approximate
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(26.09)"> <span class="QQQHyperLink-BodyText">Eqn.
   (26.9)</span></a></span> with the linear system
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.10)</mtext></mtd> 
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p class="QQQExampleText">
   Thus at the point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0.1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0.1</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   the two systems have directions that cannot be distinguished when plotted.
</p>
<p>
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis">
         <mml:mtr>
           <mml:mtd>
             <mml:mstyle fontstyle="italic">
               <mml:mtext mathcolor="black">Nonlinear System</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontstyle="italic">
               <mml:mtext mathcolor="black">Linear System</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd columnalign="left">
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mover accent="true">
                       <mml:mi>y</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>0.0025</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.1025</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="1.000000in" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mover accent="true">
                       <mml:mi>y</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd columnalign="left">
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   The matrix formulation of Eqn. (26.10) is given
   by<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">Ar,&emsp;</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">where</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">&emsp;A</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   The corresponding eigen-pairs are given by
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&lambda;</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mspace width="thickmathspace" />
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&lambda;</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mspace width="thickmathspace" />
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p class="QQQExampleText">
   (i.e., distinct eigenvalues of opposite sign), so that the linear system is a
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec25X/L25X.xht#Figure25.01"> <span class="QQQHyperLink-BodyText">saddle</span></a></span>.
</p>
<p class="QQQExampleText">
   <br/>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 26.6
   </p>
</div>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p>
   <br/><a id="PeriodicOrbit"></a>
</p>
<p>
   <br/>Another important kind of solution (and corresponding orbit) referred to
   earlier and illustrated in
   <span class="QQQHyperLink-BodyText"><a href="#Figure26.03"> <span class="QQQHyperLink-BodyText">Fig.
   26.3(b)</span></a></span> is a periodic solution (orbit). We leave its formal
   definition to
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec27X/L27X.xht#PeriodicSolution"> <span class="QQQHyperLink-BodyText">Lecture
   27</span></a></span>. For now we are content to accept an informal definition:
   a <strong>periodic orbit</strong> is a simple closed curve.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>Although nonlinear systems may behave like linear systems in the
   neighborhood of an equilibrium point, nonlinear systems may exhibit vastly
   different behavior than their linearizations away from the equilibrium points.
   First, a nonlinear system may have many (isolated) equilibrium points - this
   cannot happen in a linear system. Second , an orbit that leaves a neighborhood
   of an equilibrium point which appears to be a saddle point, a node, or a focus
   of a nonlinear system, need not become unbounded, as is the case of linear
   systems. For instance, note how in
   <span class="QQQHyperLink-BodyText"><a href="#Example26.05"> <span class="QQQHyperLink-BodyText">Fig.
   26.1(a)</span></a></span>, one orbit leaves the saddle point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and heads toward the node at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   There are many other significant ways in which nonlinear systems differ from
   linear ones. We are able to provide only a brief glimpse into the subject
   here.
</p>
<p>
   <br/>
</p>
<p>
   <a id="Linearization"></a>
</p>
<p class="QQQSubHead1">
   <br/>26.3 LINEARIZATION ABOUT AN EQUILIBRIUM POINT
</p>
<p>
   <br/>
</p>
<p>
   What kind of nonlinear systems behave like linear systems near an equilibrium
   point? From what we've seen above, it seems reasonable for the nonlinear terms
   to tend to zero faster than the linear terms. To justify replacing the
   nonlinear systems of Eqn. (26.7) and Eqn. (26.9) with linear systems, we need
   to generalize the preceding results.
</p>
<p>
   <br/>
</p>
<p>
   Suppose
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is an equilibrium point of the general nonlinear system
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
    <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.11)</mtext></mtd> 
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>f</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>g</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>Assume that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   have continuous partial derivatives of order up to 2 in some disk about
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   According to
   <span class="QQQNote-BodyText"> <a target="_blank" href="L26X__note1.xht"><span class="QQQNote-BodyText">Taylor's
   theorem for a function of two variables</span></a> </span>, each of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   may be approximated in that disk by
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
    <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.12)</mtext></mtd>  
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>f</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>f</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>f</mml:mi>
                       <mml:mi>x</mml:mi>
                     </mml:msub>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="infix">,</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>f</mml:mi>
                       <mml:mi>y</mml:mi>
                     </mml:msub>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="infix">,</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi>y</mml:mi>
                     <mml:mo form="postfix">+</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext mathcolor="black">higher order terms in&ensp;</mml:mtext>
             <mml:mi>x</mml:mi>
             <mml:mtext mathcolor="black">&ensp;and&ensp;</mml:mtext>
             <mml:mi>y</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>g</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>g</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>g</mml:mi>
                       <mml:mi>x</mml:mi>
                     </mml:msub>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="infix">,</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>g</mml:mi>
                       <mml:mi>y</mml:mi>
                     </mml:msub>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="infix">,</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi>y</mml:mi>
                     <mml:mo form="postfix">+</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext mathcolor="black">higher order terms in&ensp;</mml:mtext>
             <mml:mi>x</mml:mi>
             <mml:mtext mathcolor="black">&ensp;and&ensp;</mml:mtext>
             <mml:mi>y</mml:mi>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>y</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is close to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>
   will be small and we can neglect the higher order terms. Since
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is an equilibrium point, it follows that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>0</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>0</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Set<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mi>a</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>f</mml:mi>
                     <mml:mi>x</mml:mi>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mi>b</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>f</mml:mi>
                     <mml:mi>y</mml:mi>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mi>c</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>g</mml:mi>
                     <mml:mi>x</mml:mi>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mi>d</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>g</mml:mi>
                     <mml:mi>y</mml:mi>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>Then the nonlinear system Eqn. (26.11) is approximated by the linear
   system which we call the <strong>linearization </strong>of Eqn. (26.11)
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.13)</mtext></mtd>  
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>a</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mi>b</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   We illustrate linearization with an example based on a model of the damped
   nonlinear pendulum.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      26.7 [Damped nonlinear pendulum]:<a id="Example26.07"></a>
   </p>
</div>
<p class="QQQExampleText">
   
<!-- MSISEMCOM /FRAME{dtbpF}{1.4174in}{2.2399in}{0pt}{}{}{pendulum2.png}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 1.4174in;  height 2.2399in;  depth 0pt;  original-width 1.4944in;  original-height 2.3782in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "5";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><img src="Images/L26X__3.png" alt="pendulum2.png" height="215px" /></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p class="QQQExampleText">
   <br/>The model for a simple nonlinear damped
   pendulum<span class="QQQHyperLink-BodyText"> <a target="_blank" href="../Lec01X/L01X.xht#Example01.10"> <span class="QQQHyperLink-BodyText">(see
   Example 1.10)</span></a> </span>is given by the second-order ODE
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.14)</mtext></mtd>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi>d</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&theta;</mml:mi>
                 </mml:mrow>
                 <mml:mrow>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>t</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>c</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&theta;</mml:mi>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
               <mml:mn>2</mml:mn>
             </mml:msubsup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>&theta;</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   where
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   is the damping coefficient and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msubsup>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>0</mml:mn>
           <mml:mn>2</mml:mn>
         </mml:msubsup>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo form="infix">/</mml:mo>
           <mml:mi>L</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   In system form Eqn. (26.14) becomes<a id="Eqn.(26.15)"></a>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.15)</mtext></mtd>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&theta;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>&omega;</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>&theta;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&omega;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>The equilibrium points of Eqn. (26.15) are solutions to
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mi>&omega;</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>&theta;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi>c</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&omega;</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Hence we must have
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&omega;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">sin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>&theta;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>.
   Consequently, Eqn. (26.15) has equilibrium points at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>k</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
   for all integers
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>k</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>We seek the linearization of Eqn. (26.15) at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>&theta;</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>&omega;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>0</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   First replace
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>&theta;</mml:mi>
     </mml:mrow>
   </mml:math>
   with its Taylor series at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&theta;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   to get
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>&theta;</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>&omega;</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>&omega;</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>&theta;</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mn>1</mml:mn>
                             <mml:mrow>
                               <mml:mn>3</mml:mn>
                               <mml:mo form="postfix">!</mml:mo>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msup>
                         <mml:mi>&theta;</mml:mi>
                         <mml:mn>3</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mn>1</mml:mn>
                             <mml:mrow>
                               <mml:mn>5</mml:mn>
                               <mml:mo form="postfix">!</mml:mo>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msup>
                         <mml:mi>&theta;</mml:mi>
                         <mml:mn>5</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&ctdot;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi>c</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&omega;</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Upon regrouping terms, we have
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>&theta;</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>&omega;</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&theta;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mo form="postfix">+</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext mathcolor="black">higher order terms in&ensp;</mml:mtext>
             <mml:mi>&theta;</mml:mi>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Thus the linearized system at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is
</p>
<p class="QQQExampleText">
   <br/><mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.16)</mtext></mtd>   
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&theta;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>&omega;</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&theta;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&omega;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Fig. 26.5 below offers both the phase portrait for the nonlinear system, Eqn.
   (26.14) with integral curves where we have set
   <mml:math display="inline">
     <mml:mrow>
       <mml:msubsup>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
         <mml:mn>2</mml:mn>
       </mml:msubsup>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="false" scriptlevel="0">
           <mml:mfrac>
             <mml:mn>1</mml:mn>
             <mml:mn>2</mml:mn>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>.
   Note the behavior of the integral curves in the neighborhoods of the
   equilibrium points
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&pi;</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>0</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Each of these points resembles a focus of a linear system and is confirmed at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math> by
   dragging the Zoom-slider of the interact to the right.   

   <a id="Figure26.05"></a>
</p>
   <br/>

<p class="center">
<iframe src="https://www.wolframcloud.com/objects/70a125e4-ba6c-4236-b83c-a69bdbe57a39?_embed=iframe" width="880" height="600"></iframe>
</p>  


<p class="QQQExampleText">
   <br/>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 26.7
   </p>
</div>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>The last example does not deal with the equilibrium points at odd
   multiples of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&pi;</mml:mi>
     </mml:mrow>
   </mml:math>
   on the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&theta;</mml:mi>
     </mml:mrow>
   </mml:math>-axis;
   specifically at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>&pi;</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   in Fig, 26.5. By dragging the Zoom-slider, it appears that these equilibrium
   points resemble saddles. It is possible to linearize a nonlinear system at any
   isolated equilibrium point other than
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   by a suitable change of variables from
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>&theta;</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>&omega;</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>u</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>v</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   We illustrate this technique by linearizing Eqn. (26.14) at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>&pi;</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>u</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>&theta;</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>v</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>&omega;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Substitute into Eqn. (26.14)
   to obtain<mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.17)</mtext></mtd>   
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>u</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>v</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>v</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>u</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>v</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>Now<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>u</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>v</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>v</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>g</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>u</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>v</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>u</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi>c</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>v</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>so
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mn>0</mml:mn>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>g</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mn>0</mml:mn>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>&pi;</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Hence
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is an equilibrium point of Eqn. (26.17). Now we linearize Eqn. (26.17) at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   by following the procedure in
   <span class="QQQHyperLink-BodyText"><a href="#Example26.07"> <span class="QQQHyperLink-BodyText">Example
   26.7</span></a></span>. Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>h</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mi>u</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">sin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>u</mml:mi>
             <mml:mo form="infix">+</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and expand in a Taylor series at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>u</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>:
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>h</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>u</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>h</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mrow>
                             <mml:mi>d</mml:mi>
                             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>h</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                             <mml:mi>d</mml:mi>
                             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>u</mml:mi>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:mi>u</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
