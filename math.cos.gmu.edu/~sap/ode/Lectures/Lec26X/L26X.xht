               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>&pi;</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">cos</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mi>u</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mi>&pi;</mml:mi>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                     </mml:mrow>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:msup>
                       <mml:mi>u</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>6</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">cos</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mi>&pi;</mml:mi>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                     </mml:mrow>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:msup>
                       <mml:mi>u</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>24</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">sin</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:msup>
                       <mml:mi>u</mml:mi>
                       <mml:mn>4</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mi>u</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mn>0</mml:mn>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:msup>
                     <mml:mi>u</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>6</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:msup>
                     <mml:mi>u</mml:mi>
                     <mml:mn>3</mml:mn>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>24</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mn>0</mml:mn>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:msup>
                     <mml:mi>u</mml:mi>
                     <mml:mn>4</mml:mn>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>u</mml:mi>
                   <mml:mo form="postfix">+</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mtext mathcolor="black">higher order terms in&ensp;</mml:mtext>
               <mml:mi>u</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math><br/>
</p>
<p>
   <br/>Thus the linearization of Eqn. (26.17) at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mi>v</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is
</p>
<p>
   <br/><mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.18)</mtext></mtd>    
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>u</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>v</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>v</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>u</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>v</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <br/>Finally we must bring the linearization back to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mi>&theta;</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>&omega;</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   coordinates. So replace
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>u</mml:mi>
     </mml:mrow>
   </mml:math>
   with
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&theta;</mml:mi>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mi>&pi;</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>v</mml:mi>
     </mml:mrow>
   </mml:math>
   with
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&omega;</mml:mi>
     </mml:mrow>
   </mml:math>
   in Eqn. (26.18) to get the linearization of
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(26.15)"> <span class="QQQHyperLink-BodyText">Eqn.
   (26.15)</span></a></span> at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>&theta;</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mi>&omega;</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>&pi;</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.19)</mtext></mtd>    
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&theta;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>&omega;</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>&theta;</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi>c</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&omega;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p class="QQQSubHead1">
   26.4 THE JACOBIAN
</p>
<p>
   <br/>
</p>
<p>
   A useful tool to linearize the general nonlinear
   system<mml:math mode="display" display="block">
   <mtable side='left'>
     <mlabeledtr>
      <mtd><mtext>(26.20)</mtext></mtd> 
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>f</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mi>g</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
    	</mlabeledtr>
	</mtable> 
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   at an equilibrium point
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is based on a vector formulation of the system. Recall that Eqn. (26.20) may
   be expressed as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math><strong>h</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   where<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="left">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>x</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>y</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mrow>
           <mml:mover>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="infix">&sdot;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
           </mml:mover>
           <mml:mrow>
             <mml:mstyle fontweight="bold" fontstyle="upright">
               <mml:mo form="infix">=</mml:mo>
             </mml:mstyle>
           </mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="left">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mover accent="true">
                     <mml:mi>x</mml:mi>
                     <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                   </mml:mover>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mover accent="true">
                     <mml:mi>y</mml:mi>
                     <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                   </mml:mover>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">h</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mi>f</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mi>g</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">h</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="left">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mi>f</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mstyle fontweight="bold">
                       <mml:mtext mathcolor="black">r</mml:mtext>
                     </mml:mstyle>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mi>g</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mstyle fontweight="bold">
                       <mml:mtext mathcolor="black">r</mml:mtext>
                     </mml:mstyle>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="left">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mi>f</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mi>y</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mi>g</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mi>y</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="left">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">&midast;</mml:mo>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>y</mml:mi>
                     <mml:mo form="infix">&midast;</mml:mo>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <a id="Definition26.08"></a>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="definition">
   <span class="leadin">Definition</span><p class="definition-FirstPara">
      26.8 [The Jacobian]:
   </p>
</div>
<p>
   <br/><span class="QQQSansSerifItalic16">Assume that</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">have continuous partial derivatives of
   order up to 2 in some disk about</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>.
   <span class="QQQSansSerifItalic16">The matrix</span>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(26.21)</mtext></mtd>
	    <mml:mrow>

       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="left left">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mi>x</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mi>y</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>g</mml:mi>
                   <mml:mi>x</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>g</mml:mi>
                   <mml:mi>y</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>

      </mml:mrow>
	 </mlabeledtr>
    </mtable>	

   </mml:math>
</p>
<p>
   <span class="QQQSansSerifItalic16">is called the</span>
   <span class="QQQSansSerifBoldItalic16">Jacobian matrix</span>
   <span class="QQQSansSerifItalic16">of Eqn. (26.20) at</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">The linear system</span>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(26.22)</mtext></mtd>
	    <mml:mrow>

       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="prefix">&minus;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>

      </mml:mrow>
	 </mlabeledtr>
    </mtable>	

   </mml:math>
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <span class="QQQSansSerifItalic16">is called the
   <strong>linearization</strong> of Eqn. (26.20) at</span>
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mo form="infix">&midast;</mml:mo>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><hr class="hrulefill" />
</p>
<p>
   <br/><span class="QQQNote-BodyText"> <a target="_blank" href="L26X__note2.xht"><span class="QQQNote-BodyText">See
   here for a derivation of Eqn. (26.22).</span></a> </span>
</p>
<p>
   <br/>
</p>
<p>
   <br/>The next three examples illustrate the use of the Jacobian matrix to
   determine the linearization at the equilibrium points of some previous
   examples.
</p>
<p>
   <br/>
</p>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      26.9 [Example 26.6 redux]:<a id="Example26.09"></a>
   </p>
</div>
<p class="QQQExampleText">
   Calculate the Jacobian matrix of the system from
   <span class="QQQHyperLink-BodyText"><a href="#Example26.06"> <span class="QQQHyperLink-BodyText">Example
   26.6</span></a></span>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>y</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>4</mml:mn>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   at the equilibrium points
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:
   <span style="width:1em"><span style="padding-left:1em"></span></span></span>Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>y</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>4</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Then
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>f</mml:mi>
             <mml:mi>x</mml:mi>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>f</mml:mi>
             <mml:mi>y</mml:mi>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>g</mml:mi>
             <mml:mi>x</mml:mi>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">/</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>g</mml:mi>
             <mml:mi>y</mml:mi>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">,</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   so
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mi>x</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mi>y</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>g</mml:mi>
                   <mml:mi>x</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>g</mml:mi>
                   <mml:mi>y</mml:mi>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="infix">/</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Now we calculate the linearizations at each of the equilibrium points.
</p>
<p class="QQQExampleText">
   <br/><span style="width:2em"><span style="padding-left:2em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math>
   At
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mo form="infix">&midast;</mml:mo>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msub>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">/</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           </mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>So in vector form the linearization we seek is given by
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="prefix">&minus;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>or
   in component form<a id="Example26.09a"></a>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="left">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>x</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>y</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="left">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="left">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>x</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>y</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="left">
           <mml:mtr>
             <mml:mtd>
               <mml:mi>y</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mi>x</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span style="width:2em"><span style="padding-left:2em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math>
   At
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mo form="infix">&midast;</mml:mo>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msub>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">/</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           </mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>4</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>So in vector form the linearization we seek is given by
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">J</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mo form="infix">&midast;</mml:mo>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="prefix">&minus;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>or
   in component form by
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="left">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>x</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mi>y</mml:mi>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>4</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mn>4</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>y</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
