           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>E</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
