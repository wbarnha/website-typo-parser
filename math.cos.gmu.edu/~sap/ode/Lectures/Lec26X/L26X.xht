             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
