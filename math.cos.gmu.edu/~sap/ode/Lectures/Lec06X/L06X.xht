         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="center left left">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mtext>,</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mi>&infin;</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mi>t</mml:mi>
               <mml:mtext>,</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mtext>,</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mtext>,</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mn>2</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>&infin;</mml:mi>
