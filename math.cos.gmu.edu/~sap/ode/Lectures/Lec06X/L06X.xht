               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mtext>,</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mspace width="veryverythinmathspace" />
               </mml:mtd>
               <mml:mtd>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&infin;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>&tau;</mml:mi>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mtext>,</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mspace width="veryverythinmathspace" />
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mtext>,</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mspace width="veryverythinmathspace" />
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&pi;</mml:mi>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>

   </mml:math>and
   has graph
</p>
<p class="center">
   
<!-- MSISEMCOM /FRAME{ftbpF}{3.8035in}{3.1185in}{0pt}{}{}{example06.09a_new.png}{\special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 3.8035in;  height 3.1185in;  depth 0pt;  original-width 3.147in;  original-height 2.5763in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "13";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><img src="Images/L06X__20.png" alt="example06.09a_new.png" height="299px" /></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p class="QQQExampleText">
   The value of the integral
   <mml:math display="inline">
     <mml:mrow>
       <mml:msubsup>
         <mml:mo form="prefix" largeop="true">&int;</mml:mo>
         <mml:mn>0</mml:mn>
         <mml:mi>t</mml:mi>
       </mml:msubsup>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>&tau;</mml:mi>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>w</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>&tau;</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>d</mml:mi>
         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&tau;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   depends on the value of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   There are 3 cases:
</p>
<p class="QQQExampleText">
   (1) <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>&infin;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   (2) <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   or
</p>
<p class="QQQExampleText">
   (3) <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&pi;</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>In view of Eqn. (6.9) we have
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:msubsup>
                           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mi>t</mml:mi>
                         </mml:msubsup>
                         <mml:mrow>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mi>&tau;</mml:mi>
                           </mml:msup>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mi>g</mml:mi>
                             <mml:mo>&ApplyFunction;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mspace width="thinmathspace" />
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>d</mml:mi>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
                 <mml:mtable align="axis" columnalign="left left left">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:mrow>
                               <mml:msubsup>
                                 <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mi>t</mml:mi>
                               </mml:msubsup>
                               <mml:mspace width="thinmathspace" />
                               <mml:mrow>
                                 <mml:mn>0</mml:mn>
                                 <mml:mspace width="thinmathspace" />
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mi>d</mml:mi>
                                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mtext>,</mml:mtext>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mspace width="veryverythinmathspace" />
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&infin;</mml:mi>
                       </mml:mrow>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:mrow>
                               <mml:msubsup>
                                 <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mi>t</mml:mi>
                               </mml:msubsup>
                               <mml:mspace width="thinmathspace" />
                               <mml:mrow>
                                 <mml:mn>2</mml:mn>
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:msup>
                                   <mml:mi>e</mml:mi>
                                   <mml:mi>&tau;</mml:mi>
                                 </mml:msup>
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mrow>
                                   <mml:mi mathcolor="gray">sin</mml:mi>
                                   <mml:mo>&ApplyFunction;</mml:mo>
                                   <mml:mi>&tau;</mml:mi>
                                 </mml:mrow>
                                 <mml:mspace width="thinmathspace" />
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mi>d</mml:mi>
                                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mtext>,</mml:mtext>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mspace width="veryverythinmathspace" />
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">&le;</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo form="infix">+</mml:mo>
                           <mml:mrow>
                             <mml:msubsup>
                               <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                               <mml:mn>0</mml:mn>
                               <mml:mi>&pi;</mml:mi>
                             </mml:msubsup>
                             <mml:mspace width="thinmathspace" />
                             <mml:mrow>
                               <mml:mn>2</mml:mn>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:msup>
                                 <mml:mi>e</mml:mi>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:msup>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mrow>
                                 <mml:mi mathcolor="gray">sin</mml:mi>
                                 <mml:mo>&ApplyFunction;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                               <mml:mspace width="thinmathspace" />
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>d</mml:mi>
                               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="infix">+</mml:mo>
                           <mml:mrow>
                             <mml:msubsup>
                               <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                               <mml:mi>&pi;</mml:mi>
                               <mml:mi>t</mml:mi>
                             </mml:msubsup>
                             <mml:mspace width="thinmathspace" />
                             <mml:mrow>
                               <mml:mn>0</mml:mn>
                               <mml:mspace width="thinmathspace" />
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>d</mml:mi>
                               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                       <mml:mtext>,</mml:mtext>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mspace width="veryverythinmathspace" />
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mi>&pi;</mml:mi>
                       <mml:mo form="infix">&le;</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mi>&infin;</mml:mi>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>where the limits of integration are determined by the position of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   along the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&tau;</mml:mi>
     </mml:mrow>
   </mml:math>-axis.
   Thus
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
                   <mml:mtable align="axis" columnalign="left left left">
                     <mml:mtr>
                       <mml:mtd>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                         <mml:mtext>,</mml:mtext>
                       </mml:mtd>
                       <mml:mtd>
                         <mml:mspace width="veryverythinmathspace" />
                       </mml:mtd>
                       <mml:mtd>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>&infin;</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">&lt;</mml:mo>
                         <mml:mi>t</mml:mi>
                         <mml:mo form="infix">&lt;</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mtd>
                     </mml:mtr>
                     <mml:mtr>
                       <mml:mtd>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
