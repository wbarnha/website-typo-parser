               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
                 <mml:mtable align="axis" columnalign="left left left">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mtext>,</mml:mtext>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mspace width="veryverythinmathspace" />
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&infin;</mml:mi>
                       </mml:mrow>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">cos</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:msup>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mtext>,</mml:mtext>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mspace width="veryverythinmathspace" />
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">&le;</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mi>&pi;</mml:mi>
                           </mml:msup>
                           <mml:mo form="infix">+</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mtext>,</mml:mtext>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mspace width="veryverythinmathspace" />
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mi>&pi;</mml:mi>
                       <mml:mo form="infix">&le;</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&lt;</mml:mo>
                       <mml:mi>&infin;</mml:mi>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>The details of integration are left for the reader. Figure 6.12 displays
   a plot of the solution
</p>
<p class="center">
   
