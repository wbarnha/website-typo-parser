<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">;</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mover accent="true">
                         <mml:mi>x</mml:mi>
                         <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
                       </mml:mover>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mover accent="true">
                       <mml:mi>r</mml:mi>
                       <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
                     </mml:mover>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">;</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>r</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mover accent="true">
                     <mml:mi>x</mml:mi>
                     <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
                   </mml:mover>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mi>r</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                       <mml:msub>
                         <mml:mi>x</mml:mi>
                         <mml:mn>0</mml:mn>
                       </mml:msub>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mi>b</mml:mi>
                   </mml:mrow>
                   <mml:mi>r</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
               <mml:mrow>
                 <mml:mover accent="true">
                   <mml:mi>r</mml:mi>
                   <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
                 </mml:mover>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>r</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
               <mml:mrow>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mi>r</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac linethickness="0">
                         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">[</mml:mo>
           <mml:mrow>
             <mml:mn>0</mml:mn>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>T</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="false">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
