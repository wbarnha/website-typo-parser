                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo stretchy="true">&UnderBrace;</mml:mo>
           </mml:munder>
           <mml:msub>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
