               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>C</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>B</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>A</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>A</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">coefficient of&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">:</mml:mo>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>B</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>C</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>C</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>B</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>B</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">coefficient of&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mrow>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">:</mml:mo>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>C</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Simplify to get the following system of three linear equations for the
   variables
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>B</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>C</mml:mi>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left right">
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&#8290;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>A</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>B</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&#8290;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>C</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&#8290;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>B</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&#8290;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>C</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&#8290;</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>C</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Solve for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>B</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>C</mml:mi>
     </mml:mrow>
   </mml:math>
   (by backward substitution):
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>C</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mi>B</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>3.</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>The
   particular solution is
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>3</mml:mn>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <span class="QQQSansSerif14">3</span>. <span class="QQQSansSerif14">Solution
   to the IVP:</span> Finally we solve the IVP by substituting the initial
   condition
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>5</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   into the general solution to the nonhomogeneous ODE
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>h</mml:mi>
         </mml:msub>
         <mml:mo form="infix">+</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>3</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Differentiate to
   obtain<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>x</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>3</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Substitute the initial values into the last two equations. We get
</p>
<p class="QQQExampleText">
   <br/><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mn>0</mml:mn>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mn>3</mml:mn>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mn>5</mml:mn>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mn>2</mml:mn>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mn>3</mml:mn>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Solve these equations to get
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
   Thus the solution to the IVP is
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>h</mml:mi>
         </mml:msub>
         <mml:mo form="infix">+</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>3</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 14.11
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="alert">
   <span class="leadin">Alert</span><p class="alert-FirstPara">
      <span style="width:1em"><span style="padding-left:1em"></span></span>[First
      add
      <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>h</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
      to
      <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
      then calculate
      <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
      and
      <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>]
   </p>
</div>
<p class="QQQSlantText">
   <br/>In solving an IVP for a nonhomogeneous ODE, do
   <span class="QQQSansSerifBoldItalic16">not</span> attempt to compute the
   unknowns
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   of the homogeneous solution until the particular solution has been properly
   computed and added to the homogeneous solution. Failure to do so usually leads
   to an incorrect solution.
</p>
<p class="QQQSlantText">
   <br/>
</p>
<div class="solution">
   <span class="leadin">End of Alert</span><p class="solution-FirstPara">
      _
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>
</p>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/><a id="Caveat"></a>
</p>
<p>
   Now suppose that we change the input in
   <span class="QQQHyperLink-BodyText"><a href="#Example14.02"> <span class="QQQHyperLink-BodyText">Example
   14.2</span></a></span> to the function
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   and proceed to calculate a particular solution to the
   ODE<a id="Eqn.(14.10)"></a>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(14.10)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>
	
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   The UC set is
   simply<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Form
   the trial
   solution<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   into the ODE and equate coefficients of the elements of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   in order to evaluate
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>B</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>C</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Now
   substitute these into the
   ODE<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>which
   simplifies
   to<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mn>0</mml:mn>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>It
   follows that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is an <span class="QQQSansSerifBold16">incorrect</span> trial solution.
</p>
<p>
   <br/>
</p>
<p class="QQQCenteredBodyText">
   <span class="QQQSansSerifBoldItalic16">The problem is that
   </span><mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifBoldItalic16"> is
   a member of the basis:</span>
</p>
<p class="QQQCenteredBodyText">
   <span class="QQQSansSerifBoldItalic16">i.e.,</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifBoldItalic16"> is
   a homogeneous solution.</span>
</p>
<p>
   <br/>
</p>
<p>
   Thus we need to change our trial solution. But to what? We want our candidate
   for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   to balance out the forcing function
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   So try
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   The introduction of the factor
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   ensures that the expression
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">+</mml:mo>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is a linear combination of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>.
   Because of the way the product rule of calculus works, the terms involving
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   add up to zero, leaving us with a
   <span class="QQQSansSerifBold16">nonzero</span> multiple of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   The UC set is still
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Because
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   is a member of the basis, form the trial
   solution<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   into the ODE and equate coefficients in order to evaluate
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mi>p</mml:mi>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi>A</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mi>p</mml:mi>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&die;</mml:mo>
               </mml:mover>
               <mml:mi>p</mml:mi>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>Now
   substitute these into
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(14.10)"> <span class="QQQHyperLink-BodyText">Eqn.
   (14.10)</span></a></span> to obtain
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:munder>
           <mml:munder>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo stretchy="true">&UnderBrace;</mml:mo>
           </mml:munder>
           <mml:msub>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&die;</mml:mo>
             </mml:mover>
             <mml:mi>p</mml:mi>
           </mml:msub>
         </mml:munder>
         <mml:mo form="infix">+</mml:mo>
         <mml:munder>
           <mml:munder>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo stretchy="true">&UnderBrace;</mml:mo>
           </mml:munder>
           <mml:msub>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mi>p</mml:mi>
           </mml:msub>
         </mml:munder>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:munder>
             <mml:munder>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo stretchy="true">&UnderBrace;</mml:mo>
             </mml:munder>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mi>p</mml:mi>
             </mml:msub>
           </mml:munder>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>which
   simplifies
   to<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>3</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>A</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mi>t</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math>So
   choose
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="false" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mn>3</mml:mn>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It follows
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mn>3</mml:mn>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>is
   a CORRECT trial solution.
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/><hr class="hrulefill" />
</p>
<div class="alert">
   <span class="leadin">Alert</span><p class="alert-FirstPara">
      <span style="width:1em"><span style="padding-left:1em"></span></span>[Always
      check the basis first]:
   </p>
</div>
<p class="QQQSlantText">
   <br/>Eqn.(14.10) raises a warning flag. We must check to see if any member of
   the UC set is homogeneous solution. If this happens, then we multiply
   <span class="QQQSansSerifBoldItalic16">every</span> member of the UC set by
   the lowest power of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   so that <span class="QQQSansSerifBoldItalic16">no</span> term in the resulting
   UC set is a homogeneous solution.
</p>
<p class="QQQSlantText">
   Thus <span class="QQQSansSerifBoldItalic16">before we even start the UC
   method</span>, we must determine a basis for the homogeneous ODE. We
   illustrate with some examples. Be sure that you can verify each step in these
   examples. This is the heart of the UC method<span class="large2">.</span>
</p>
<p class="center">
   <br/>
</p>
<div class="solution">
   <span class="leadin">End of Alert</span><p class="solution-FirstPara">
      _<hr class="hrulefill" />
   </p>
</div>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      14.12<a id="Example14.12"></a>
   </p>
</div>
<p class="QQQExampleText">
   Compute a particular solution to the
   ODE<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>4</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>First
   determine a basis for the homogeneous
   ODE<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>The
   characteristic equation is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Factor
   as<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>&lambda;</mml:mi>
             <mml:mo form="infix">+</mml:mo>
             <mml:mi>i</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mi>i</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>to
   get the complex conjugate
   roots<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&lambda;</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&lambda;</mml:mi>
           <mml:mo form="postfix" accent="true">&macr;</mml:mo>
         </mml:mover>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>i</mml:mi>
     </mml:mrow>
   </mml:math>A
   basis is given by the
   pair<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mspace width="thickmathspace" />
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Next calculate
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>.
   Calculate successive higher order derivatives of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>4</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   until repetitions begin to appear
</p>
<p class="QQQExampleText">
   <br/><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mn>4</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>f</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>f</mml:mi>
                 <mml:mo form="postfix" accent="true">&die;</mml:mo>
               </mml:mover>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>8</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>f</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mn>3</mml:mn>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
               </mml:msup>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>8</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   By the time we compute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>f</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>3</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we have all possible derivatives of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   up to constant multiples. Thus we can write
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Now check to see if any member of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is a linear combination of members of the basis. We see immediately that both
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">cos</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   are members of the basis. Consequently, we form a modified UC set
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&Gamma;</mml:mi>
           <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
         </mml:mover>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   by multiplying each member of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   by the lowest power of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   so that no member of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&Gamma;</mml:mi>
           <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
         </mml:mover>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is a linear combination of members of the basis. It is easy to see that the
   lowest power of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   that works is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   Indeed,
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">&sdot;</mml:mo>
           <mml:msub>
             <mml:mi>&Gamma;</mml:mi>
             <mml:mi>f</mml:mi>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mo>&InvisibleComma;</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>t</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>t</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   So proceed to use the modified UC
   set<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&Gamma;</mml:mi>
           <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
         </mml:mover>
         <mml:mi>f</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&sdot;</mml:mo>
         <mml:msub>
           <mml:mi>&Gamma;</mml:mi>
           <mml:mi>f</mml:mi>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Form
   the trial
   solution<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>B</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>C</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>D</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   into the ODE and equate coefficients of the elements of
   <mml:math display="inline">
     <mml:mrow>
