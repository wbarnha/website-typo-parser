<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:munder>
           <mml:munder>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>B</mml:mi>
                       </mml:mrow>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mn>4</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>A</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mn>6</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>C</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mn>8</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>B</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mn>4</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>A</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>12</mml:mn>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>C</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mn>4</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>B</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mn>4</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>C</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
             <mml:mo stretchy="true">&UnderBrace;</mml:mo>
           </mml:munder>
           <mml:msub>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&die;</mml:mo>
             </mml:mover>
             <mml:mi>p</mml:mi>
           </mml:msub>
         </mml:munder>
         <mml:mo form="infix">+</mml:mo>
         <mml:munder>
           <mml:munder>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mrow>
                   <mml:mi>A</mml:mi>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>B</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>A</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mn>3</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>C</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>B</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>C</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
             <mml:mo stretchy="true">&UnderBrace;</mml:mo>
           </mml:munder>
           <mml:msub>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mi>p</mml:mi>
           </mml:msub>
         </mml:munder>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:munder>
             <mml:munder>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>A</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mi>B</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mi>C</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo stretchy="true">&UnderBrace;</mml:mo>
             </mml:munder>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mi>p</mml:mi>
             </mml:msub>
           </mml:munder>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>9</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>t</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
