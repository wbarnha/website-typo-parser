         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mi>C</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mi>B</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>D</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
