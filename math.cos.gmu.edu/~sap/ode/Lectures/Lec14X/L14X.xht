           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mn>0</mml:mn>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mn>0</mml:mn>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            for
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>&omega;</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0.7</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0.8</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0.9</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            and
            <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1.0</mml:mn>
     </mml:mrow>
   </mml:math></span>
         </td>
      </tr></tbody>
</table></span><br/>
