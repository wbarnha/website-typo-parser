<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>A</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mi>&omega;</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>B</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>B</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mi>&omega;</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>f</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mi>&omega;</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>A</mml:mi>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mi>A</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>B</mml:mi>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mi>B</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Solve
   for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>B</mml:mi>
     </mml:mrow>
   </mml:math>:<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
                 <mml:mspace width="thinmathspace" />
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mi>B</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>So
   our particular solution
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
                 <mml:mspace width="thinmathspace" />
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mi>&omega;</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math><br/><span class="QQQSansSerif14">3</span>.
   <span class="QQQSansSerif14">Solution to the IVP: </span>First combine the the
   homogeneous solution
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>h</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   with the particular solution
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   to get the general solution to
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(14.13)"> <span class="QQQHyperLink-BodyText">Eqn.
   (14.13</span></a></span>).
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                   <mml:mspace width="thinmathspace" />
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mi>&omega;</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
