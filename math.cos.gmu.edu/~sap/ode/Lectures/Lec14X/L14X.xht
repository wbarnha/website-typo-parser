                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>72</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>144</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>72</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>72</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="0.550000in" />
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>108</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>216</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>72</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Observe that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>f</mml:mi>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>3</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:msup>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is a linear combination of the same functions as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>f</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   namely
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>t</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   So
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Now check to see if any member of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is a linear combination of members of the basis. We see immediately that the
   first member,
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   is a member of the basis. Consequently, we form a modified UC set
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&Gamma;</mml:mi>
           <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
         </mml:mover>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   by multiplying each member of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&Gamma;</mml:mi>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   by the lowest power of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   so that no member of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&Gamma;</mml:mi>
           <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
         </mml:mover>
         <mml:mi>f</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is a linear combination of members of the basis. It is easy to see that the
   lowest power of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   that works is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   itself. Indeed,
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&sdot;</mml:mo>
         <mml:msub>
           <mml:mi>&Gamma;</mml:mi>
           <mml:mi>f</mml:mi>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>3</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>So
   proceed to use the modified UC
   set<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mover accent="true">
           <mml:mi>&Gamma;</mml:mi>
           <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
         </mml:mover>
         <mml:mi>f</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&sdot;</mml:mo>
         <mml:msub>
           <mml:mi>&Gamma;</mml:mi>
           <mml:mi>f</mml:mi>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>3</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Form
   the trial
   solution<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>B</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
