           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
