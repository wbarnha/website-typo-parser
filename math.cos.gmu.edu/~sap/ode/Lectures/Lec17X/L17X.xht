<p class="QQQExampleText">
   Use the Green's kernel to solve the
   IVP<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>where<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="right left left">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="center">
   
<!-- MSISEMCOM /FRAME{dtbpFU}{5.54in}{1.8671in}{0pt}{}{}{step-up step-down.png}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 5.54in;  height 1.8671in;  depth 0pt;  original-width 4.5939in;  original-height 1.5333in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "5";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L17X__3.png" alt="step-up step-down.png" height="179px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 17.3: Two sided
            <mml:math display="inline">
     <mml:mrow>
       <mml:mn>2</mml:mn>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mi>&pi;</mml:mi>
     </mml:mrow>
   </mml:math>-pulse</span>
         </td>
      </tr></tbody>
</table></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p class="QQQExampleText">
   and plot the input and the solution on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mrow>
           <mml:mn>6</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>As
   we've seen many times, a basis for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   is given by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mspace width="thickmathspace" />
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   and that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>&phi;</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&phi;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">sin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   constitutes an ideal basis. Now compute the integral part of
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(17.03)"> <span class="QQQHyperLink-BodyText">Eqn.
   (17.3)</span></a></span>. From
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec16X/L16X.xht#Theorem16.16"> <span class="QQQHyperLink-BodyText">Theorem
   16.16</span></a></span> we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>G</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">sin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>We
   can take
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>I</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   is piecewise continuous on all of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
     </mml:mrow>
   </mml:math>.
   Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mi>G</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Evaluation
   of the integral depends on where
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   falls. There are four cases:
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <strong>1.<span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <strong>2.<span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <strong>3.<span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mrow>
         <mml:mn>4</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <strong>4.<span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>4</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>&infin;</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><br/><strong>1</strong>.<strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   Here
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   so<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><strong>2</strong>.<strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   Here
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   so<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&theta;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>d</mml:mi>
         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&theta;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>d</mml:mi>
         </mml:mrow>
         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&tau;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   so that
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mi>p</mml:mi>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msubsup>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mi>&theta;</mml:mi>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>d</mml:mi>
             </mml:mrow>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&theta;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>&theta;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&theta;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:msubsup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mi>&tau;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac linethickness="0">
                         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mspace width="thinmathspace" />
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mrow>
               <mml:mspace width="thinmathspace" />
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msubsup>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><strong>3</strong>.<strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mrow>
           <mml:mn>4</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   In this case
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>1.</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   As the integrals start at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then we must remember to let
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   on the interval
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Thus we split the integration over two intervals:
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mspace width="thickmathspace" />
     </mml:mrow>
   </mml:math>and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Hence<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&pi;</mml:mi>
             </mml:mrow>
           </mml:msubsup>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&pi;</mml:mi>
             </mml:mrow>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>In
   Case 2 we
   calculated<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>Thus
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
