         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
   must be split into two integrals.
   Thus<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mspace width="thickmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>g</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mn>1</mml:mn>
           </mml:msubsup>
           <mml:mspace width="thickmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mi>&tau;</mml:mi>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>1</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mspace width="thickmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mn>3</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math><br/>(d)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   The graphs of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>g</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>&tau;</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   overlap on the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&tau;</mml:mi>
     </mml:mrow>
   </mml:math>-interval
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>
   as suggested by Figure 17.19.
</p>
<p class="center">
   
