         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mrow>
               <mml:mn>4</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&pi;</mml:mi>
             </mml:mrow>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>In
   Case 2 we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>and
   in Case 3 we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
           <mml:mrow>
             <mml:mn>4</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mn>4</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>Since
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mn>4</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mi>p</mml:mi>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msubsup>
                       <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                       <mml:mn>0</mml:mn>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                     </mml:msubsup>
                     <mml:mspace width="thinmathspace" />
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>t</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="infix">&sdot;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mspace width="thinmathspace" />
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>d</mml:mi>
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:msubsup>
                       <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mrow>
                         <mml:mn>4</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                     </mml:msubsup>
                     <mml:mspace width="thinmathspace" />
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>t</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="infix">&sdot;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>d</mml:mi>
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:msubsup>
                       <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                       <mml:mrow>
                         <mml:mn>4</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mi>t</mml:mi>
                     </mml:msubsup>
                     <mml:mspace width="thinmathspace" />
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>t</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="infix">&sdot;</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mspace width="thinmathspace" />
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>d</mml:mi>
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mtext mathcolor="black">Answer to Case 2 + Answer to Case 3 +&ensp;0</mml:mtext>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">cos</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">cos</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Thus
   the (piecewise) solution
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="center left left">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="center">
   
