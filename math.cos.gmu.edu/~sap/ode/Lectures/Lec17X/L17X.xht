<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      17.10<a id="Example17.10"></a>
   </p>
</div>
<p class="QQQExampleText">
   <br/>Compute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mo form="infix">&sstarf;</mml:mo>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">&sstarf;</mml:mo>
                   <mml:mrow>
                     <mml:mi>g</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mspace width="thickmathspace" />
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi>f</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mi>g</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mi>&tau;</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mspace width="thickmathspace" />
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>t</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mi>&tau;</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:msup>
                           <mml:mi>&tau;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mspace width="thickmathspace" />
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mi>t</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:msup>
                             <mml:mi>&tau;</mml:mi>
                             <mml:mn>2</mml:mn>
                           </mml:msup>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:msup>
                           <mml:mi>&tau;</mml:mi>
                           <mml:mn>3</mml:mn>
                         </mml:msup>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:msup>
                             <mml:mi>&tau;</mml:mi>
                             <mml:mn>2</mml:mn>
                           </mml:msup>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>12</mml:mn>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>4</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>3</mml:mn>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 17.10
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>It follows immediately from the definition of convolution that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mo form="infix">&sstarf;</mml:mo>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   is <span class="QQQSansSerifBold16">not</span> the same as the product
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">&sdot;</mml:mo>
           <mml:mrow>
             <mml:mi>g</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>t</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
