                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mi>v</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>E</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
