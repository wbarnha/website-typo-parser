             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:msup>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mn>3</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mn>2</mml:mn>
               </mml:msup>
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mn>2</mml:mn>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
