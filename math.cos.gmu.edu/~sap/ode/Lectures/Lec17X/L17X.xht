		  </mlabeledtr>
		</mtable>

   </mml:math>According
   to
   <span class="QQQNote-BodyText"> <a target="_blank" href="L17X__note1.xht"><span class="QQQNote-BodyText">Leibniz's
   Rule for differentiation of integrals</span></a> </span>, we
   have<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(17.7)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 </mml:mrow>
                 <mml:mrow>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:msubsup>
                 <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                 <mml:mn>0</mml:mn>
                 <mml:mi>t</mml:mi>
               </mml:msubsup>
               <mml:mspace width="thinmathspace" />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>G</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mspace width="thinmathspace" />
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi>f</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mspace width="thinmathspace" />
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>d</mml:mi>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
               <mml:mspace width="thinmathspace" />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac linethickness="0">
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi>G</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi>f</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mspace width="thinmathspace" />
               <mml:mrow>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:msub>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:msubsup>
                 <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                 <mml:mn>0</mml:mn>
                 <mml:mi>t</mml:mi>
               </mml:msubsup>
               <mml:mrow>
                 <mml:mspace width="thinmathspace" />
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mo form="prefix">&PartialD;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix">&PartialD;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>Using
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec16X/L16X.xht#Eqn.(16.11)"> <span class="QQQHyperLink-BodyText">Eqn.
   (16.11)</span></a></span>, the first term on the right side of Eqn. (17.7)
   evaluates
   to<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac linethickness="0">
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mi>&tau;</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:msub>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac linethickness="0">
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>1</mml:mn>
                         </mml:msub>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msub>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msub>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>1</mml:mn>
                         </mml:msub>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mi>W</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">[</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>1</mml:mn>
                         </mml:msub>
                         <mml:mo form="infix">,</mml:mo>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msub>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">]</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mi>&tau;</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
</p>
