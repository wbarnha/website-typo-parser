<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      17.12<a id="Example17.12"></a>
   </p>
</div>
<p class="QQQExampleText">
   Compute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>w</mml:mi>
       <mml:mo form="infix">&sstarf;</mml:mo>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   when<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>w</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mtable align="axis" columnalign="right left left">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext mathcolor="black">if</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&infin;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext mathcolor="black">if</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext mathcolor="black">if</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mtable align="axis" columnalign="right left left">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext mathcolor="black">if</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&infin;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext mathcolor="black">if</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>and
   plot
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">&sstarf;</mml:mo>
       <mml:mrow>
         <mml:mi>w</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="false">[</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="false">]</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(17.14)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>w</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">&sstarf;</mml:mo>
           <mml:mrow>
             <mml:mi>g</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mspace width="thickmathspace" />
           <mml:mrow>
             <mml:mrow>
               <mml:mi>g</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>w</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p class="QQQExampleText">
   The calculation of the integral in Eqn. (17.13) must be evaluated in a
   piecewise fashion, much like was done in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec06X/L06X.xht#Example06.08"> <span class="QQQHyperLink-BodyText">Example
   6.8</span></a></span> with the integral formula method. Thus
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>w</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&sstarf;</mml:mo>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="left left left">
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mspace width="thickmathspace" />
                   <mml:mrow>
                     <mml:mn>0</mml:mn>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mspace width="thinmathspace" />
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;</mml:mtext>
               <mml:mrow>
                 <mml:mi>d</mml:mi>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mtext>&ThickSpace;&ThinSpace;</mml:mtext>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>t</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mi>&tau;</mml:mi>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mspace width="thinmathspace" />
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;</mml:mtext>
               <mml:mrow>
                 <mml:mi>d</mml:mi>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mn>1</mml:mn>
                   </mml:msubsup>
                   <mml:mtext>&ThickSpace;&ThinSpace;</mml:mtext>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>t</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mi>&tau;</mml:mi>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mspace width="thinmathspace" />
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;</mml:mtext>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>1</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mtext>&ThickSpace;&ThinSpace;</mml:mtext>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>t</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mi>&tau;</mml:mi>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>where
   the case for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>&infin;</mml:mi>
     </mml:mrow>
   </mml:math>
   must be split into two integrals: the first over
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>&tau;</mml:mi>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   where
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>w</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>&tau;</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   and the second over
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>&tau;</mml:mi>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   where
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>w</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>&tau;</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   In particular,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>w</mml:mi>
     </mml:mrow>
   </mml:math>
   jumps from
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&tau;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
   Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>w</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
