               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">1.</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Zero:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">2.</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Commutative:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mi>g</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mi>g</mml:mi>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mi>f</mml:mi>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">3.</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Associative:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>f</mml:mi>
                         <mml:mo form="infix">&sstarf;</mml:mo>
                         <mml:mi>g</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mi>h</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>g</mml:mi>
                         <mml:mo form="infix">&sstarf;</mml:mo>
                         <mml:mi>h</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">4.</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Distributive:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>g</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mi>h</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi>f</mml:mi>
                       <mml:mo form="infix">&sstarf;</mml:mo>
                       <mml:mi>g</mml:mi>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi>f</mml:mi>
                       <mml:mo form="infix">&sstarf;</mml:mo>
                       <mml:mi>h</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">5.</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Scalar Multiplication:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>g</mml:mi>
                         <mml:mo form="infix">&sstarf;</mml:mo>
                         <mml:mi>h</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>f</mml:mi>
                         <mml:mo form="infix">&sdot;</mml:mo>
                         <mml:mi>g</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mi>h</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mi>g</mml:mi>
                     <mml:mo form="infix">&sstarf;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>f</mml:mi>
                         <mml:mo form="infix">&sdot;</mml:mo>
                         <mml:mi>h</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mtext mathcolor="black">where&ensp;</mml:mtext>
           <mml:mo form="infix">&sdot;</mml:mo>
           <mml:mtext mathcolor="black">&ensp;represents scalar multiplication; e.g .,&ensp;</mml:mtext>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo form="infix">&sdot;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>g</mml:mi>
               <mml:mo form="infix">&sstarf;</mml:mo>
               <mml:mi>h</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;is the function</mml:mtext>
       <mml:mspace width="thickmathspace" />
       <mml:mtext mathcolor="black">whose value at&ensp;</mml:mtext>
       <mml:mi>t</mml:mi>
       <mml:mtext mathcolor="black">&ensp;is&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">&sdot;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>g</mml:mi>
               <mml:mo form="infix">&sstarf;</mml:mo>
               <mml:mi>f</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif16">Proof:<span style="width:1em"><span style="padding-left:1em"></span></span></span>We
   just prove the commutative
   property: <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo form="infix">&sstarf;</mml:mo>
           <mml:mi>g</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo form="infix">&sstarf;</mml:mo>
           <mml:mi>f</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   So
   consider<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&sstarf;</mml:mo>
         <mml:mrow>
           <mml:mi>g</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>g</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Change
   variables: set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>z</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>&tau;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   so that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&tau;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>z</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>d</mml:mi>
         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&tau;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mo form="infix">&minus;</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>d</mml:mi>
         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>z</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   (Note that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   is "constant" with respect to this change.) With
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>z</mml:mi>
     </mml:mrow>
   </mml:math>
   as the new variable of integration, the limits of integration go from
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>z</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>z</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Thus<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi>f</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">&sstarf;</mml:mo>
                   <mml:mrow>
                     <mml:mi>g</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
