                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>b</mml:mi>
                      <mml:mrow>
                        <mml:mi>r</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>n</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                </mml:mtr>
              </mml:mtable>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
            <mml:mo form="infix">=</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mtable align="axis">
                <mml:mtr>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>2</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>j</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>1</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>n</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>2</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>2</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>2</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>2</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>j</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mn>2</mml:mn>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>n</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&dtdot;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&dtdot;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>i</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>i</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>2</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>i</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>j</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>i</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>n</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&dtdot;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&dtdot;</mml:mtext>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mtext>&vellip;</mml:mtext>
                  </mml:mtd>
                </mml:mtr>
                <mml:mtr>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>m</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>1</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>m</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mn>2</mml:mn>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>m</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>j</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:mi>&ctdot;</mml:mi>
                  </mml:mtd>
                  <mml:mtd>
                    <mml:msub>
                      <mml:mi>c</mml:mi>
                      <mml:mrow>
                        <mml:mi>m</mml:mi>
                        <mml:mo>&InvisibleComma;</mml:mo>
                        <mml:mi>n</mml:mi>
                      </mml:mrow>
                    </mml:msub>
                  </mml:mtd>
                </mml:mtr>
              </mml:mtable>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
        </mml:mrow>
      </mml:math>
</p>
<p>
   <span class="vspace" style="height:-0.20in"></span>
</p>
<p>
   <span style="width:4in"><span style="padding-left:4in"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&uarr;</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <span style="width:4.05in"><span style="padding-left:4.05in"></span></span><span class="vspace" style="height:-0.25in"></span><strong>b</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mrow>
           <mml:mspace width="negativethinmathspace" />
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math><span class="vspace" style="height:0.3in"></span>
</p>
<p>
   In order for the product <strong>AB </strong>to be possible, the dimension of
   the row vectors
   <strong>a</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mi>i</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   must be the same as the dimension of the column vectors
   <strong>b</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mi>j</mml:mi>
       </mml:msub>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   equivalently, number of columns of <strong>A </strong>must equal the number of
   rows of <strong>B</strong>.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      A.5
   </p>
</div>
<p>
   <br/><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mtable align="axis" columnalign="right right right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>2</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>2</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>3</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>3</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>2</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mspace width="0.100000in" />
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>2</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>2</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>2</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>2</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>8</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>3</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>Note that the scalar product
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math><strong>v
   </strong>of two
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>-dimensional
   (column) vectors can be expressed as a matrix product by writing
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">u</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">&bull;</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">&ensp;=&ensp;</mml:mtext>
       <mml:msup>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">u</mml:mtext>
         </mml:mstyle>
         <mml:mi>T</mml:mi>
       </mml:msup>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Thus
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math><strong>v
   </strong>is the matrix product of the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
   row vector
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mi>T</mml:mi>
       </mml:msup>
     </mml:mrow>
   </mml:math><strong> </strong>and
   the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   column vector <strong>v</strong>.
</p>
<p>
   <span style="width:0.5in"><span style="padding-left:0.5in"></span></span>The
   zero and identity matrices behave like the numbers 0 and 1 of arithmetic:
</p>
<p>
   <br/><br/><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A + 0 = 0 = A = A;</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mtext>&emsp;&emsp;</mml:mtext>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">AI = IA =&ensp;A</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<ul>
   <li>
      <p>
         <strong>Inverse</strong>:<span style="width:1em"><span style="padding-left:1em"></span></span>Suppose
         <strong>A</strong> is an
         <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
         (square) matrix. If there is an
         <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
         matrix <strong>B</strong> so that both <strong>AB </strong>=
         <strong>BA</strong> = <strong>I</strong>, then <strong>B</strong> is called
         the <strong>multiplicative inverse</strong> of
         <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
         If <strong>A</strong> has an inverse, it can be shown that this inverse is
         unique. Thus we denote the inverse of <strong>A</strong> (whenever it exits)
         by
         <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
         It also can be shown that whenever
         <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math>
         does
         exist<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">AA</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mspace width="thickmathspace" />
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:msup>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">A</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msup>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I,</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mtext>&emsp;&emsp;</mml:mtext>
         </mml:mstyle>
       </mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:msup>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">A</mml:mtext>
               </mml:mstyle>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
             </mml:msup>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mspace width="thickmathspace" />
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
      </p>
   </li>
</ul>
<p>
   We say that <strong>A</strong> is <strong>invertible</strong>
   (<strong>nonsingular</strong>) if
   <strong>A</strong><mml:math display="inline">
        <mml:mrow>
          <mml:msup>
            <mml:mspace width="veryverythinmathspace" />
            <mml:mrow>
              <mml:mo form="prefix">&minus;</mml:mo>
              <mml:mn>1</mml:mn>
            </mml:mrow>
          </mml:msup>
        </mml:mrow>
      </mml:math>
   exists, and <strong>A</strong> is <strong>noninvertible</strong>
   (<strong>singular) </strong>if
   <strong>A</strong><mml:math display="inline">
        <mml:mrow>
          <mml:msup>
            <mml:mspace width="veryverythinmathspace" />
            <mml:mrow>
              <mml:mo form="prefix">&minus;</mml:mo>
              <mml:mn>1</mml:mn>
            </mml:mrow>
          </mml:msup>
        </mml:mrow>
      </mml:math>
   doesn't exist.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="theorem">
   <span class="leadin">Theorem</span><p class="theorem-FirstPara">
      A.6 [Properties of Vectors &amp; Matrices]
   </p>
</div>
<p class="QQQTheoremText">
   Let <strong>A</strong>, <strong>B</strong>,
   <strong>C</strong>,<mml:math display="inline">
     <mml:mrow>
       <mml:mi>&hellip;</mml:mi>
     </mml:mrow>
   </mml:math>
   be matrices, <strong>x</strong>, <strong>y, z</strong>,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&hellip;</mml:mi>
     </mml:mrow>
   </mml:math>
   vectors, and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   scalars. Then for appropriate dimensions, all matrices and vectors satisfy the
   following properties
</p>
<p>
   <br/><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" rowlines="solid none none none none none none none none" columnalign="left left left left">
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">PROPERTY</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext mathcolor="black">MATRICES</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext mathcolor="black">&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext mathcolor="black">VECTORS</mml:mtext>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">Commutativity:</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A + B = B +&ensp;A</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">x + y = y +&ensp;x</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">x</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="infix">&bull;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">y =&ensp;y</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="infix">&bull;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">x</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">Associativity:</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">(A + B) + C = A + (B + C)</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">(x + y) + z = x + (y + z)</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">(AB)C</mml:mtext>
             </mml:mstyle>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">= A(BC)</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>x</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">=&ensp;</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mstyle fontweight="bold" fontstyle="upright">
                       <mml:mi>x</mml:mi>
                     </mml:mstyle>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>A</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">=&ensp;</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mstyle fontweight="bold" fontstyle="upright">
                       <mml:mi>A</mml:mi>
                     </mml:mstyle>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">Distributivity:</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A +&ensp;B</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mo form="infix">=</mml:mo>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mo>+</mml:mo>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">B</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mtext mathcolor="black">(</mml:mtext>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">x +&ensp;y</mml:mtext>
             </mml:mstyle>
             <mml:mtext mathcolor="black">)&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mo form="infix">=</mml:mo>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>x</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mo>+</mml:mo>
             </mml:mrow>
             <mml:mtext mathcolor="black">&ensp;</mml:mtext>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>y</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>A</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle fontweight="bold" fontstyle="upright">
                     <mml:mi>A</mml:mi>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle fontweight="bold" fontstyle="upright">
                     <mml:mi>A</mml:mi>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>x</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle fontweight="bold" fontstyle="upright">
                     <mml:mi>x</mml:mi>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle fontweight="bold" fontstyle="upright">
                     <mml:mi>x</mml:mi>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">C(A + B) = CA + CB</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">x</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="infix">&bull;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">(y + z) =&ensp;x</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="infix">&bull;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">y +&ensp;x</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mstyle fontweight="bold" fontstyle="upright">
                 <mml:mo form="infix">&bull;</mml:mo>
               </mml:mstyle>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">z</mml:mtext>
             </mml:mstyle>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
