             <mml:mrow>
               <mml:mi>n</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   matrix obtained from <strong>A</strong> by deleting the
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>i</mml:mi>
         <mml:mtext mathcolor="black">th</mml:mtext>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   row and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>j</mml:mi>
         <mml:mtext mathcolor="black">th&ensp;</mml:mtext>
       </mml:msup>
     </mml:mrow>
   </mml:math>column
   of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mi>A</mml:mi>
         </mml:mstyle>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   det <strong>A </strong>can be computed by the <strong>cofactor expansion along
   the
   </strong><mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>i</mml:mi>
         <mml:mtext mathcolor="black">th</mml:mtext>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   row of <strong>A </strong>from
</p>
<p class="QQQSpecialText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtext mathcolor="black">det&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mi>A</mml:mi>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">&ensp;=</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">&ensp;det</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&dtdot;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>i</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>i</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>i</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&dtdot;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:munder>
             <mml:mover>
               <mml:mo form="prefix" largeop="true">&sum;</mml:mo>
               <mml:mi>n</mml:mi>
             </mml:mover>
             <mml:mrow>
               <mml:mi>j</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:munder>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>i</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>j</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>a</mml:mi>
             <mml:mrow>
               <mml:mi>i</mml:mi>
               <mml:mo>&InvisibleComma;</mml:mo>
               <mml:mi>j</mml:mi>
             </mml:mrow>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;det&ensp;</mml:mtext>
       <mml:msub>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mi>A</mml:mi>
           </mml:mstyle>
         </mml:mrow>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msub>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">(</mml:mtext>
       <mml:mi>i</mml:mi>
       <mml:mtext mathcolor="black">&ensp;fixed)</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQSpecialText">
   or the cofactor expansion along the
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>j</mml:mi>
         <mml:mtext mathcolor="black">th</mml:mtext>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   column of <strong>A</strong> by
</p>
<p class="QQQSpecialText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtext mathcolor="black">det&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mi>A</mml:mi>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">&ensp;=</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">&ensp;det</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>j</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>j</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&dtdot;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&dtdot;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>j</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:munder>
             <mml:mover>
               <mml:mo form="prefix" largeop="true">&sum;</mml:mo>
               <mml:mi>n</mml:mi>
             </mml:mover>
             <mml:mrow>
               <mml:mi>i</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:munder>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>i</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>j</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>a</mml:mi>
             <mml:mrow>
               <mml:mi>i</mml:mi>
               <mml:mo>&InvisibleComma;</mml:mo>
               <mml:mi>j</mml:mi>
             </mml:mrow>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;det&ensp;</mml:mtext>
       <mml:msub>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mi>A</mml:mi>
           </mml:mstyle>
         </mml:mrow>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msub>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">(</mml:mtext>
       <mml:mi>j</mml:mi>
       <mml:mtext mathcolor="black">&ensp;fixed)</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQSpecialText">
   The number
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msup>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>det
   <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is called the <strong>cofactor </strong>of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>a</mml:mi>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mi>j</mml:mi>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>.
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>It can be shown that det <strong>A</strong> has the same value no matter
   which row or column is used. The most efficient way to calculate det
   <strong>A</strong> is to row reduce (and multiply the diagonal entries).
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      A.17
   </p>
</div>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
                   <mml:mspace width="thinmathspace" />
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mtable align="axis" columnalign="right right right">
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>1</mml:mn>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mn>2</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>0</mml:mn>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mn>0</mml:mn>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mn>1</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>2</mml:mn>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mn>1</mml:mn>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                     </mml:mtable>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mtable align="axis" columnalign="right right">
                         <mml:mtr>
                           <mml:mtd>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                           <mml:mtd>
                             <mml:mn>2</mml:mn>
                           </mml:mtd>
                         </mml:mtr>
                         <mml:mtr>
                           <mml:mtd>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                           <mml:mtd>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                         </mml:mtr>
                       </mml:mtable>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mtable align="axis" columnalign="right right">
                         <mml:mtr>
                           <mml:mtd>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                           <mml:mtd>
                             <mml:mn>2</mml:mn>
                           </mml:mtd>
                         </mml:mtr>
                         <mml:mtr>
                           <mml:mtd>
                             <mml:mn>2</mml:mn>
                           </mml:mtd>
                           <mml:mtd>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                         </mml:mtr>
                       </mml:mtable>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo form="infix">&sdot;</mml:mo>
                       <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mtable align="axis" columnalign="right right">
                         <mml:mtr>
                           <mml:mtd>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                           <mml:mtd>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                         </mml:mtr>
                         <mml:mtr>
                           <mml:mtd>
                             <mml:mn>2</mml:mn>
                           </mml:mtd>
                           <mml:mtd>
                             <mml:mn>1</mml:mn>
                           </mml:mtd>
                         </mml:mtr>
                       </mml:mtable>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="false">[</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mn>2</mml:mn>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="false">]</mml:mo>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>3</mml:mn>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p>
   where the determinant has been computed by cofactor expansion of the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>3</mml:mn>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mn>3</mml:mn>
     </mml:mrow>
   </mml:math>
   matrix along the second row.
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   Although row reduction methods for solving a system <strong>Ax = b</strong>
   are more efficient, Cramer's rule has its advantages for two- and
   three-dimensional systems.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="theorem">
   <span class="leadin">Theorem</span><p class="theorem-FirstPara">
      A.18 [Cramer's Rule]<a id="Cramer"></a>
   </p>
</div>
<p class="QQQTheoremText">
   Suppose det <strong>A</strong>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&ne;</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   for the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
   system <strong>Ax =
   b</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   where<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mi>A</mml:mi>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&dtdot;</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>a</mml:mi>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>n</mml:mi>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mi>x</mml:mi>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mi>n</mml:mi>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mi>b</mml:mi>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>b</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>b</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mtext>&vellip;</mml:mtext>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>b</mml:mi>
                   <mml:mi>n</mml:mi>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQTheoremText">
   Then
</p>
<p class="QQQTheoremText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>j</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mfrac>
           <mml:mrow>
             <mml:mrow>
               <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
               <mml:msub>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">A</mml:mtext>
                 </mml:mstyle>
                 <mml:mi>j</mml:mi>
               </mml:msub>
             </mml:mrow>
             <mml:mtext mathcolor="black">(</mml:mtext>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">b</mml:mtext>
             </mml:mstyle>
             <mml:mtext mathcolor="black">)</mml:mtext>
           </mml:mrow>
           <mml:mrow>
             <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
           </mml:mrow>
         </mml:mfrac>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mi>j</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mn>2</mml:mn>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>&hellip;</mml:mi>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQTheoremText">
   where
   <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mi>j</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>(<strong>b</strong>)
   is the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
   matrix obtained from <strong>A</strong> by replacing the
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>j</mml:mi>
         <mml:mtext mathcolor="black">th</mml:mtext>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   column of <strong>A</strong> by
   <strong>b</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>In the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>2</mml:mn>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
   case, Cramer's rule provides
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo form="infix">=</mml:mo>
           <mml:mfrac>
             <mml:mrow>
               <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
               <mml:mspace width="thinmathspace" />
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>b</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>a</mml:mi>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>b</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>a</mml:mi>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
               <mml:mspace width="thinmathspace" />
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>a</mml:mi>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>a</mml:mi>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>a</mml:mi>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>a</mml:mi>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mfrac>
         </mml:mrow>
         <mml:mo form="postfix">;</mml:mo>
       </mml:mrow>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mfrac>
           <mml:mrow>
             <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
             <mml:mspace width="thinmathspace" />
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>a</mml:mi>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>b</mml:mi>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>a</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>b</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mrow>
             <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
             <mml:mspace width="thinmathspace" />
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>a</mml:mi>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>a</mml:mi>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>a</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:msub>
                       <mml:mi>a</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mfrac>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   Inversion of matrices using determinants is straightforward for a
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>2</mml:mn>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
   matrix <strong>A</strong> when det <strong>A
   </strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&ne;</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   If
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle fontweight="bold" fontstyle="upright">
               <mml:mi>A</mml:mi>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;&emsp;</mml:mtext>
         <mml:mtext mathcolor="black">then</mml:mtext>
       </mml:mrow>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msup>
           <mml:mrow>
             <mml:mstyle fontweight="bold" fontstyle="upright">
               <mml:mi>A</mml:mi>
             </mml:mstyle>
           </mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mfrac>
             <mml:mn>1</mml:mn>
             <mml:mrow>
               <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mi>A</mml:mi>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
           </mml:mfrac>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mtable align="axis" columnalign="right right">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>a</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   More generally, for the
   <mml:math display="inline">
