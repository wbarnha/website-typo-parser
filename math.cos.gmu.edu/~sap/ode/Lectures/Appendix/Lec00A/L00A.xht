           </mml:msup>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
                 <mml:mrow>
                   <mml:mstyle fontweight="bold" fontstyle="upright">
                     <mml:mi>A</mml:mi>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mfrac>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>C</mml:mi>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>C</mml:mi>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:mi>&ctdot;</mml:mi>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>C</mml:mi>
                         <mml:mrow>
                           <mml:mn>1</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mi>n</mml:mi>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>C</mml:mi>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                       </mml:msub>
                     </mml:mtd>
                     <mml:mtd>
                       <mml:msub>
                         <mml:mi>C</mml:mi>
                         <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleComma;</mml:mo>
                           <mml:mn>2</mml:mn>
