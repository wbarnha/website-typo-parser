</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mover>
         <mml:mo form="infix">&xrarr;</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi fontstyle="upright">&Rscr;</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo form="infix">&rarr;</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>4</mml:mn>
                 </mml:mfrac>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mi fontstyle="upright">&Rscr;</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
         </mml:mrow>
       </mml:mover>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mspace width="0.100000in" />
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
       </mml:mrow>
       <mml:mspace width="0.100000in" />
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>3</mml:mn>
                     <mml:mn>8</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle displaystyle="false" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>4</mml:mn>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mover>
           <mml:mo form="infix">&xrarr;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi fontstyle="upright">&Rscr;</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">&rarr;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi fontstyle="upright">&Rscr;</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi fontstyle="upright">&Rscr;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mover>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mspace width="0.100000in" />
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mspace width="0.100000in" />
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
         <mml:mtable align="axis" columnalign="right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>8</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mtd>
             <mml:mtd>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>4</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>3</mml:mn>
                   <mml:mn>8</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mtd>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>4</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
