                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>5</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>3</mml:mn>
                   </mml:mrow>
                   <mml:mo form="infix">&lt;</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mtext mathcolor="black">&emsp;so&ensp;</mml:mtext>
               </mml:mrow>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;is an attractor</mml:mtext>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>2.<span class="QQQNote-BodyText"><span style="width:1em"><span style="padding-left:1em"></span></span></span><a id="Example10.16Eqn2"></a>Since
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we see that there are equilibria at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math><span class="QQQNote-BodyText"> </span>Set<span class="QQQNote-BodyText"> </span><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Calculate<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi>d</mml:mi>
               <mml:mspace width="negativethinmathspace" />
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi>f</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mfrac>
           <mml:mi>d</mml:mi>
           <mml:mrow>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mfrac>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msup>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>x</mml:mi>
               <mml:mn>3</mml:mn>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>3</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mspace width="negativethinmathspace" />
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mrow>
                                 <mml:mi>f</mml:mi>
                                 <mml:mo>&ApplyFunction;</mml:mo>
                                 <mml:mrow>
                                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                   <mml:mi>x</mml:mi>
                                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                 </mml:mrow>
                               </mml:mrow>
                             </mml:mrow>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:maligngroup />
                   <mml:mo form="infix">=</mml:mo>
                   <mml:maligngroup />
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mn>2</mml:mn>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mrow>
                               <mml:mn>3</mml:mn>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:msup>
                                 <mml:mi>x</mml:mi>
                                 <mml:mn>2</mml:mn>
                               </mml:msup>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mtext mathcolor="black">&emsp;so Theorem 10.15 is silent at&ensp;</mml:mtext>
               </mml:mrow>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mspace width="negativethinmathspace" />
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mrow>
                                 <mml:mi>f</mml:mi>
                                 <mml:mo>&ApplyFunction;</mml:mo>
                                 <mml:mrow>
                                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                   <mml:mi>x</mml:mi>
                                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                 </mml:mrow>
                               </mml:mrow>
                             </mml:mrow>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:maligngroup />
                   <mml:mo form="infix">=</mml:mo>
                   <mml:maligngroup />
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mn>2</mml:mn>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mrow>
                               <mml:mn>3</mml:mn>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:msup>
                                 <mml:mi>x</mml:mi>
                                 <mml:mn>2</mml:mn>
                               </mml:msup>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mtext mathcolor="black">&emsp;so&ensp;</mml:mtext>
               </mml:mrow>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;is an attractor</mml:mtext>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>3.<span class="QQQNote-BodyText"><span style="width:1em"><span style="padding-left:1em"></span></span></span>There
   are equilibria at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>K</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math><span class="QQQNote-BodyText"> </span>Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>r</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mi>x</mml:mi>
                     <mml:mi>K</mml:mi>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and expand to obtain
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>r</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mi>r</mml:mi>
                   <mml:mi>K</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Calculate<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi>d</mml:mi>
               <mml:mspace width="negativethinmathspace" />
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi>f</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mfrac>
           <mml:mi>d</mml:mi>
           <mml:mrow>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mfrac>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>r</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mi>r</mml:mi>
                     <mml:mi>K</mml:mi>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>x</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>r</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mfrac>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>r</mml:mi>
             </mml:mrow>
             <mml:mi>K</mml:mi>
           </mml:mfrac>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mspace width="negativethinmathspace" />
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mrow>
                                 <mml:mi>f</mml:mi>
                                 <mml:mo>&ApplyFunction;</mml:mo>
                                 <mml:mrow>
                                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                   <mml:mi>x</mml:mi>
                                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                 </mml:mrow>
                               </mml:mrow>
                             </mml:mrow>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:maligngroup />
                   <mml:mo form="infix">=</mml:mo>
                   <mml:maligngroup />
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>r</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mrow>
                               <mml:mfrac>
                                 <mml:mrow>
                                   <mml:mn>2</mml:mn>
                                   <mml:mo>&InvisibleTimes;</mml:mo>
                                   <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mi>K</mml:mi>
                               </mml:mfrac>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mi>r</mml:mi>
                   <mml:mo form="infix">></mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mtext mathcolor="black">&emsp;so&ensp;</mml:mtext>
               </mml:mrow>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;is a repeller</mml:mtext>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mspace width="negativethinmathspace" />
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mrow>
                                 <mml:mi>f</mml:mi>
                                 <mml:mo>&ApplyFunction;</mml:mo>
                                 <mml:mrow>
                                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                   <mml:mi>x</mml:mi>
                                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                 </mml:mrow>
                               </mml:mrow>
                             </mml:mrow>
                             <mml:mrow>
                               <mml:mi>d</mml:mi>
                               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:maligngroup />
                   <mml:mo form="infix">=</mml:mo>
                   <mml:maligngroup />
                   <mml:msub>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>r</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mrow>
                               <mml:mfrac>
                                 <mml:mrow>
                                   <mml:mn>2</mml:mn>
                                   <mml:mo>&InvisibleTimes;</mml:mo>
                                   <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mi>K</mml:mi>
                               </mml:mfrac>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>x</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mi>K</mml:mi>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>r</mml:mi>
                   </mml:mrow>
                   <mml:mo form="infix">&lt;</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mtext mathcolor="black">&emsp;so&ensp;</mml:mtext>
               </mml:mrow>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mi>K</mml:mi>
               </mml:mrow>
               <mml:mtext mathcolor="black">&ensp;is an attractor</mml:mtext>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
