           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
