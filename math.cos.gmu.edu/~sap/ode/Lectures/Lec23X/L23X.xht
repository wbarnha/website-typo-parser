                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   So<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
         </mml:mrow>
       </mml:msup>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mi mathcolor="gray">cosh</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mtd>
             <mml:mtd>
               <mml:mi mathcolor="gray">sinh</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mi mathcolor="gray">sinh</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mtd>
             <mml:mtd>
               <mml:mi mathcolor="gray">cosh</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 23.19
   </p>
</div>
