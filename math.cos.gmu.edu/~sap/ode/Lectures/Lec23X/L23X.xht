       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a solution and</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">v</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">v</mml:mtext>
             </mml:mstyle>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a  basis,</span>
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">must be a linear combination of</span>
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">Thus there are constants</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>b</mml:mi>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">and</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>b</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">so that</span>
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msub>
         <mml:mi>b</mml:mi>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:msub>
         <mml:mi>b</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">(The choice of subscripts will become clear
   shortly.)
   Since</span><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">V</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mspace width="thickmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mfrac linethickness="0">
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             </mml:mfrac>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifItalic16">then
   we can
   write</span><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">u</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">v</mml:mtext>
                 </mml:mstyle>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mspace width="thickmathspace" />
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mfrac linethickness="0">
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               </mml:mfrac>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">v</mml:mtext>
                 </mml:mstyle>
                 <mml:mn>2</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>b</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>b</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">V</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>b</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>b</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math><span class="QQQSansSerifItalic16">Likewise</span>
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is also a solution; thus there are
   constants</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   so that
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msub>
         <mml:mi>b</mml:mi>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:msub>
         <mml:mi>b</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">Hence</span><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">u</mml:mtext>
             </mml:mstyle>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">v</mml:mtext>
                 </mml:mstyle>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mspace width="thickmathspace" />
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mfrac linethickness="0">
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               </mml:mfrac>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">v</mml:mtext>
                 </mml:mstyle>
                 <mml:mn>2</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
