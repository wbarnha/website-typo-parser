         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mover>
