           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                   <mml:mn>0</mml:mn>
                   <mml:mi>T</mml:mi>
                 </mml:msubsup>
                 <mml:mspace width="thickmathspace" />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>s</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mspace width="thinmathspace" />
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                         <mml:mrow>
                           <mml:mstyle displaystyle="true" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mi>s</mml:mi>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>s</mml:mi>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                     <mml:mi>T</mml:mi>
                   </mml:mrow>
                 </mml:msubsup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mi>s</mml:mi>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>s</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>T</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mi>s</mml:mi>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>s</mml:mi>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>T</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mtext>&emsp;&emsp;</mml:mtext>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">Re</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>s</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">></mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Using
   the notation
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>u</mml:mi>
         <mml:mi>T</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   to denote the time translation of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>u</mml:mi>
     </mml:mrow>
   </mml:math>
   by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   so
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>u</mml:mi>
           <mml:mi>T</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>u</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mi>T</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>we
   have that
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle fontweight="bold" fontstyle="italic">
               <mml:mi fontstyle="upright">&Lscr;</mml:mi>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
             <mml:mrow>
               <mml:mi>u</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:msub>
                 <mml:mi>u</mml:mi>
                 <mml:mi>T</mml:mi>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mi>s</mml:mi>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>s</mml:mi>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>T</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">Re</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">></mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
