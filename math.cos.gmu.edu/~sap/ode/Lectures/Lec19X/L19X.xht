           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">></mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">cos</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mrow>
                             <mml:mstyle displaystyle="true" scriptlevel="0">
                               <mml:mfrac>
                                 <mml:mn>1</mml:mn>
                                 <mml:mrow>
                                   <mml:mi>s</mml:mi>
                                   <mml:mo form="infix">&minus;</mml:mo>
                                   <mml:mi>i</mml:mi>
                                 </mml:mrow>
                               </mml:mfrac>
                             </mml:mstyle>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>0</mml:mn>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle displaystyle="true" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mrow>
                                 <mml:mi>s</mml:mi>
                                 <mml:mo form="infix">+</mml:mo>
                                 <mml:mi>i</mml:mi>
                               </mml:mrow>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>0</mml:mn>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mi>i</mml:mi>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:mi>i</mml:mi>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mi>s</mml:mi>
                     <mml:mrow>
                       <mml:msup>
                         <mml:mi>s</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math><br/>
</p>
<p class="QQQExampleText">
   If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">Re</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">></mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>&alpha;</mml:mi>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">&rarr;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mspace width="thickmathspace" />
     </mml:mrow>
   </mml:math>as<mml:math display="inline">
     <mml:mrow>
       <mml:mspace width="thickmathspace" />
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&rarr;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It follows
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mi>s</mml:mi>
               <mml:mrow>
                 <mml:msup>
                   <mml:mi>s</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">Re</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">></mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
