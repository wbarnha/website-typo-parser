                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>&beta;</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mrow>
           </mml:msubsup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msubsup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mrow>
                         <mml:mi>s</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mi>i</mml:mi>
                       </mml:mrow>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>&alpha;</mml:mi>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>i</mml:mi>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>&beta;</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mrow>
           </mml:msubsup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&alpha;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">Re</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
