           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mrow>
                   <mml:mi>s</mml:mi>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mn>1</mml:mn>
