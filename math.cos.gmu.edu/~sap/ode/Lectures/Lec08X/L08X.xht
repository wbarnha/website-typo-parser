                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">Right-side derivative at&ensp;</mml:mtext>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">=</mml:mo>
