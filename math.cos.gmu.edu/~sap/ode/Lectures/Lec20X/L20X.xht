             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mspace width="thickmathspace" />
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mi>&tau;</mml:mi>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mspace width="thickmathspace" />
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msubsup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac linethickness="0">
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mrow>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msubsup>
         </mml:mrow>
         <mml:mspace width="thinmathspace" />
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   As the inverse LT must be causal, multiply the result by the unit step
   function. We get
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>s</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>s</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
