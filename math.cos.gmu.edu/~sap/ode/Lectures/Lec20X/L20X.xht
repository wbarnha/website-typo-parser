                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:munderover>
                   <mml:mo form="prefix" movablelimits="false" largeop="true">&sum;</mml:mo>
                   <mml:mrow>
                     <mml:mi>n</mml:mi>
                     <mml:mo form="infix">=</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                   <mml:mi>&infin;</mml:mi>
                 </mml:munderover>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mn>50</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mn>2</mml:mn>
                             </mml:mrow>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mrow>
                                   <mml:mn>10</mml:mn>
                                   <mml:mo>&InvisibleTimes;</mml:mo>
                                   <mml:mi>n</mml:mi>
                                 </mml:mrow>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:mn>50</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>t</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mrow>
                               <mml:mn>10</mml:mn>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>n</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mrow>
                                   <mml:mn>10</mml:mn>
                                   <mml:mo>&InvisibleTimes;</mml:mo>
                                   <mml:mi>n</mml:mi>
                                 </mml:mrow>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi>u</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>t</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mn>10</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>n</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>A
   plot of the first few periods of
   <mml:math display="inline">
