                         <mml:mn>4</mml:mn>
                       </mml:msup>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mn>6</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>s</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mn>1</mml:mn>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mn>6</mml:mn>
                       </mml:mrow>
                       <mml:msup>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mn>4</mml:mn>
                       </mml:msup>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mrow>
                         <mml:mn>6</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:msup>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mn>4</mml:mn>
                       </mml:msup>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>6</mml:mn>
                       <mml:msup>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mn>4</mml:mn>
                       </mml:msup>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>6</mml:mn>
                       <mml:msup>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
