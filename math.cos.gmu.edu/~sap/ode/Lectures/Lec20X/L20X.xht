<p class="QQQExampleText">
   Use the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>-periodiodicity
   property to calculate
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Lscr;</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mi>f</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   where<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="right left left">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>n</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>n</mml:mi>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>n</mml:mi>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>n</mml:mi>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>n</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>2</mml:mn>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   ... .
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:<span style="width:1em"><span style="padding-left:1em"></span></span></span>This
   is the precise function illustrated in
   <span class="QQQHyperLink-BodyText"><a href="#Example20.12"> <span class="QQQHyperLink-BodyText">Example
   20.12</span></a></span>. Now we apply the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>-periodicity
   property to compute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi fontstyle="upright">&Lscr;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mi>f</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Note that the period
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>T</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
             <mml:mi>f</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&pi;</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>s</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
         </mml:msubsup>
         <mml:mspace width="thinmathspace" />
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>s</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                   <mml:mn>0</mml:mn>
                   <mml:mi>&pi;</mml:mi>
                 </mml:msubsup>
                 <mml:mspace width="thinmathspace" />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>s</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                   <mml:mi>&pi;</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&pi;</mml:mi>
                   </mml:mrow>
                 </mml:msubsup>
                 <mml:mspace width="thinmathspace" />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>s</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&sdot;</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&pi;</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>s</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
