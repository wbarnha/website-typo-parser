             <mml:mi>a</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi>s</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>a</mml:mi>
             </mml:mrow>
             <mml:mrow>
               <mml:msup>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>s</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>a</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo form="infix">+</mml:mo>
               <mml:msup>
                 <mml:mi>b</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
