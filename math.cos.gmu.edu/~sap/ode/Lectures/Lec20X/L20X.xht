                 </mml:mrow>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:munder>
           <mml:mo mathcolor="gray" form="prefix" movablelimits="false">lim</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">&rarr;</mml:mo>
             <mml:msup>
               <mml:mn>0</mml:mn>
               <mml:mo>+</mml:mo>
             </mml:msup>
           </mml:mrow>
         </mml:munder>
         <mml:mfrac>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mn>1</mml:mn>
         </mml:mfrac>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>(Note
   that
   <mml:math display="inline">
     <mml:mrow>
       <mml:munder>
         <mml:mo mathcolor="gray" form="prefix" movablelimits="false">lim</mml:mo>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">&rarr;</mml:mo>
           <mml:msup>
             <mml:mn>0</mml:mn>
             <mml:mo>+</mml:mo>
           </mml:msup>
         </mml:mrow>
       </mml:munder>
     </mml:mrow>
   </mml:math>
   implies that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>.
   Consequently
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   thereby justifying our dropping the term in the last equation.) Thus the
   desired limit exists so the inverse LT is valid. As the inverse LT must be
   causal, multiply the result by the unit step function. We get
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">ln</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mi>s</mml:mi>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mi>s</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mi>t</mml:mi>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 20.10
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p class="QQQSubHead2">
   <br/>8.
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>-Shifted
   unit step:
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>c</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&harr;</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mi>s</mml:mi>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>c</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math><span style="width:1em"><span style="padding-left:1em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   This property says that the transform of the shifted unit step function is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>c</mml:mi>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>s</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">/</mml:mo>
         <mml:mi>s</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      20.11
      [<mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>-shifted
      unit step I]:
   </p>
</div>
<p class="QQQExampleText">
   Use the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>-shifted
   unit step property to calculate
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(20.5)</mtext></mtd>

       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>s</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mi>s</mml:mi>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:<span style="width:1em"><span style="padding-left:1em"></span></span></span>By
   the
   <span class="QQQHyperLink-BodyText"><a href="#Linearity"> <span class="QQQHyperLink-BodyText">Linearity
   Property</span></a> </span>we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>s</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mi>s</mml:mi>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msup>
             <mml:mi fontstyle="upright">&Lscr;</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mi>s</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi fontstyle="upright">&Lscr;</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>s</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mi>s</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Then apply the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>-shifted
   unit step property to both terms above:
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   in the first term and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>&pi;</mml:mi>
     </mml:mrow>
   </mml:math>
   in the second. Thus we get
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msup>
             <mml:mi fontstyle="upright">&Lscr;</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mi>s</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi fontstyle="upright">&Lscr;</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>s</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mi>s</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>&pi;</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>a pulse of length
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&pi;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>The
   inverse LT must be causal, so multiply the result by the unit step function to
   obtain a pulse of length
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&pi;</mml:mi>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>s</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mi>s</mml:mi>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
