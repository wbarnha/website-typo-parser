                 <mml:mi>&infin;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>n</mml:mi>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>n</mml:mi>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mrow>
                 <mml:mi>n</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
