           <mml:mi>&theta;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
            (with
            <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
