                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mrow>
