         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>10</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>y</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <span style="width:2em"><span style="padding-left:2em"></span></span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">&bull;</mml:mo>
     </mml:mrow>
   </mml:math> <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>10</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   On this edge
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
