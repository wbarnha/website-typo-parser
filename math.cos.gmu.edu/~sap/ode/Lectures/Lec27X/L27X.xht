            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>&theta;</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>&omega;</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:msubsup>
             <mml:mi>&omega;</mml:mi>
