         <img src="Images/L27X__21.png" alt="nullclinesexamplelec27..png" height="358px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Nullclines for
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>y</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:msup>
           <mml:mi>y</mml:mi>
           <mml:mn>3</mml:mn>
