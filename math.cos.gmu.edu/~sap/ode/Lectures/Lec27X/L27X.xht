           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Calculate<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">H</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>H</mml:mi>
                   <mml:mrow>
                     <mml:mi>x</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>H</mml:mi>
                   <mml:mrow>
                     <mml:mi>y</mml:mi>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mi>y</mml:mi>
                   </mml:mrow>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mo form="infix">&midast;</mml:mo>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>H</mml:mi>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mi>x</mml:mi>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>x</mml:mi>
                         <mml:mo form="infix">&midast;</mml:mo>
                       </mml:msub>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:msub>
                         <mml:mi>y</mml:mi>
                         <mml:mo form="infix">&midast;</mml:mo>
                       </mml:msub>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>H</mml:mi>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mi>y</mml:mi>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>x</mml:mi>
                         <mml:mo form="infix">&midast;</mml:mo>
                       </mml:msub>
                       <mml:mo form="infix">,</mml:mo>
                       <mml:msub>
                         <mml:mi>y</mml:mi>
                         <mml:mo form="infix">&midast;</mml:mo>
                       </mml:msub>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="false" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mo form="infix">&midast;</mml:mo>
                   </mml:msub>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtext mathcolor="black">det</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">H</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>4</mml:mn>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mtext mathcolor="black">det&ensp;</mml:mtext>
       </mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mstyle displaystyle="false" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mn>1</mml:mn>
                             <mml:mn>2</mml:mn>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>x</mml:mi>
                     </mml:mrow>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           </mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="infix">&midast;</mml:mo>
                 </mml:msub>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:msub>
                   <mml:mi>y</mml:mi>
                   <mml:mo form="infix">&midast;</mml:mo>
                 </mml:msub>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>4</mml:mn>
                 </mml:mrow>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mtext mathcolor="black">det</mml:mtext>
       </mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="right right">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">></mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>and<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>H</mml:mi>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo>&InvisibleComma;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>4</mml:mn>
             </mml:mrow>
             <mml:mo form="infix">,</mml:mo>
