               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>(a) Initial Data Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>y</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>y</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>Substitute
   the initial data in Eqns. (11.29) and (11.30):
</p>
<p class="QQQCenteredExampleText">
   <mml:math display="inline">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Solve for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>c</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mi>c</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>2</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>So
   the solution for the initial data set (a) is
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>(b) Initial data set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>y</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>y</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
   We leave it to the reader to show that the solution for the initial data set
   (b) is
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
