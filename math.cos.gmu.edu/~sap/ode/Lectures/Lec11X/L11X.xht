                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi>x</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mn>0</mml:mn>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">=</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                   <mml:mtext>,</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mn>0</mml:mn>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Return:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mi>m</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mover accent="true">
                           <mml:mi>x</mml:mi>
                           <mml:mo form="postfix" accent="true">&die;</mml:mo>
                         </mml:mover>
                       </mml:mrow>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:mi>c</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mover accent="true">
                           <mml:mi>x</mml:mi>
                           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
