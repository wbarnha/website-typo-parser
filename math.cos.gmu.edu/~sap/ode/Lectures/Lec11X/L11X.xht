               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">Recoil:</mml:mtext>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mi>m</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
