                         <mml:mn>11</mml:mn>
                       </mml:msqrt>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>So
   the solution to the IVP
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">/</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="false" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>3</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msqrt>
                   <mml:mn>11</mml:mn>
                 </mml:msqrt>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="false" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:msqrt>
                   <mml:mn>11</mml:mn>
                 </mml:msqrt>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">/</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="false" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>3</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msqrt>
                   <mml:mn>11</mml:mn>
                 </mml:msqrt>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>and
   with a plot in Figure 11.9.
</p>
<p class="center">
   
<!-- MSISEMCOM /FRAME{dtbpFU}{5.6187in}{3.576in}{0pt}{}{}{figure11.10.png}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 5.6187in;  height 3.576in;  depth 0pt;  original-width 5.9205in;  original-height 3.7619in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "H";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L11X__9.png" alt="figure11.10.png" height="343px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 11.9:
            :<mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>25</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math></span>
         </td>
      </tr></tbody>
</table></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 11.15
   </p>
</div>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>The plots in Examples 11.14 and 11.15 may be constructed by the reader
   with the interact of Figure 11.10 below. This interact allows you to specify
   any ODE by choosing values for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>p</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>q</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   The corresponding characteristic roots and the discriminant
   <mml:math display="inline">
     <mml:mrow>
       <mml:msubsup>
         <mml:mi>p</mml:mi>
         <mml:mn>0</mml:mn>
         <mml:mn>2</mml:mn>
       </mml:msubsup>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mn>4</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mi>q</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   are displayed as well. Then by selecting the initial values
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>,
   you may visualize the plots of any IVP subject to the range of allowable
   parameters on the slider bars. The black dot on the solution curve represents
   the position of the initial values 
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>t</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
 Some parameter choices may require you to zoom in on the plot. In this case adjust the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>
   parameter (extent of the domain of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>)
   and the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>R</mml:mi>
     </mml:mrow>
   </mml:math>
   parameter (extent of the range of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>).

   Finally observe the value of the discriminant as you vary the parameters
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>p</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>q</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   The discriminant will be negative whenever the imaginary part of the
   characteristic roots is non zero.
</p>
<p>
   <br/>
</p>

<p class = "center">
<iframe src="https://www.wolframcloud.com/objects/fb4d0482-836b-4a25-a40a-d913d183774e?_embed=iframe" width="1100" height="650" />
</p>
<p>

   <br/><hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p class="QQQSubHead1">
   <br/>11.4 PHYSICAL MODELS AND THEIR INTERPRETATION
</p>
<p>
   <br/>
</p>
<p>
   <a id="UnforcedPhysicalSystems"></a>
</p>
<p>
   <br/>The simplest way to understand the nature of solutions to linear
   homogeneous second-order ODEs is via a real system with whose behavior we are
   familiar. Consider the damped mass-spring system below.
</p>
<h5 class="subsubsection"><br/></h5>
<p class="center">
   
<!-- MSISEMCOM /FRAME{dtbpFU}{2.3895in}{1.6449in}{0pt}{}{}{unforcedhorizontalmassspringdamper.wmf}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 2.3895in;  height 1.6449in;  depth 0pt;  original-width 2.0197in;  original-height 1.3819in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "L";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L11X__11.png" alt="unforcedhorizontalmassspringdamper.wmf" height="158px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Unforced Mechanical
            System</span>
         </td>
      </tr></tbody>
</table></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p>
   The ODE
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.15)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>m</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&die;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>c</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>k</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p>
   models the motion of the block. Here
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>m</mml:mi>
     </mml:mrow>
   </mml:math>
   denotes the mass of the block,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is the displacement of the block from its rest position at
   time<span class="sf"> </span><mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
     </mml:mrow>
   </mml:math>
   is the damping coefficient of the shock absorber, and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>k</mml:mi>
     </mml:mrow>
   </mml:math>
   is the spring constant. Divide Eqn. (11.15) by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>m</mml:mi>
     </mml:mrow>
   </mml:math>
   to get
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.16)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mi>c</mml:mi>
                   <mml:mi>m</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mi>k</mml:mi>
                   <mml:mi>m</mml:mi>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p>
   For algebraic convenience (which we shall see shortly),
   set<a id="Eqn.(11.17)"></a>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.17)</mtext></mtd>

       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mi>c</mml:mi>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>m</mml:mi>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:msqrt>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mi>k</mml:mi>
               <mml:mi>m</mml:mi>
             </mml:mfrac>
           </mml:mstyle>
         </mml:msqrt>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p>
   Then Eqn.(11.15) transforms to<a id="Eqn.(11.18)"></a>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.18)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>b</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
               <mml:mn>2</mml:mn>
             </mml:msubsup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p>
   The parameters
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   are always nonnegative as they must be to model a physical system. As
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
     </mml:mrow>
   </mml:math>
   is the coefficient of friction and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>k</mml:mi>
     </mml:mrow>
   </mml:math>
   is the spring constant
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec01X/L01X.xht#Example01.09"> <span class="QQQHyperLink-BodyText">(see
   Example 1.9)</span></a></span>, the parameters
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   measure the following
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" rowlines="solid none none none">
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">Parameter</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext mathcolor="black">Physical Interpretation</mml:mtext>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd columnalign="left">
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mi>b</mml:mi>
           </mml:mtd>
           <mml:mtd columnalign="left">
             <mml:mtext mathcolor="black">damping (i.e., friction) strength</mml:mtext>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
           </mml:mtd>
           <mml:mtd columnalign="left">
             <mml:mtext mathcolor="black">strength of the spring's restoring force</mml:mtext>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <a id="SHM"></a>
</p>
<p class="QQQSubHead2">
   <br/><strong>Simple Harmonic Motion</strong>
   (<mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>When
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   ODE for the motion of the block is given by
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.19)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
               <mml:mn>2</mml:mn>
             </mml:msubsup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p>
   <br/>and its characteristic polynomial is
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:msubsup>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>0</mml:mn>
           <mml:mn>2</mml:mn>
         </mml:msubsup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>It
   factors
   as<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>&lambda;</mml:mi>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mi>&lambda;</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>and
   has complex conjugate characteristic roots with zero real
   parts:<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>&omega;</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:msub>
           <mml:mover accent="true">
             <mml:mi>&lambda;</mml:mi>
             <mml:mo form="postfix" accent="true">&macr;</mml:mo>
           </mml:mover>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>i</mml:mi>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>&omega;</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   According to
   <span class="QQQHyperLink-BodyText"><a href="#Theorem11.11"> <span class="QQQHyperLink-BodyText">Theorem
   11.11</span></a></span>, Case (3) prevails with
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&alpha;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">Re</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mi>&lambda;</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   so that the general solution is<a id="Eqn.(11.20)"></a>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.20)</mtext></mtd>

       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p>
   for arbitrary constants
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>.
</p>
<p>
   The parameter
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   plays the role of an angular frequency in Eqn. (11.20). This frequency with
   which the block oscillates depends on the spring constant
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>k</mml:mi>
     </mml:mrow>
   </mml:math>.
   In fact, according to
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(11.18)"> <span class="QQQHyperLink-BodyText">Eqn.
   (11.18)</span></a></span>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:msqrt>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mi>k</mml:mi>
             <mml:mi>m</mml:mi>
           </mml:mfrac>
         </mml:mstyle>
       </mml:msqrt>
     </mml:mrow>
   </mml:math>
</p>
<p>
   No matter what initial conditions are imposed on the ODE
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
               <mml:mn>2</mml:mn>
             </mml:msubsup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the block ALWAYS oscillates with angular frequency
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It is for this reason that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is called the <strong>natural frequency</strong> of the ODE. Moreover
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is related to the period
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>
   through the relation
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>T</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&pi;</mml:mi>
             </mml:mrow>
             <mml:msub>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   The motion of the block (and any physical system modeled by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mi>&omega;</mml:mi>
             <mml:mn>0</mml:mn>
             <mml:mn>2</mml:mn>
           </mml:msubsup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>)
   is called <strong>simple harmonic motion</strong>.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      11.16 [<strong>Complex Conjugate Characteristic Roots - Real part
      Zero</strong>]
   </p>
</div>
<p class="QQQExampleText">
   Solve the
   IVP<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>y</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>4</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>y</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>y</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>b</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>for
   arbitrary parameters
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Then plot the three solutions that correspond to the following sets of values
   for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   on the interval
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>&pi;</mml:mi>
     </mml:mrow>
   </mml:math>.
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" rowlines="solid none none none">
         <mml:mtr>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mi>a</mml:mi>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mi>b</mml:mi>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>0.5</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>0.5</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>0.5</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>0.5</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>0.5</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>Note
   that the dependent variable is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   the independent variable is still
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:<span style="width:1em"><span style="padding-left:1em"></span></span></span>The
   characteristic equation is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msup>
             <mml:mi>&lambda;</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:mn>4</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   it factors
   as<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>&lambda;</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>i</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>&lambda;</mml:mi>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>i</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>to
   get the complex conjugate roots (with real part
   zero)<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>i</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>i</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math><span class="QQQHyperLink-BodyText"><a href="#Theorem11.11"> <span class="QQQHyperLink-BodyText">Theorem
   11.11</span></a></span>, Case (3) shows that a general solution is given
   by<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.21)</mtext></mtd>

       <mml:mrow>
         <mml:mi>y</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>We
   calculate values of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   to satisfy the initial conditions
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>y</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>y</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>b</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Differentiate Eqn. (11.21) to
   obtain<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(11.22)</mtext></mtd>

       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>y</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>Eqn.
   (11.21) and (11.22) imply
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" columnalign="left left left">
         <mml:mtr>
           <mml:mtd>
             <mml:mi>a</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo form="infix">&sdot;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="infix">&rArr;</mml:mo>
           </mml:mtd>
           <mml:mtd>
             <mml:mi>a</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mi>b</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mn>0</mml:mn>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo form="infix">&sdot;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="infix">&rArr;</mml:mo>
           </mml:mtd>
           <mml:mtd>
             <mml:mi>b</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mi>c</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>Hence
   the solution to the IVP
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>b</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>The
   calculations of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   for each of the three sets of initial conditions are left to the reader. Note
   that all three initial conditions have the same value for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>y</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   they just differ in the values for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>y</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   We
   get<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtable align="axis" rowlines="solid none none none">
         <mml:mtr>
           <mml:mtd>
             <mml:mi>a</mml:mi>
           </mml:mtd>
           <mml:mtd>
             <mml:mi>b</mml:mi>
           </mml:mtd>
           <mml:mtd>
             <mml:mi>y</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mn>0.5</mml:mn>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mn>0.5</mml:mn>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd columnalign="left">
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>0.5</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>0.25</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mn>0.5</mml:mn>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mn>0</mml:mn>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd columnalign="left">
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mn>0.5</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mn>0.5</mml:mn>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>0.5</mml:mn>
             </mml:mrow>
             <mml:mtext>&emsp;</mml:mtext>
           </mml:mtd>
           <mml:mtd columnalign="left">
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>0.5</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>0.25</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>Also
   observe that all solutions as well as the general solution are
   <span class="QQQNote-BodyText"> <a target="_blank" href="L11X__note9.xht"><span class="QQQNote-BodyText">periodic</span></a> </span>
   with period
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>T</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="center">
   
