             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mi>s</mml:mi>
