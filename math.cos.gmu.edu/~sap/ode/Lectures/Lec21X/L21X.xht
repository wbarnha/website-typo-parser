                 <mml:mi>X</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>6</mml:mn>
                         <mml:mrow>
                           <mml:mi>s</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mspace width="thickmathspace" />
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:msup>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>s</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mn>1</mml:mn>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                         <mml:mrow>
                           <mml:mi>s</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>s</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mn>2</mml:mn>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mrow>
                       <mml:mn>6</mml:mn>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>s</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mi>s</mml:mi>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>s</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>6</mml:mn>
                       <mml:mrow>
                         <mml:mi>s</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mn>6</mml:mn>
                       <mml:mrow>
                         <mml:mi>s</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mn>2</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Now
      we invert each of the preceding terms. The first term is
      easy:<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>6</mml:mn>
                 <mml:mrow>
                   <mml:mi>s</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>6</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>The
   second term requires the
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec20X/L20X.xht#Table20.1"> <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math><span class="QQQHyperLink-BodyText">-integration</span></a></span>
   property. Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>Q</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>6</mml:mn>
           <mml:mo form="infix">/</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:mrow>
               <mml:mi>s</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
      Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>q</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mi>Q</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>s</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>6</mml:mn>
                 <mml:mrow>
                   <mml:mi>s</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>6</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>so<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>6</mml:mn>
                           <mml:mrow>
                             <mml:mi>s</mml:mi>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>s</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mn>2</mml:mn>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:mn>0</mml:mn>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mspace width="thickmathspace" />
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi>q</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mi>&tau;</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                   <mml:mn>0</mml:mn>
                   <mml:mi>t</mml:mi>
                 </mml:msubsup>
                 <mml:mspace width="thickmathspace" />
                 <mml:mrow>
                   <mml:mn>6</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:msubsup>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>6</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&tau;</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
                 </mml:mrow>
                 <mml:mn>0</mml:mn>
                 <mml:mi>t</mml:mi>
               </mml:msubsup>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mn>3</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi>u</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Putting
      it all together yields the
      solution<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac>
                             <mml:mn>6</mml:mn>
                             <mml:mrow>
                               <mml:mi>s</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mn>2</mml:mn>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                       <mml:mrow>
