                   </mml:mstyle>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Then
   upon taking inverse LTs we
   get<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mi>X</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&Phi;</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                         <mml:mi>s</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>y</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&Phi;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msub>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                         <mml:mi>s</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Lscr;</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mi>&Phi;</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                         <mml:mi>s</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>In
   view of the zero-state solution to Eqn.
   <mml:math display="inline">
   </mml:math>(21.4) for causal inputs
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(21.5)</mtext></mtd>

       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>&phi;</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>&phi;</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mo form="prefix" largeop="true">&int;</mml:mo>
               <mml:mn>0</mml:mn>
               <mml:mi>t</mml:mi>
             </mml:msubsup>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>g</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mspace width="thinmathspace" />
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi>f</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&tau;</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>we
   must have by the uniqueness of Laplace transforms
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>&phi;</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:maligngroup />
                 <mml:mo form="infix">&harr;</mml:mo>
                 <mml:maligngroup />
                 <mml:msub>
                   <mml:mi>&Phi;</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>&phi;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:maligngroup />
                 <mml:mo form="infix">&harr;</mml:mo>
                 <mml:maligngroup />
                 <mml:msub>
                   <mml:mi>&Phi;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>g</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&starf;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>f</mml:mi>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">&harr;</mml:mo>
                 <mml:maligngroup />
                 <mml:mi>&Phi;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Thus,<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&starf;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>f</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi>F</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>s</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>p</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:msub>
                 <mml:mi>q</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>If
   we
   set<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>G</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>s</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mn>1</mml:mn>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>s</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>p</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:msub>
                 <mml:mi>q</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>then
   we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>g</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>&starf;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>f</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&harr;</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>F</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>which
   is precisely what the
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec20X/L20X.xht#Table20.1"> <span class="QQQHyperLink-BodyText">convolution
   property of Table 20.1</span></a></span> tells us. In particular, when
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
      the solution to
      <span class="QQQHyperLink-BodyText"><a href="#Eqn.(21.04)"> <span class="QQQHyperLink-BodyText">Eqn.
   (21.4)</span></a></span><span class="QQQHyperLink-BodyText"> </span> is given by the inverse Laplace
      transform<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>F</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   </p>
   <p>
      <br/><a id="TransferFunction"></a>
   </p>
   <p>
      <hr class="hrulefill" />
   </p>
   <div class="definition">
      <span class="leadin">Definition</span><p class="definition-FirstPara">
         <mml:math display="inline">
   </mml:math>21.10 [Transfer Function]:
      </p>
   </div>
   <p class="QQQSpecialText">
      <br/>The function
      <mml:math display="inline">
        <mml:mrow>
          <mml:mi>G</mml:mi>
        </mml:mrow>
      </mml:math>
      defined
      by<mml:math mode="display" display="block">
        <mml:mrow>
          <mml:mrow>
            <mml:mi>G</mml:mi>
            <mml:mo>&ApplyFunction;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mi>s</mml:mi>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">=</mml:mo>
          <mml:mrow>
            <mml:mstyle displaystyle="true" scriptlevel="0">
              <mml:mfrac>
                <mml:mn>1</mml:mn>
                <mml:mrow>
                  <mml:msup>
                    <mml:mi>s</mml:mi>
                    <mml:mn>2</mml:mn>
                  </mml:msup>
                  <mml:mo form="infix">+</mml:mo>
                  <mml:mrow>
                    <mml:msub>
                      <mml:mi>p</mml:mi>
                      <mml:mn>0</mml:mn>
                    </mml:msub>
                    <mml:mo>&InvisibleTimes;</mml:mo>
                    <mml:mi>s</mml:mi>
                  </mml:mrow>
                  <mml:mo form="infix">+</mml:mo>
                  <mml:msub>
                    <mml:mi>q</mml:mi>
                    <mml:mn>0</mml:mn>
                  </mml:msub>
                </mml:mrow>
              </mml:mfrac>
            </mml:mstyle>
          </mml:mrow>
        </mml:mrow>
      </mml:math><br/>is
      called the transfer function of the Eqn. (21.2). Observe that the expression
      <mml:math display="inline">
        <mml:mrow>
          <mml:msup>
            <mml:mi>s</mml:mi>
            <mml:mn>2</mml:mn>
          </mml:msup>
          <mml:mo form="infix">+</mml:mo>
          <mml:mrow>
            <mml:msub>
              <mml:mi>p</mml:mi>
              <mml:mn>0</mml:mn>
            </mml:msub>
            <mml:mo>&InvisibleTimes;</mml:mo>
            <mml:mi>s</mml:mi>
          </mml:mrow>
          <mml:mo form="infix">+</mml:mo>
          <mml:msub>
            <mml:mi>q</mml:mi>
            <mml:mn>0</mml:mn>
          </mml:msub>
        </mml:mrow>
      </mml:math>
      is precisely the characteristic polynomial of the associated homogeneous ODE.
      Thus the impulse response
      <mml:math display="inline">
        <mml:mrow>
          <mml:mi>g</mml:mi>
          <mml:mo>&ApplyFunction;</mml:mo>
          <mml:mrow>
            <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
            <mml:mi>t</mml:mi>
            <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
          </mml:mrow>
        </mml:mrow>
      </mml:math>
      is the inverse LT of the transfer function, i.e.,
      <mml:math mode="display" display="block">
        <mml:mrow>
          <mml:mrow>
            <mml:mi>g</mml:mi>
            <mml:mo>&ApplyFunction;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mi>t</mml:mi>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">=</mml:mo>
          <mml:mrow>
            <mml:msup>
              <mml:mi fontstyle="upright">&Lscr;</mml:mi>
              <mml:mrow>
                <mml:mo form="prefix">&minus;</mml:mo>
                <mml:mn>1</mml:mn>
              </mml:mrow>
            </mml:msup>
            <mml:mo>&InvisibleTimes;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
              <mml:mrow>
                <mml:mstyle displaystyle="true" scriptlevel="0">
                  <mml:mfrac>
                    <mml:mn>1</mml:mn>
                    <mml:mrow>
                      <mml:msup>
                        <mml:mi>s</mml:mi>
                        <mml:mn>2</mml:mn>
                      </mml:msup>
                      <mml:mo form="infix">+</mml:mo>
                      <mml:mrow>
                        <mml:msub>
                          <mml:mi>p</mml:mi>
                          <mml:mn>0</mml:mn>
                        </mml:msub>
                        <mml:mo>&InvisibleTimes;</mml:mo>
                        <mml:mi>s</mml:mi>
                      </mml:mrow>
                      <mml:mo form="infix">+</mml:mo>
                      <mml:msub>
                        <mml:mi>q</mml:mi>
                        <mml:mn>0</mml:mn>
                      </mml:msub>
                    </mml:mrow>
                  </mml:mfrac>
                </mml:mstyle>
              </mml:mrow>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
            </mml:mrow>
          </mml:mrow>
        </mml:mrow>
      </mml:math>Moreover,<mml:math mode="display" display="block">
        <mml:mrow>
          <mml:mrow>
            <mml:mover accent="true">
              <mml:mi>g</mml:mi>
              <mml:mo form="postfix" accent="true">&Hat;</mml:mo>
            </mml:mover>
            <mml:mo>&ApplyFunction;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mi>t</mml:mi>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">=</mml:mo>
          <mml:mrow>
            <mml:mrow>
              <mml:mi>g</mml:mi>
              <mml:mo>&ApplyFunction;</mml:mo>
              <mml:mrow>
                <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                <mml:mi>t</mml:mi>
                <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
              </mml:mrow>
            </mml:mrow>
            <mml:mo>&InvisibleTimes;</mml:mo>
            <mml:mrow>
              <mml:mi>u</mml:mi>
              <mml:mo>&ApplyFunction;</mml:mo>
              <mml:mrow>
                <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                <mml:mi>t</mml:mi>
                <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
              </mml:mrow>
            </mml:mrow>
          </mml:mrow>
        </mml:mrow>
      </mml:math>where
      <mml:math display="inline">
        <mml:mrow>
          <mml:mi>g</mml:mi>
          <mml:mo>&ApplyFunction;</mml:mo>
          <mml:mrow>
            <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
            <mml:mi>t</mml:mi>
            <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
          </mml:mrow>
        </mml:mrow>
      </mml:math>
      is the Green's function.
   </p>
   <p class="center">
      <hr class="hrulefill" />
   </p>
   <p>
      <br/>
   </p>
   <p>
      Also note
      that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>&Phi;</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mrow>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:msub>
                   <mml:mi>p</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mrow>
                 <mml:msup>
                   <mml:mi>s</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>p</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>s</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:msub>
                   <mml:mi>q</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&emsp;and&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>&Phi;</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:msup>
                   <mml:mi>s</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>p</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>s</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:msub>
                   <mml:mi>q</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   </p>
   <p>
      must be the Laplace transforms of the ideal basis for
      <mml:math display="inline">
     <mml:mrow>
       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>p</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>q</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0.</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   </p>
   <p>
      <br/>
   </p>
   <p>
      <br/><hr class="hrulefill" />
   </p>
   <p>
      <br/>
   </p>
   <p class="QQQSubHead1">
      <br/><mml:math display="inline">
   </mml:math>21.6 INTEGRAL EQUATIONS
   </p>
   <p class="QQQExampleText">
      Some linear equations other than ODEs can be solved with the aid of Laplace
      transforms. We illustrate the usefulness of Laplace transforms in the next few
      examples. The first example is a linear integral equation.
   </p>
   <p>
      <br/>
   </p>
   <p>
      <hr class="hrulefill" />
   </p>
   <div class="example">
      <span class="leadin">Example</span><p class="example-FirstPara">
         <mml:math display="inline">
   </mml:math>21.11<a id="Example21.11"></a>
      </p>
   </div>
   <p class="QQQExampleText">
      Use Laplace transforms and the convolution theorem to solve the following
      integral equation for
      <mml:math display="inline">
        <mml:mrow>
          <mml:mrow>
            <mml:mi>x</mml:mi>
            <mml:mo>&ApplyFunction;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mi>t</mml:mi>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">:</mml:mo>
        </mml:mrow>
      </mml:math><mml:math mode="display" display="block">
        <mml:mrow>
          <mml:mrow>
            <mml:msubsup>
              <mml:mo form="prefix" largeop="true">&int;</mml:mo>
              <mml:mrow>
                <mml:mspace width="negativethinmathspace" />
                <mml:mn>0</mml:mn>
              </mml:mrow>
              <mml:mi>t</mml:mi>
            </mml:msubsup>
            <mml:mspace width="thinmathspace" />
            <mml:mrow>
              <mml:mrow>
                <mml:mi>x</mml:mi>
                <mml:mo>&ApplyFunction;</mml:mo>
                <mml:mrow>
                  <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                  <mml:mrow>
                    <mml:mi>t</mml:mi>
                    <mml:mo form="infix">&minus;</mml:mo>
                    <mml:mi>&tau;</mml:mi>
                  </mml:mrow>
                  <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                </mml:mrow>
              </mml:mrow>
              <mml:mo>&InvisibleTimes;</mml:mo>
              <mml:msup>
                <mml:mi>e</mml:mi>
                <mml:mrow>
                  <mml:mo form="prefix">&minus;</mml:mo>
                  <mml:mi>&tau;</mml:mi>
                </mml:mrow>
              </mml:msup>
              <mml:mtext>&ZeroWidthSpace;</mml:mtext>
              <mml:mo>&InvisibleTimes;</mml:mo>
              <mml:mi>d</mml:mi>
              <mml:mtext>&ZeroWidthSpace;</mml:mtext>
              <mml:mo>&InvisibleTimes;</mml:mo>
              <mml:mi>&tau;</mml:mi>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">=</mml:mo>
          <mml:mrow>
            <mml:mi>t</mml:mi>
            <mml:mo>&InvisibleTimes;</mml:mo>
            <mml:msup>
              <mml:mi>e</mml:mi>
              <mml:mrow>
                <mml:mo form="prefix">&minus;</mml:mo>
                <mml:mi>t</mml:mi>
              </mml:mrow>
            </mml:msup>
          </mml:mrow>
        </mml:mrow>
      </mml:math>
   </p>
   <p class="QQQExampleText">
      <br/><span class="QQQSansSerif14">Solution:<span style="width:1em"><span style="padding-left:1em"></span></span></span>Observe
      that the integral is a convolution of
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
      and
      <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
      Take the Laplace transform of both sides of the
      equation:<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi fontstyle="upright">&Lscr;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mo form="prefix" largeop="true">&int;</mml:mo>
               <mml:mn>0</mml:mn>
               <mml:mi>t</mml:mi>
             </mml:msubsup>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
               </mml:msup>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&tau;</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi fontstyle="upright">&Lscr;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>to
      get<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi fontstyle="upright">&Lscr;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&starf;</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mfrac>
         <mml:mn>1</mml:mn>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>s</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mfrac>
     </mml:mrow>
   </mml:math>Let
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>X</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi fontstyle="upright">&Lscr;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>.
      Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>X</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mfrac>
           <mml:mn>1</mml:mn>
           <mml:mrow>
             <mml:mi>s</mml:mi>
             <mml:mo form="infix">+</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:mfrac>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mfrac>
         <mml:mn>1</mml:mn>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>s</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mfrac>
     </mml:mrow>
   </mml:math>Solve
      for
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>X</mml:mi>
     </mml:mrow>
   </mml:math>
      to
      obtain<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>X</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mfrac>
         <mml:mn>1</mml:mn>
         <mml:mrow>
           <mml:mi>s</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mfrac>
     </mml:mrow>
   </mml:math>It
      follows that
      <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mi>X</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mfrac>
             <mml:mn>1</mml:mn>
             <mml:mrow>
               <mml:mi>s</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:mfrac>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   </p>
   <div class="conclusion">
      <span class="leadin">End</span><p class="conclusion-FirstPara">
         of Example <mml:math display="inline">
   </mml:math>21.11
      </p>
   </div>
   <p class="center">
      <hr class="hrulefill" />
   </p>
   <p>
      <br/><a id="VolterraIntegralEquation"></a>
   </p>
   <p class="QQQSubHead2">
      <br/>Volterra Integral Equations
   </p>
   <p>
      An important class of integral equations is <strong>Volterra's integral
      equation, </strong>which has
      the<strong> </strong>form<mml:math mode="display" display="block">
    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(21.6)</mtext></mtd>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msubsup>
               <mml:mo form="prefix" largeop="true">&int;</mml:mo>
               <mml:mn>0</mml:mn>
               <mml:mi>t</mml:mi>
             </mml:msubsup>
             <mml:mspace width="thickmathspace" />
             <mml:mrow>
               <mml:mi>k</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>d</mml:mi>
               <mml:mtext>&ZeroWidthSpace;</mml:mtext>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&tau;</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
  	  </mlabeledtr>
	</mtable>
   </mml:math>where
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
      and
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>k</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
      are known functions with Laplace transforms
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>F</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi fontstyle="upright">&Lscr;</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mi>f</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
      and
      <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>K</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
             <mml:mi>k</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
      We recognize that the integral is a convolution so that Eqn.
      (<mml:math display="inline">
   </mml:math>21.4) can be rewritten
   as<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(21.7)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>k</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&starf;</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>Apply
      the Laplace transform operator
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Lscr;</mml:mi>
     </mml:mrow>
   </mml:math>
      to Eqn. (<mml:math display="inline">
   </mml:math>21.5) to
      obtain<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>X</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>s</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>F</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>s</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>K</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>s</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>X</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>s</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>where
      <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>X</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
      We can readily solve for
      <mml:math display="inline">
     <mml:mrow>
       <mml:mi>X</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>s</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
      to
      get<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>X</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>s</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi>F</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>s</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi>K</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>s</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Finally,
      the solution to Volterra's integral equation can be represented
      as<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi fontstyle="upright">&Lscr;</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mi>F</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>s</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mi>K</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mi>s</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   </p>
   <p>
      <br/>
   </p>
   <p>
      <hr class="hrulefill" />
   </p>
   <div class="example">
      <span class="leadin">Example</span><p class="example-FirstPara">
         <mml:math display="inline">
   </mml:math>21.12<a id="Example21.12"></a>
      </p>
   </div>
   <p class="QQQExampleText">
      Use Laplace transforms and the convolution theorem to solve the following
      Volterra integral equation for
      <mml:math display="inline">
        <mml:mrow>
          <mml:mrow>
            <mml:mi>x</mml:mi>
            <mml:mo>&ApplyFunction;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mi>t</mml:mi>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">:</mml:mo>
        </mml:mrow>
      </mml:math><mml:math mode="display" display="block">
        <mml:mrow>
          <mml:mrow>
            <mml:mi>x</mml:mi>
            <mml:mo>&ApplyFunction;</mml:mo>
            <mml:mrow>
              <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
              <mml:mi>t</mml:mi>
              <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
            </mml:mrow>
          </mml:mrow>
          <mml:mo form="infix">=</mml:mo>
          <mml:mrow>
            <mml:mrow>
              <mml:mn>6</mml:mn>
              <mml:mo>&InvisibleTimes;</mml:mo>
              <mml:msup>
                <mml:mi>e</mml:mi>
                <mml:mi>t</mml:mi>
              </mml:msup>
            </mml:mrow>
            <mml:mo form="infix">+</mml:mo>
            <mml:mrow>
