                     <mml:mi>&pi;</mml:mi>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>s</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">+</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>&pi;</mml:mi>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>s</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
           </mml:mfrac>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Next recognize that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">/</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   can be expressed as the product
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mspace width="thickmathspace" />
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mfrac>
         <mml:mn>1</mml:mn>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mfrac>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   The term
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">/</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is the sum of the geometric series
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msubsup>
           <mml:mi>&Sigma;</mml:mi>
           <mml:mrow>
             <mml:mi>k</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mi>&infin;</mml:mi>
         </mml:msubsup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mi>k</mml:mi>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&pi;</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mi>k</mml:mi>
         </mml:msup>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   so that we have
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mfrac>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>s</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mfrac>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&pi;</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:munderover>
             <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
             <mml:mrow>
               <mml:mi>k</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mi>&infin;</mml:mi>
           </mml:munderover>
           <mml:mrow>
             <mml:msup>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mi>k</mml:mi>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>&pi;</mml:mi>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>s</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mi>k</mml:mi>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&pi;</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:munderover>
             <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
             <mml:mrow>
               <mml:mi>k</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mi>&infin;</mml:mi>
           </mml:munderover>
           <mml:mrow>
             <mml:msup>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mi>k</mml:mi>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>k</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>&pi;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   Multiply the infinite series termwise by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>&pi;</mml:mi>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>s</mml:mi>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   to get
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>&pi;</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>s</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>&pi;</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>s</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mfrac>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:munderover>
                       <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
                       <mml:mrow>
                         <mml:mi>k</mml:mi>
                         <mml:mo form="infix">=</mml:mo>
                         <mml:mn>0</mml:mn>
                       </mml:mrow>
                       <mml:mi>&infin;</mml:mi>
                     </mml:munderover>
                     <mml:mrow>
                       <mml:msup>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mi>k</mml:mi>
                       </mml:msup>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>k</mml:mi>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&pi;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>s</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>&pi;</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>s</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:munderover>
                         <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
                         <mml:mrow>
                           <mml:mi>k</mml:mi>
                           <mml:mo form="infix">=</mml:mo>
                           <mml:mn>0</mml:mn>
                         </mml:mrow>
                         <mml:mi>&infin;</mml:mi>
                       </mml:munderover>
                       <mml:mrow>
                         <mml:msup>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mn>1</mml:mn>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mi>k</mml:mi>
                         </mml:msup>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>k</mml:mi>
                             </mml:mrow>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>&pi;</mml:mi>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>s</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:munderover>
                     <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
                     <mml:mrow>
                       <mml:mi>k</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mi>&infin;</mml:mi>
                   </mml:munderover>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mi>k</mml:mi>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>k</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&pi;</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>s</mml:mi>
                       </mml:mrow>
                     </mml:msup>
