     </mml:mrow>
   </mml:math>-shift:<span class="QQQHyperLink-BodyText"> </span><span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec20X/L20X.xht#Table20.1"> <span class="QQQHyperLink-BodyText">Property
   9 of Table 20.1</span></a></span>. First
   write<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>s</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>s</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>s</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">&sdot;</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>s</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>s</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:munderover>
             <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
             <mml:mrow>
               <mml:mi>n</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mi>&infin;</mml:mi>
           </mml:munderover>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>s</mml:mi>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mi>n</mml:mi>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>s</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:munderover>
             <mml:mo form="prefix" movablelimits="true" largeop="true">&sum;</mml:mo>
             <mml:mrow>
               <mml:mi>n</mml:mi>
               <mml:mo form="infix">=</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
             <mml:mi>&infin;</mml:mi>
           </mml:munderover>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>n</mml:mi>
