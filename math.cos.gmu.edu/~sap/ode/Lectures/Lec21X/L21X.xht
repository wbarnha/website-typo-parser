                                 <mml:mrow>
                                   <mml:msup>
                                     <mml:mi>s</mml:mi>
                                     <mml:mn>2</mml:mn>
                                   </mml:msup>
                                   <mml:mo form="infix">+</mml:mo>
                                   <mml:mn>1</mml:mn>
                                 </mml:mrow>
                                 <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                               </mml:mrow>
                             </mml:mrow>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mspace width="thinmathspace" />
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mfrac>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:msup>
                               <mml:mi>e</mml:mi>
                               <mml:mrow>
                                 <mml:mrow>
                                   <mml:mo form="prefix">&minus;</mml:mo>
                                   <mml:mi>&pi;</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mi>s</mml:mi>
                               </mml:mrow>
                             </mml:msup>
                           </mml:mrow>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:msup>
                               <mml:mi>e</mml:mi>
                               <mml:mrow>
                                 <mml:mrow>
