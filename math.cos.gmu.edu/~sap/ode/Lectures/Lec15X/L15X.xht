             </mml:mrow>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mtext mathcolor="black">(simplification)</mml:mtext>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="infix">=</mml:mo>
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
