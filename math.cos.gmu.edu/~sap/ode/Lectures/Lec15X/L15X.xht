           <mml:mtd>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Dscr;</mml:mi>
                     <mml:mn>3</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mn>3</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi fontstyle="upright">&Dscr;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>3</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi fontstyle="upright">&Dscr;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="infix">=</mml:mo>
           </mml:mtd>
           <mml:mtd>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">cos</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>3</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>3</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">cos</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="infix">=</mml:mo>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>The exponential shift rule eliminated the need for a tedious calculation
   involving the third derivative of the product
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 15.6
   </p>
</div>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/><a id="Complexification"></a>
</p>
<p class="QQQSubHead1">
   15.2 COMPLEXIFICATION
</p>
<p>
   <br/>
</p>
<p>
   <br/>Before proceeding with the examples in this lecture the reader may need
   to study or review <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Appendix/Lec00B/L00B.xht"> <span class="QQQHyperLink-BodyText">Appendix B: Complex Numbers
   &amp; Functions.</span></a></span>


 The use of complex numbers and functions in operator
   methods reduces the amount of calculation required the UC method and provides
   for a more general approach for a particular solution.
</p>
<p>
   <br/>
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="definition">
   <span class="leadin">Definition</span><p class="definition-FirstPara">
      15.7 [Complexification]
   </p>
</div>
<p class="QQQSpecialText">
   <br/>Given a real valued function
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>u</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   a complexification of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>u</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is a complex function
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>w</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   so that either
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p class="QQQSpecialText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo mathcolor="gray" form="prefix">Re</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mi>w</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">or</mml:mtext>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo mathcolor="gray" form="prefix">Im</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:mi>w</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQSpecialText">
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>For instance, a complexification of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   may be taken to be
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>w</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Here we have taken
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   to be
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo mathcolor="gray" form="prefix">Im</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mi>w</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Another complexification is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>w</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Here we have taken
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   to be
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo mathcolor="gray" form="prefix">Re</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mi>w</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It is clear that the complexification of a real valued function is not unique.
   Any complex valued function
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>w</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   can serve to complexify
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   as long as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   is the real or imaginary part of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>w</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   As demonstrated in the next example, the context of the real valued function
   may suggest an appropriate complexification to
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Appendix/Lec00B/L00B.xht#FullExponential"> <span class="QQQHyperLink-BodyText">full
   exponential form.</span></a></span>
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      15.8:
   </p>
</div>
<p class="QQQExampleText">
   <br/>Complexify
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>u</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>According
   to Euler's formula, we may consider
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   to be the imaginary part of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It follows that one possible complexification
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>w</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mi>i</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>i</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>i</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Thus<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo mathcolor="gray" form="prefix">Im</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mi>i</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
