                   </mml:msubsup>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mi>i</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mn>2</mml:mn>
