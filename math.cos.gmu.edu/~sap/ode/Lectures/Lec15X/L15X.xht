                     </mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:msubsup>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>0</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:msubsup>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:msup>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mi>i</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>b</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&omega;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:msubsup>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>0</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:msubsup>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:msup>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mi>i</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>b</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&omega;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:msubsup>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>0</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:msubsup>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:msup>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msup>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mi>i</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>b</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&omega;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>f</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">cos</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>b</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mi mathcolor="gray">sin</mml:mi>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mrow>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>b</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>or<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mrow>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>b</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">cos</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>b</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>&omega;</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi mathcolor="gray">sin</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Next we express
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   on
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec11X/L11X.xht#PhaseAmplitudeForm"> <span class="QQQHyperLink-BodyText">phase-amplitude
   form</span></a></span>. If we set
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>0</mml:mn>
           <mml:mn>2</mml:mn>
         </mml:msubsup>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:msup>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>c</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>b</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&omega;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   so that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:msqrt>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>4</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>b</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
         </mml:msqrt>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we get
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mrow>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>b</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msqrt>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>4</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>b</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>&omega;</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
         </mml:msqrt>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>&delta;</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:msqrt>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>b</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:msqrt>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>&delta;</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>where
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>&delta;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">arctan</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">arctan</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>b</mml:mi>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&omega;</mml:mi>
                 </mml:mrow>
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>0</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:msubsup>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Application 15.18
   </p>
</div>
<p class="QQQExampleText">
   <hr class="hrulefill" />
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQSubHead2">
   <br/>Resonance<a id="Eqn.(15.29)"></a>
</p>
<p>
   <br/>The next application tells us what input frequency
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&omega;</mml:mi>
     </mml:mrow>
   </mml:math>
   maximizes the amplitude of the particular solution
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
     </mml:mrow>
   </mml:math>.
   Given that damping precludes pure resonance as illustrated in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec14X/L14X.xht#Eqn.(14.15)"> <span class="QQQHyperLink-BodyText">Eqn.
   (14.15)</span></a></span> of
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec14X/L14X.xht#Application14.17"> <span class="QQQHyperLink-BodyText">Application
   14.17</span></a></span>, we expect that the introduction of damping should
   ameliorate the amplitude.
</p>
<p>
   The amplitude of the particular solution depends on the input frequency
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&omega;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Letting
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   denote this dependence, we have from
   <span class="QQQHyperLink-BodyText"><a href="#Application15.18"> <span class="QQQHyperLink-BodyText">Eqn.
   (15.28)</span></a></span> that
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(15.29)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>&omega;</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:msqrt>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>b</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:msqrt>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p>
   An application of max-min theory from calculus
   <span class="QQQNote-BodyText"> <a target="_blank" href="L15X__note1.xht"><span class="QQQNote-BodyText">shows</span></a> </span>
   that if
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mo form="infix">&ge;</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">/</mml:mo>
         <mml:msqrt>
           <mml:mn>2</mml:mn>
         </mml:msqrt>
       </mml:mrow>
     </mml:mrow>
   </mml:math>,
   then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is maximized at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&omega;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>&omega;</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo form="infix">/</mml:mo>
           <mml:msqrt>
             <mml:mn>2</mml:mn>
           </mml:msqrt>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is maximized at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&omega;</mml:mi>
           <mml:mtext mathcolor="black">max</mml:mtext>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:msqrt>
           <mml:msubsup>
             <mml:mi>&omega;</mml:mi>
             <mml:mn>0</mml:mn>
             <mml:mn>2</mml:mn>
           </mml:msubsup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>b</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
         </mml:msqrt>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   The maximum amplitude is
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>A</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:msub>
             <mml:mi>&omega;</mml:mi>
             <mml:mtext mathcolor="black">max</mml:mtext>
           </mml:msub>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:msub>
               <mml:mi>f</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>b</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msqrt>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>b</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:msqrt>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   The frequency
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mtext mathcolor="black">max</mml:mtext>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is called the <strong>resonance frequency</strong> of the system; it is the
   input frequency that maximizes the amplitude of the steady-state response of
   the solution. When the system is driven at this frequency, we say that the
   system is <strong>at resonance</strong>. Note that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>&omega;</mml:mi>
           <mml:mtext mathcolor="black">max</mml:mtext>
         </mml:msub>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:msqrt>
           <mml:msubsup>
             <mml:mi>&omega;</mml:mi>
             <mml:mn>0</mml:mn>
             <mml:mn>2</mml:mn>
           </mml:msubsup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>b</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:msqrt>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec11X/L11X.xht#QuasiFrequency"> <span class="QQQHyperLink-BodyText">quasifrequency
   for unforced underdamped motion</span></a>.</span>
</p>
<p>
   <br/>Figure 15.1 illustrates the graphs of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   for different values of the damping parameter
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   We have chosen
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>f</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   for convenience. As
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   decreases, the peaks of the graphs increase and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mtext mathcolor="black">max</mml:mtext>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   approaches the value of the natural frequency,
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>&omega;</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
   Observe that as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   increases beyond some threshold,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
       <mml:mo form="infix">/</mml:mo>
       <mml:msqrt>
         <mml:mn>2</mml:mn>
       </mml:msqrt>
     </mml:mrow>
   </mml:math>,
   resonance fails to occur. Intuition tells us that the damping is too strong to
   allow for any peaks in the amplitude. This is consistent with what we have
   just explained. When
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(15.29)"> <span class="QQQHyperLink-BodyText">Eqn.
   (15.19)</span></a></span> shows us that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>A</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>&omega;</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>0</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:msubsup>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>, so
   that pure resonance occurs when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:msub>
           <mml:mi>&omega;</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math><a id="FrequencyResponse"></a>
</p>
<p>
   <br/>When
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>,
   the graph of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>A</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>&omega;</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is called the <strong>frequency response curve</strong> or <strong>resonance
   curve</strong> for the ODE. The expression defined by<a id="Gain"></a>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(15.30)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mrow>
                 <mml:mi>A</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>&omega;</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:msqrt>
                 <mml:msup>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:msubsup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:msubsup>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>b</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&omega;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:msqrt>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>is
   called the <strong>gain</strong> of the system at frequency
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&omega;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="center">
   
