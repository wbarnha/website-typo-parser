                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">sin</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>i</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">cos</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">sin</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>&emsp;</mml:mtext>
             <mml:mtext mathcolor="black">(real&ensp;+</mml:mtext>
             <mml:mi>i</mml:mi>
             <mml:mtext mathcolor="black">imaginary)</mml:mtext>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mtext mathcolor="black">Step&ensp;3</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mspace width="veryverythinmathspace" />
           </mml:mtd>
           <mml:mtd>
             <mml:mo mathcolor="gray" form="prefix">Im</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi fontstyle="upright">&Dscr;</mml:mi>
