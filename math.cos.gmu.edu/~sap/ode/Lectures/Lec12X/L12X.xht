         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   so
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <span style="width:2em"><span style="padding-left:2em"></span></span>Next find
   values of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mspace width="thickmathspace" />
     </mml:mrow>
   </mml:math>and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   that satisfy
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>3</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                     <mml:mspace width="thickmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                     <mml:mspace width="thickmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                     <mml:mspace width="thickmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                     <mml:mspace width="thickmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                     <mml:mspace width="thickmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>2</mml:mn>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   to
   get<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mn>3</mml:mn>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mi>c</mml:mi>
                       <mml:mrow>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleComma;</mml:mo>
                         <mml:mn>1</mml:mn>
                       </mml:mrow>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Then
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   so
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Thus
   the basis we want is
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>2</mml:mn>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>2</mml:mn>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 12.16
   </p>
</div>
