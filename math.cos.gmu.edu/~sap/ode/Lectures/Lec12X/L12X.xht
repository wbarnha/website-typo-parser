     <mml:mrow>
       <mml:mi>W</mml:mi>
     </mml:mrow>
   </mml:math>
   for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>W</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Then
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mover accent="true">
                   <mml:mi>W</mml:mi>
                   <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                 </mml:mover>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mover accent="true">
                         <mml:mi>x</mml:mi>
                         <mml:mo form="postfix" accent="true">&die;</mml:mo>
                       </mml:mover>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mover accent="true">
                         <mml:mi>x</mml:mi>
                         <mml:mo form="postfix" accent="true">&die;</mml:mo>
                       </mml:mover>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:msub>
                             <mml:mi>p</mml:mi>
                             <mml:mn>0</mml:mn>
                           </mml:msub>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msub>
                           <mml:mover accent="true">
                             <mml:mi>x</mml:mi>
                             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                           </mml:mover>
                           <mml:mn>2</mml:mn>
                         </mml:msub>
                       </mml:mrow>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>q</mml:mi>
                           <mml:mn>0</mml:mn>
                         </mml:msub>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msub>
                           <mml:mi>x</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msub>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:msub>
                             <mml:mi>p</mml:mi>
                             <mml:mn>0</mml:mn>
                           </mml:msub>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msub>
                           <mml:mover accent="true">
                             <mml:mi>x</mml:mi>
                             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                           </mml:mover>
                           <mml:mn>1</mml:mn>
                         </mml:msub>
                       </mml:mrow>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>q</mml:mi>
                           <mml:mn>0</mml:mn>
                         </mml:msub>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msub>
                           <mml:mi>x</mml:mi>
                           <mml:mn>1</mml:mn>
                         </mml:msub>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msub>
                       <mml:mi>p</mml:mi>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>q</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>p</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>q</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:msub>
                       <mml:mi>p</mml:mi>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:msub>
                     <mml:mi>p</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msub>
                     <mml:mover accent="true">
                       <mml:mi>x</mml:mi>
                       <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                     </mml:mover>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msub>
                     <mml:mi>p</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>x</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msub>
                         <mml:mover accent="true">
                           <mml:mi>x</mml:mi>
                           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                         </mml:mover>
                         <mml:mn>2</mml:mn>
                       </mml:msub>
                     </mml:mrow>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>x</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msub>
                         <mml:mover accent="true">
                           <mml:mi>x</mml:mi>
                           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                         </mml:mover>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msub>
                     <mml:mi>p</mml:mi>
                     <mml:mn>0</mml:mn>
                   </mml:msub>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>W</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p>
   Thus
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>W</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>W</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   which is known as Abel's ODE. Observe that Abel's ODE is a linear first-order
   ODE with solution
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>W</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>c</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msub>
                   <mml:mi>p</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mspace width="thickmathspace" />
       <mml:mtext mathcolor="black">and some constant&ensp;</mml:mtext>
       <mml:mi>c</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   If the constant of integration
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>,
   then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>W</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is zero for all
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mrow>
         <mml:mstyle fontstyle="slanted">
           <mml:mtext>,</mml:mtext>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and if
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>c</mml:mi>
         <mml:mo form="infix">&ne;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>then
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>W</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is never zero for any
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mrow>
         <mml:mstyle fontstyle="slanted">
           <mml:mtext>.</mml:mtext>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   The latter assertion follows from the fact that the exponential term in the
   last equation is never zero. We summarize this result in the following
   theorem.
</p>
<p>
   <br/><a id="Abel"></a>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="theorem">
   <span class="leadin">Theorem</span><p class="theorem-FirstPara">
      12.2: [Abel - The Wronskian Condition for Solutions]:<a id="Theorem12.02"></a>
   </p>
</div>
<p class="QQQTheoremText">
   Suppose
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   are any two solutions to the ODE
</p>
<p class="QQQTheoremText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>q</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQTheoremText">
   Then the Wronskian
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>W</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>W</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mspace width="negativethinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is either never zero for any
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&Element;</mml:mo>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
     </mml:mrow>
   </mml:math>
   or zero for all
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Moreover <strong>Abel's formula</strong> for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>W</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mspace width="negativethinmathspace" />
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is given
   by<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>W</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">,</mml:mo>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="negativethinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>c</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msub>
                   <mml:mi>p</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mspace width="thickmathspace" />
       <mml:mtext mathcolor="black">and some constant&ensp;</mml:mtext>
       <mml:mi>c</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>A solution pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   to the ODE
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>q</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   for which
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>W</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is never zero is special. We will see later in
   <span class="QQQHyperLink-BodyText"><a href="#Corollary12.08"> <span class="QQQHyperLink-BodyText">Corollary
   12.8</span></a></span> <span class="QQQHyperLink-BodyText">t</span>hat the
   Wronskian distinguishes amongst pairs of solutions that can serve as a general
   solution and those that cannot. For now, we single out those solution pairs for
   which the Wronskian is never zero.
</p>
<p>
   <br/>
</p>
<p>
   <a id="Basis"></a>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="definition">
   <span class="leadin">Definition</span><p class="definition-FirstPara">
      12.3: [Basis]:<a id="Definition12.03"></a>
   </p>
</div>
<p class="QQQSpecialText">
   A pair of solutions
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   to the ODE
</p>
<p class="QQQSpecialText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>q</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQSpecialText">
   is called a <strong>basis </strong>if
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>W</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is never zero on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Henceforth we denote a basis by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   Thus each of the pairs
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   listed in
   <span class="QQQHyperLink-BodyText"><a href="#Table12.01"> <span class="QQQHyperLink-BodyText">Table
   12.1</span></a></span> is a basis. By taking
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec11X/L11X.xht#LinCombo"> <span class="QQQHyperLink-BodyText">linear
   combinations</span></a> </span>of the basis set functions for any case in
   <span class="QQQHyperLink-BodyText"><a href="#Table12.01"> <span class="QQQHyperLink-BodyText">Table
   12.1</span></a></span>, we can create other solutions.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      12.4: <a id="Example12.04"></a>
   </p>
</div>
<p class="QQQExampleText">
   We saw in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec11X/L11X.xht#Example11.08"> <span class="QQQHyperLink-BodyText">Example
   11.8</span></a></span> that the
   ODE<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>has
   solutions<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   The Wronskian of the pair
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>W</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">,</mml:mo>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="negativethinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mn>2</mml:mn>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&ne;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Since any multiple of a solution is a solution,
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>3</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>t</mml:mi>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is also a solution. Consequently
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>W</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">,</mml:mo>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>3</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="negativethinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mi>t</mml:mi>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   In view of
   <span class="QQQHyperLink-BodyText"><a href="#Definition12.03"> <span class="QQQHyperLink-BodyText">Definition
   12.3</span></a></span>, the pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   is a basis and the pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>3</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   is not a basis. What distinguishes the pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   from the pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>3</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   is that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>3</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is a constant multiple of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   that is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>3</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Yet another solution to the ODE is
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>4</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   for all
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Consequently
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>W</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">,</mml:mo>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>4</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="negativethinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
           <mml:mspace width="thinmathspace" />
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mi>t</mml:mi>
             </mml:msup>
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mi>t</mml:mi>
             </mml:msup>
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   The pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>4</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   is not a basis. Note that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>4</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is a constant multiple of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="postfix">;</mml:mo>
     </mml:mrow>
   </mml:math>
   that is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>4</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">&sdot;</mml:mo>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 12.4
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p class="QQQSubHead2">
   <br/>Geometric interpretation of the Wronskian<a id="GeometricWronskian"></a>
</p>
<p>
   <br/>Suppose
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   is a pair of solutions to the ODE
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>q</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   The matrix
</p>
<p>
   <mml:math display="inline">
   </mml:math><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
       <mml:mtable align="axis">
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mspace width="negativethinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
           <mml:mtd>
             <mml:msub>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mspace width="negativethinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   whose determinant defines the Wronskian, is comprised of the two vectors
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mover accent="true">
                     <mml:mi>x</mml:mi>
                     <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                   </mml:mover>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mspace width="negativethinmathspace" />
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&emsp;and&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mover accent="true">
                     <mml:mi>x</mml:mi>
                     <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                   </mml:mover>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mspace width="negativethinmathspace" />
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   A relationship from vector calculus ties the magnitude of the cross-product of
   the vectors
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   to the absolute value of the Wronskian (Click
   <span class="QQQNote-BodyText"> <a target="_blank" href="L12X__note1.xht"><span class="QQQNote-BodyText">here</span></a> </span>
   for proof of this relationship.):
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">&Verbar;</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo form="infix">&times;</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">&Verbar;</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         <mml:mrow>
           <mml:mi>W</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">,</mml:mo>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   We know from the definition of the cross-product that
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">&times;</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">r</mml:mtext>
           </mml:mstyle>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">&Verbar;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">&Verbar;</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">&Verbar;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">&Verbar;</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>&theta;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   where
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&theta;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>&theta;</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is the angle formed by the vectors
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mspace width="veryverythinmathspace" />
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   In turn, this formula is the area of the parallelogram formed by the vectors
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>&theta;</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>&pi;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Thus the value of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>W</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mspace width="negativethinmathspace" />
     </mml:mrow>
   </mml:math>
   depends on the sine of the angle
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&theta;</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQCenteredBodyText">
   
<!-- MSISEMCOM /FRAME{ftbpFU}{2.7596in}{2.3964in}{0pt}{}{}{geometry of wronskian.png}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 2.7596in;  height 2.3964in;  depth 0pt;  original-width 3.9435in;  original-height 3.4765in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "1";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><img src="Images/L12X__1.png" alt="geometry of wronskian.png" height="230px" /></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<p>
Note that all vectors are functions of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
   so that their positions may change with
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   When
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   is a basis for the ODE, their Wronskian is never zero; hence the area of the
   parallelogram can never be zero.
</p>
<p>
   <br/>
</p>
<p class="QQQSubHead1">
   <br/>12.2 LINEAR INDEPENDENCE OF SOLUTIONS<a id="LinInd"></a>
</p>
<p>
   <br/>
</p>
<p>
   <span class="QQQHyperLink-BodyText"><a href="#Example12.04"> <span class="QQQHyperLink-BodyText">Example
   12.4</span></a></span> illustrates both possible outcomes of the
   <span class="QQQHyperLink-BodyText"><a href="#Theorem12.02"> <span class="QQQHyperLink-BodyText">Wronskian
   condition</span></a></span>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mtext mathcolor="black">Either&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>W</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mspace width="negativethinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;is zero for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;or&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>W</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo form="infix">,</mml:mo>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mspace width="negativethinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">is nonzero for all&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><a id="Eqn.(12.05)"></a>
</p>
<p>
   In that example we see that when one solution is zero or a constant multiple
   of the other, then the Wronskian is zero for all
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Indeed, this is the case in general. Let
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   be nonzero solutions to the ODE
</p>
<p class="QQQSpecialText">
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(12.5)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>p</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>q</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>c</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   for some constant
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>c</mml:mi>
     </mml:mrow>
   </mml:math>
   and all
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&Element;</mml:mo>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mi>W</mml:mi>
