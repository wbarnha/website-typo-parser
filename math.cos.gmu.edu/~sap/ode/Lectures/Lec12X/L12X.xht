               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   What are the implications of this theorem?
</p>
<p>
   <br/>1. ALL bases for a given ODE are linearly related by an invertible matrix
   that is easy to calculate.
</p>
<p>
   2. There are infinitely many bases.
</p>
<p>
   <br/>
</p>
<p>
   We include the proof of this theorem because the steps involved are useful in
   problem solving.
</p>
<p>
   <br/><span class="large2">Proof of All Possible
   Bases:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>Suppose
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>y</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>y</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   are bases for the ODE
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>q</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   In particular, since
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is a solution, then
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   must be a linear combination of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>,
   say
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo>&InvisibleComma;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:msub>
           <mml:mspace width="thickmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo>&InvisibleComma;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
           </mml:msub>
           <mml:mspace width="thickmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   Likewise, since
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is a solution, then
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   must also be a linear combination of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>,
   say
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>y</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleComma;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:msub>
           <mml:mspace width="thickmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleComma;</mml:mo>
               <mml:mn>2</mml:mn>
             </mml:mrow>
           </mml:msub>
           <mml:mspace width="thickmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Since
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>y</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   are
   LI<mml:math display="inline">
     <mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>&alpha;</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>y</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mi>&beta;</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>y</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
