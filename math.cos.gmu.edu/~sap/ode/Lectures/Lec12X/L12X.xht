           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p class="QQQExampleText">
   In order to use these requirements we must first differentiate Eqn. (12.6) to
   obtain
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(12.8)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mover accent="true">
                     <mml:mi>&phi;</mml:mi>
                     <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                   </mml:mover>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mover accent="true">
                     <mml:mi>&phi;</mml:mi>
                     <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                   </mml:mover>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mi>t</mml:mi>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>Set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   in Eqn. (12.6) and use Eqn. (12.7a) to
   get<a id="Eqn.(12.9ab)"></a><mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(12.9a)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>Similarly,
   set
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   in Eqn. (12.8) and use Eqn. (12.7b) to
   get<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(12.9b)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
