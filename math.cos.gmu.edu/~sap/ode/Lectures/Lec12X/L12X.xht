               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>c</mml:mi>
                     <mml:mrow>
                       <mml:mn>2</mml:mn>
                       <mml:mo>&InvisibleComma;</mml:mo>
                       <mml:mn>2</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>
	
   </mml:math>
</p>
<p class="QQQExampleText">
   These two vector equations can be combined to get the single matrix
   equation<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   It follows that the coefficients
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msub>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>12</mml:mn>
           <mml:mtext>,</mml:mtext>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>21</mml:mn>
           <mml:mtext>,</mml:mtext>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleComma;</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   we want are the entries of the inverse of the matrix
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
       <mml:mtable align="axis" columnalign="right right">
         <mml:mtr>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>In
   fact
   set<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>1</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">C</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>1</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleComma;</mml:mo>
                     <mml:mn>2</mml:mn>
                   </mml:mrow>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mtext mathcolor="black">and</mml:mtext>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mi>&Phi;</mml:mi>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Then we have the matrix
   equation<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">C</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mi>&Phi;</mml:mi>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>which
   implies<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">C</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math>Use
   row reduction (or any other applicable) method to
   get<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">C</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mtd>
             <mml:mtd>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mstyle displaystyle="false" scriptlevel="0">
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>2</mml:mn>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mtd>
             <mml:mtd>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Finally
   from
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(12.06)"> <span class="QQQHyperLink-BodyText">Eqn.
   (12.6)</span></a></span> we have the ideal
   basis<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                   <mml:mtable align="axis">
                     <mml:mtr>
                       <mml:mtd>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>1</mml:mn>
                         </mml:msub>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mtd>
                     </mml:mtr>
                     <mml:mtr>
                       <mml:mtd>
                         <mml:msub>
                           <mml:mi>&phi;</mml:mi>
                           <mml:mn>2</mml:mn>
                         </mml:msub>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mtd>
                     </mml:mtr>
                   </mml:mtable>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                 </mml:mrow>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mtable align="axis" columnalign="right right">
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mtd>
                         <mml:mtd>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mrow>
                             <mml:mstyle displaystyle="false" scriptlevel="0">
                               <mml:mfrac>
                                 <mml:mn>1</mml:mn>
                                 <mml:mn>2</mml:mn>
                               </mml:mfrac>
                             </mml:mstyle>
                           </mml:mrow>
                         </mml:mtd>
                       </mml:mtr>
                     </mml:mtable>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                   <mml:mspace width="thinmathspace" />
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mtable align="axis">
                       <mml:mtr>
                         <mml:mtd>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mi>t</mml:mi>
                           </mml:msup>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:msup>
                         </mml:mtd>
                       </mml:mtr>
                     </mml:mtable>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mtable align="axis">
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mi>t</mml:mi>
                         </mml:msup>
                       </mml:mrow>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mtd>
                   </mml:mtr>
                   <mml:mtr>
                     <mml:mtd>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mi>t</mml:mi>
                         </mml:msup>
                       </mml:mrow>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle displaystyle="false" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mtd>
                   </mml:mtr>
                 </mml:mtable>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>or<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&phi;</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&phi;</mml:mi>
