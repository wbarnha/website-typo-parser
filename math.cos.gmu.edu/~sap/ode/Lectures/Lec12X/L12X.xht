       </mml:mrow>
       <mml:mrow>
         <mml:mstyle mathsize="2.40ex">
           <mml:mtext>,</mml:mtext>
         </mml:mstyle>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>and<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>p</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>q</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>x</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mrow>
         <mml:mstyle mathsize="2.40ex">
           <mml:mtext>,</mml:mtext>
         </mml:mstyle>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
