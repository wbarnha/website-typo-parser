             </mml:msub>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
             </mml:mrow>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mi>t</mml:mi>
               </mml:msup>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle displaystyle="false" scriptlevel="0">
