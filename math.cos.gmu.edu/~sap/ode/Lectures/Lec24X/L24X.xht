                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&lambda;</mml:mi>
                           <mml:mn>2</mml:mn>
