                             <mml:mn>1</mml:mn>
                             <mml:mn>2</mml:mn>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mspace width="thickmathspace" />
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mtable align="axis" columnalign="right">
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>0</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mn>1</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                     </mml:mtable>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mtable align="axis">
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>2</mml:mn>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:msup>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mi mathcolor="gray">sin</mml:mi>
                             <mml:mo>&ApplyFunction;</mml:mo>
                             <mml:mrow>
                               <mml:mfrac>
                                 <mml:mn>1</mml:mn>
                                 <mml:mn>2</mml:mn>
                               </mml:mfrac>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>0</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                     </mml:mtable>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                     <mml:mtable align="axis">
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mn>0</mml:mn>
                         </mml:mtd>
                       </mml:mtr>
                       <mml:mtr>
                         <mml:mtd>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:msup>
                               <mml:mi>e</mml:mi>
                               <mml:mrow>
                                 <mml:mo form="prefix">&minus;</mml:mo>
                                 <mml:mi>t</mml:mi>
                               </mml:mrow>
                             </mml:msup>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mi mathcolor="gray">cos</mml:mi>
                             <mml:mo>&ApplyFunction;</mml:mo>
                             <mml:mrow>
                               <mml:mfrac>
                                 <mml:mn>1</mml:mn>
                                 <mml:mn>2</mml:mn>
                               </mml:mfrac>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mtd>
                       </mml:mtr>
                     </mml:mtable>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="infix">=</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                   <mml:mtable align="axis">
                     <mml:mtr>
                       <mml:mtd>
                         <mml:mn>2</mml:mn>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mo form="prefix">&minus;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mtd>
                     </mml:mtr>
                     <mml:mtr>
                       <mml:mtd>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:msup>
                             <mml:mi>e</mml:mi>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:msup>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">cos</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mfrac>
                               <mml:mn>1</mml:mn>
                               <mml:mn>2</mml:mn>
                             </mml:mfrac>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mtd>
                     </mml:mtr>
                   </mml:mtable>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Then
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <strong>w</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   are two linearly independent (vector) solutions to
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mrow>
                 <mml:mi>d</mml:mi>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">r</mml:mtext>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mrow>
                 <mml:mi>d</mml:mi>
                 <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mfrac>
                   <mml:mn>1</mml:mn>
                   <mml:mn>4</mml:mn>
                 </mml:mfrac>
               </mml:mtd>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mspace width="thinmathspace" />
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>To calculate a fundamental matrix just create the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>2</mml:mn>
       <mml:mo form="infix">&times;</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
   matrix whose columns are
   <strong>u</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <strong>w</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>Thus
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">R</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mtd>
               <mml:mtd>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:mi>t</mml:mi>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>The
   general solution to the system
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">cos</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:mfrac>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">sin</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:mfrac>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mo form="prefix">&minus;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">sin</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:mfrac>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mrow>
                       <mml:mo form="prefix">&minus;</mml:mo>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">cos</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:mfrac>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>or
   in terms of the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>
   components<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mi>y</mml:mi>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mfrac>
                       <mml:mn>1</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:mfrac>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 24.17
   </p>
</div>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <a id="RealRepeatedEigenvalue"></a>
</p>
<p class="QQQSubHead2">
   <br/>Real Repeated Eigenvalue
</p>
<p>
   <br/>When <strong>A</strong> has a real repeated eigenvalue
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&lambda;</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Appendix/Lec00A/L00A.xht#Schur"> <span class="QQQHyperLink-BodyText">Schur's theorem </span></a></span>
    tells us that <strong>A</strong> is similar to the
   matrix<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
       <mml:mtable align="axis" columnalign="right right">
         <mml:mtr>
           <mml:mtd>
             <mml:mi>a</mml:mi>
           </mml:mtd>
           <mml:mtd>
             <mml:mi>b</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>0</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mi>a</mml:mi>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   one of two possibilities arise.
</p>
<p class="QQQIndentRemarkText">
   <br/>Case (a):
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   There are two corresponding linearly independent eigenvectors
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   so that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mi>a</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">v</mml:mtext>
           </mml:mstyle>
           <mml:mn>1</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mi>a</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">v</mml:mtext>
           </mml:mstyle>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   are eigen-pairs,
</p>
<p>
   or<a id="DeficientMatrix"></a>
</p>
<p class="QQQIndentRemarkText">
   Case (b):
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
       <mml:mo form="infix">&ne;</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   There is just one corresponding eigenvector <strong>v</strong> and
   consequently just one eigen-pair
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">v</mml:mtext>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   In this case we call  <strong>A</strong> <strong>deficient</strong>.
</p>
<p>
   <br/>In either case when
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mi>a</mml:mi>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>b</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>a</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>its
   characteristic polynomial
   is<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(24.19)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mi>p</mml:mi>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>&lambda;</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">det</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mrow>
                 <mml:mstyle fontweight="bold" fontstyle="upright">
                   <mml:mspace width="thickmathspace" />
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>&lambda;</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo mathcolor="gray" form="prefix">det</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mi>a</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&lambda;</mml:mi>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mi>b</mml:mi>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mi>a</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mi>&lambda;</mml:mi>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>a</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>&lambda;</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>&lambda;</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>a</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>Thus
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&lambda;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math>
   is a repeated eigenvalue of <strong>A.</strong>
</p>
<p>
   <br/>Case (a): the first possibility. If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then every nonzero vector is an eigenvector corresponding to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&lambda;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Indeed, then <strong>A</strong> is the diagonal
   matrix<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mi>a</mml:mi>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>a</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>so
   that for any nonzero
   vector<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:msub>
                 <mml:mi>v</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:msub>
                 <mml:mi>v</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msub>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(24.20)</mtext></mtd>

	  <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">Av</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="right right">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mi>a</mml:mi>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mi>a</mml:mi>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>a</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>a</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v</mml:mtext>
       </mml:mstyle>
      </mml:mrow>
		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Since all nonzero vectors are eigenvectors corresponding to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&lambda;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   and there can be at most two linearly independent vectors in
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi fontstyle="upright">&Ropf;</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   without loss of generality we can choose these eigenvectors to be the unit
   vectors
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">e</mml:mtext>
           </mml:mstyle>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and</mml:mtext>
       <mml:mtext>&emsp;&emsp;</mml:mtext>
       <mml:mrow>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">e</mml:mtext>
           </mml:mstyle>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Indeed
   <strong>Ae</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math><strong>e</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <strong>Ae</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math><strong>e</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   must hold as Eqn. (24.20) shows that
   <strong>Av</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math><strong>v</strong>
   for all vectors <strong>v</strong>. Thus we have the two eigen-pairs
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mi>a</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">e</mml:mtext>
           </mml:mstyle>
           <mml:mn>1</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">e</mml:mtext>
             </mml:mstyle>
             <mml:mn>2</mml:mn>
           </mml:msub>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   It follows that the linear system
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>Ar</strong>
   has fundamental matrix (in columnar form)
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">R</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mi>a</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">e</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mrow>
             <mml:mrow>
               <mml:mstyle displaystyle="true" scriptlevel="0">
                 <mml:mfrac linethickness="0">
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                 </mml:mfrac>
               </mml:mstyle>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mi>a</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">e</mml:mtext>
               </mml:mstyle>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>Case (b): the second possibility. If
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">&ne;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mi>a</mml:mi>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>b</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>a</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>has just one eigenvector. Indeed, the eigenvector is the solution to the
   equation
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mspace width="thickmathspace" />
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>0</strong>,
   or equivalently, solve
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>a</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>a</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>b</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>a</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>a</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>v</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>v</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(24.21)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mi>b</mml:mi>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>1</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>v</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msub>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Using
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Appendix/Lec00A/L00A.xht#ElementaryRowOperations"> <span class="QQQHyperLink-BodyText">elementary
   row operations</span></a></span>, we reduce this Eqn. (24.21) to the single
   scalar equation
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mi>v</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>Since
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">&ne;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we must have
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>v</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v&ensp;</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right">
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>v</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>v</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>Since the variable
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>v</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is free and any nonzero scalar multiple of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mstyle fontweight="bold" fontstyle="upright">
         <mml:mi>v</mml:mi>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
   is also a solution of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mspace width="thickmathspace" />
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>0</strong>,
   we may choose
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>v</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
   (Note that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>v</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   cannot equal zero; otherwise
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>0 </strong>and<strong> </strong>is
   not an eigenvector.) Thus we have the first eigen-pair
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">v</mml:mtext>
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo form="infix">,</mml:mo>
           <mml:mspace width="thickmathspace" />
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>and
   a solution to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>d</mml:mi>
       <mml:mtext>&ZeroWidthSpace;</mml:mtext>
     </mml:mrow>
   </mml:math><strong>r/</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>d</mml:mi>
         <mml:mtext>&ZeroWidthSpace;</mml:mtext>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math>
   <strong>Ar</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mstyle fontweight="bold" fontstyle="upright">
         <mml:mtext>,</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
   namely<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>a</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Because
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&lambda;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>a</mml:mi>
     </mml:mrow>
   </mml:math>
   is the only eigenvalue, there cannot be any other linearly independent
   solutions to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mspace width="thickmathspace" />
           </mml:mstyle>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>a</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>0.
   </strong>So we ask, how can we determine a second linearly independent
   solution
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r&ensp;</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>Ar</strong>?
   Because the matrix exponential shows us through
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec23X/L23X.xht#Theorem23.16"> <span class="QQQHyperLink-BodyText">Theorem
   23.16</span></a></span> that we can construct a pair of linearly independent
   solutions to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r&ensp;</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>Ar</strong>,
   we return to the matrix exponential to calculate a second linearly independent
   solution.
</p>
<p>
   <br/>Start with the matrix exponential
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   First we write
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>a</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I&ensp;</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mtext mathcolor="black">&ensp;</mml:mtext>
           <mml:mi>a</mml:mi>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I&ensp;</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mo form="prefix">+</mml:mo>
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>a</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">+</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>a</mml:mi>
           </mml:mrow>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">I</mml:mtext>
           </mml:mstyle>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Since
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec23X/L23X.xht#Eqn.(23.17)"> <span class="QQQHyperLink-BodyText">Eqn.
   (23.17)</span></a></span> implies
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>a</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>a</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mspace width="thinmathspace" />
     </mml:mrow>
   </mml:math><strong>I</strong>
   <span class="QQQNote-BodyText"> <a target="_blank" href="L24X__note5.xht"><span class="QQQNote-BodyText">(see
   here for a proof)</span></a> </span>, then
</p>
<p>
   <br/><mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(24.22)</mtext></mtd>

	 <mml:mrow>
	  <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>a</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>

             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">+</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">A</mml:mtext>
                 </mml:mstyle>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>a</mml:mi>
                 </mml:mrow>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">I</mml:mtext>
                 </mml:mstyle>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>a</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mspace width="thinmathspace" />
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">I</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mspace width="thinmathspace" />
         </mml:mstyle>
       </mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">A</mml:mtext>
               </mml:mstyle>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>a</mml:mi>
               </mml:mrow>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">I</mml:mtext>
               </mml:mstyle>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:msup>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mi>a</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:msup>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">A</mml:mtext>
                 </mml:mstyle>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>a</mml:mi>
                 </mml:mrow>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">I</mml:mtext>
                 </mml:mstyle>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
      </mml:mrow>
		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p>
   <br/>According to
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec23X/L23X.xht#Theorem23.16"> <span class="QQQHyperLink-BodyText">Theorem
   23.16</span></a></span>,
   <strong>r</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
         </mml:mrow>
       </mml:msup>
       <mml:mspace width="thinmathspace" />
     </mml:mrow>
   </mml:math><strong>u</strong>
   is a solution to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mover>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">r</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mstyle fontweight="bold" fontstyle="upright">
             <mml:mo form="infix">&sdot;</mml:mo>
           </mml:mstyle>
         </mml:mrow>
       </mml:mover>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="infix">=</mml:mo>
     </mml:mrow>
   </mml:math><strong>Ar</strong>
   for every vector <strong>u</strong>. Thus by Eqn. (24.22) we have
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
         </mml:mrow>
       </mml:msup>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">u</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>a</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
         </mml:msup>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">A</mml:mtext>
               </mml:mstyle>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>a</mml:mi>
               </mml:mrow>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">I</mml:mtext>
               </mml:mstyle>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">u</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   If we can calculate
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>a</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   then hopefully we can determine another linearly independent solution of the
   form
   e<mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>a</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math><strong>u</strong>
   for some vector <strong>u</strong>. Again use
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec23X/L23X.xht#Eqn.(23.17)"> <span class="QQQHyperLink-BodyText">Eqn.
   (23.17)</span></a></span> to expand
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>a</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:msup>
       <mml:mspace width="thinmathspace" />
     </mml:mrow>
   </mml:math><strong>u.</strong>
   Substitute
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">A</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>a</mml:mi>
         </mml:mrow>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">I</mml:mtext>
         </mml:mstyle>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mspace width="thickmathspace" />
     </mml:mrow>
   </mml:math>for
   <strong>A</strong> in
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec23X/L23X.xht#Eqn.(23.17)"> <span class="QQQHyperLink-BodyText">Eqn.
   (23.17)</span></a> </span>to get
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(24.23)</mtext></mtd>

	  <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">A</mml:mtext>
             </mml:mstyle>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>a</mml:mi>
             </mml:mrow>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">I</mml:mtext>
             </mml:mstyle>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:msup>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">u</mml:mtext>
       </mml:mstyle>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">I</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">+</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">A</mml:mtext>
               </mml:mstyle>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>a</mml:mi>
               </mml:mrow>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">I</mml:mtext>
               </mml:mstyle>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo form="postfix">!</mml:mo>
               </mml:mrow>
             </mml:mfrac>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">A</mml:mtext>
                 </mml:mstyle>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>a</mml:mi>
                 </mml:mrow>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">I</mml:mtext>
                 </mml:mstyle>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mn>2</mml:mn>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mrow>
                 <mml:mn>3</mml:mn>
                 <mml:mo form="postfix">!</mml:mo>
               </mml:mrow>
             </mml:mfrac>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mi>t</mml:mi>
               <mml:mn>3</mml:mn>
             </mml:msup>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msup>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">A</mml:mtext>
                 </mml:mstyle>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>a</mml:mi>
                 </mml:mrow>
                 <mml:mstyle fontweight="bold">
                   <mml:mtext mathcolor="black">I</mml:mtext>
                 </mml:mstyle>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mn>3</mml:mn>
             </mml:msup>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mi>&ctdot;</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">u</mml:mtext>
       </mml:mstyle>
      </mml:mrow>
		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p>
   <br/>An easy calculation shows us that
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>a</mml:mi>
           </mml:mrow>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">I</mml:mtext>
           </mml:mstyle>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math><strong>0</strong>
   (<span class="it">the zero matrix</span>). Indeed,
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">A</mml:mtext>
           </mml:mstyle>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mi>a</mml:mi>
           </mml:mrow>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">I</mml:mtext>
           </mml:mstyle>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mi>a</mml:mi>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>b</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>a</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mi>a</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0</mml:mn>
                   </mml:mtd>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>b</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
         <mml:mtable align="axis" columnalign="right right">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Now choose any nonzero vector <strong>u</strong> so that <strong>u</strong>
   and <strong>v</strong> are linearly independent. As <strong>u</strong> cannot
   be a eigenvector for <strong>A</strong> (remember that <strong>u</strong> and
   <strong>v</strong> are linearly independent), we can choose <strong>u</strong>
   to be the solution to
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">A</mml:mtext>
         </mml:mstyle>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>a</mml:mi>
         </mml:mrow>
         <mml:mstyle fontweight="bold">
           <mml:mtext mathcolor="black">I</mml:mtext>
         </mml:mstyle>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">u</mml:mtext>
       </mml:mstyle>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mstyle fontweight="bold" fontstyle="upright">
           <mml:mo form="infix">=</mml:mo>
         </mml:mstyle>
       </mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">v</mml:mtext>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>It
   follows from Eqn. (24.23) that<a id="Eqn.(24.24)"></a>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>t</mml:mi>
           <mml:mstyle fontweight="bold">
