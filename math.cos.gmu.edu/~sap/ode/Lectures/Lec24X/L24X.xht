                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">v</mml:mtext>
                       </mml:mstyle>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mtext mathcolor="black">2. One real repeated</mml:mtext>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mi>&lambda;</mml:mi>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mtext>&emsp;</mml:mtext>
                   <mml:mtext mathcolor="black">(a)</mml:mtext>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mtext mathcolor="black">Linearly independent</mml:mtext>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">v</mml:mtext>
                       </mml:mstyle>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">v</mml:mtext>
                       </mml:mstyle>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">v</mml:mtext>
                       </mml:mstyle>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">v</mml:mtext>
                       </mml:mstyle>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mtext>&emsp;</mml:mtext>
                   <mml:mtext mathcolor="black">(b)</mml:mtext>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mtext mathcolor="black">Single vector&ensp;</mml:mtext>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">v</mml:mtext>
                   </mml:mstyle>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mstyle fontweight="bold">
                       <mml:mtext mathcolor="black">v</mml:mtext>
                     </mml:mstyle>
                     <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mstyle displaystyle="true" scriptlevel="0">
                           <mml:mfrac linethickness="0">
                             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           </mml:mfrac>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mi>&lambda;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mstyle fontweight="bold">
                           <mml:mtext mathcolor="black">u</mml:mtext>
                         </mml:mstyle>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mstyle fontweight="bold" fontstyle="upright">
                               <mml:mo form="prefix">+</mml:mo>
                             </mml:mstyle>
                           </mml:mrow>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                         <mml:mstyle fontweight="bold">
                           <mml:mtext mathcolor="black">v</mml:mtext>
                         </mml:mstyle>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mtext mathcolor="black">,</mml:mtext>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mtext mathcolor="black">where&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mstyle fontweight="bold">
                       <mml:mtext mathcolor="black">A</mml:mtext>
                     </mml:mstyle>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mstyle fontweight="bold" fontstyle="upright">
                           <mml:mo form="prefix">&minus;</mml:mo>
                         </mml:mstyle>
                       </mml:mrow>
                       <mml:mi>&lambda;</mml:mi>
                     </mml:mrow>
                     <mml:mstyle fontweight="bold">
                       <mml:mtext mathcolor="black">I</mml:mtext>
                     </mml:mstyle>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">u</mml:mtext>
                   </mml:mstyle>
                   <mml:mrow>
                     <mml:mstyle fontweight="bold" fontstyle="upright">
                       <mml:mo form="infix">=</mml:mo>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">v</mml:mtext>
                   </mml:mstyle>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mtext mathcolor="black">3. Complex conjugate</mml:mtext>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>&lambda;</mml:mi>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                     <mml:mo form="infix">=</mml:mo>
                     <mml:msub>
                       <mml:mover>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mo stretchy="true" accent="true">&OverBar;</mml:mo>
                       </mml:mover>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mtext mathcolor="black">Complex conjugate</mml:mtext>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">v</mml:mtext>
                       </mml:mstyle>
                       <mml:mtext mathcolor="black">1</mml:mtext>
                     </mml:msub>
                     <mml:mrow>
                       <mml:mstyle fontweight="bold" fontstyle="upright">
                         <mml:mo form="infix">=</mml:mo>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:msub>
                       <mml:mover>
                         <mml:mstyle fontweight="bold">
                           <mml:mtext mathcolor="black">v</mml:mtext>
                         </mml:mstyle>
                         <mml:mo stretchy="true" accent="true">&OverBar;</mml:mo>
                       </mml:mover>
                       <mml:mtext mathcolor="black">2</mml:mtext>
                     </mml:msub>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mi>&alpha;</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">[</mml:mo>
                       <mml:mrow>
                         <mml:mi mathcolor="gray">cos</mml:mi>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mi>&beta;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mspace width="thinmathspace" />
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">a</mml:mtext>
                       </mml:mstyle>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle fontweight="bold" fontstyle="upright">
                             <mml:mo form="prefix">&minus;</mml:mo>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mi>&beta;</mml:mi>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mspace width="thinmathspace" />
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">b</mml:mtext>
                       </mml:mstyle>
                       <mml:mo form="postfix" fence="true" stretchy="false">]</mml:mo>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mrow>
                         <mml:mi>&alpha;</mml:mi>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mi>i</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&beta;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msub>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mrow>
                         <mml:mi>&alpha;</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mrow>
                           <mml:mi>i</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>&beta;</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mtext>,</mml:mtext>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:msub>
                     <mml:mstyle fontweight="bold">
                       <mml:mtext mathcolor="black">v</mml:mtext>
                     </mml:mstyle>
                     <mml:mtext mathcolor="black">1</mml:mtext>
                   </mml:msub>
                   <mml:mrow>
                     <mml:mstyle fontweight="bold" fontstyle="upright">
                       <mml:mo form="infix">=</mml:mo>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">a</mml:mtext>
                   </mml:mstyle>
                   <mml:mrow>
                     <mml:mstyle fontweight="bold" fontstyle="upright">
                       <mml:mo form="infix">+</mml:mo>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mstyle fontweight="bold">
                   </mml:mstyle>
                   <mml:mrow>
                     <mml:mi>i</mml:mi>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msub>
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">b,&ensp;v</mml:mtext>
                       </mml:mstyle>
                       <mml:mtext mathcolor="black">2</mml:mtext>
                     </mml:msub>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:mstyle fontweight="bold" fontstyle="upright">
                       <mml:mo form="infix">=</mml:mo>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">a</mml:mtext>
                   </mml:mstyle>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle fontweight="bold" fontstyle="upright">
                         <mml:mo form="prefix">&minus;</mml:mo>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mi>i</mml:mi>
                   </mml:mrow>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">b</mml:mtext>
                   </mml:mstyle>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac linethickness="0">
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mi>&alpha;</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mrow>
                             <mml:mi>&beta;</mml:mi>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mspace width="thinmathspace" />
                       <mml:mstyle fontweight="bold">
                         <mml:mtext mathcolor="black">a</mml:mtext>
                       </mml:mstyle>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle fontweight="bold" fontstyle="upright">
                             <mml:mo form="prefix">+</mml:mo>
                           </mml:mstyle>
                         </mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi mathcolor="gray">cos</mml:mi>
                             <mml:mo>&ApplyFunction;</mml:mo>
                             <mml:mrow>
                               <mml:mi>&beta;</mml:mi>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mspace width="thinmathspace" />
                           <mml:mstyle fontweight="bold">
                             <mml:mtext mathcolor="black">b</mml:mtext>
                           </mml:mstyle>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mspace width="thinmathspace" />
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd columnalign="left">
                   <mml:mrow>
                     <mml:mi>&alpha;</mml:mi>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>&beta;</mml:mi>
                   </mml:mrow>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mtext mathcolor="black">real</mml:mtext>
                   <mml:mtext mathcolor="black">,&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:mi>&beta;</mml:mi>
                     <mml:mo form="infix">&ne;</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">a,&ensp;b&ensp;</mml:mtext>
                   </mml:mstyle>
                   <mml:mtext mathcolor="black">real</mml:mtext>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><span class="QQQSansSerifItalic16">If
   </span><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">r</mml:mtext>
             </mml:mstyle>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">is a  basis, then a general solution has
   the form</span>
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">r</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">r</mml:mtext>
               </mml:mstyle>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <span class="vspace" style="height:0.15in"></span><span class="QQQSansSerifItalic16">where</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>c</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo form="infix">&Element;</mml:mo>
           <mml:mi fontstyle="upright">&Ropf;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   <span class="QQQSansSerifItalic16">Each of these solutions is defined at for
   all</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&Element;</mml:mo>
       <mml:mi fontstyle="upright">&Ropf;</mml:mi>
     </mml:mrow>
   </mml:math>.
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
