               </mml:mstyle>
               <mml:mn>1</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>4</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="right">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>4</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>8</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis" columnalign="right">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>4</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>8</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and</mml:mtext>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&lambda;</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">v</mml:mtext>
                   </mml:mstyle>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis" columnalign="right">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>4</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>8</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis" columnalign="right">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>4</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mn>8</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mo form="infix">&Implies;</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>1</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">v</mml:mtext>
             </mml:mstyle>
             <mml:mn>1</mml:mn>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;is an eigenvector</mml:mtext>
     </mml:mrow>
   </mml:math>Also
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle fontweight="bold">
         <mml:mtext mathcolor="black">A</mml:mtext>
       </mml:mstyle>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:msub>
               <mml:mi>c</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:msub>
               <mml:mstyle fontweight="bold">
                 <mml:mtext mathcolor="black">v</mml:mtext>
               </mml:mstyle>
               <mml:mn>2</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>4</mml:mn>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>0.5</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
           <mml:mtable align="axis">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>1.5</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>3</mml:mn>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and</mml:mtext>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:msub>
               <mml:mi>&lambda;</mml:mi>
               <mml:mn>1</mml:mn>
             </mml:msub>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>c</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">v</mml:mtext>
                   </mml:mstyle>
                   <mml:mn>2</mml:mn>
                 </mml:msub>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mn>3</mml:mn>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
               <mml:mtable align="axis">
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>0.5</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
                 <mml:mtr>
                   <mml:mtd>
                     <mml:mn>1</mml:mn>
                   </mml:mtd>
                 </mml:mtr>
               </mml:mtable>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
             <mml:mtable align="axis">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>1.5</mml:mn>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mn>3</mml:mn>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mo form="infix">&Implies;</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mrow>
           <mml:msub>
             <mml:mi>c</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msub>
             <mml:mstyle fontweight="bold">
               <mml:mtext mathcolor="black">v</mml:mtext>
             </mml:mstyle>
             <mml:mn>2</mml:mn>
           </mml:msub>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;is an eigenvector</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>The vectors
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   determine the lines
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   in the plane as depicted below in Fig. 24.1. Thus the scalar multiples of
   <strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   (<strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>)
   lie on
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   (<mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>)
   as suggested by the calculations above of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math><strong>v</strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mspace width="veryverythinmathspace" />
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   More significant is the fact that
   <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>c</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">v</mml:mtext>
           </mml:mstyle>
           <mml:mn>1</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   lies on
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <strong>A</strong><mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>c</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msub>
           <mml:mstyle fontweight="bold">
             <mml:mtext mathcolor="black">v</mml:mtext>
           </mml:mstyle>
           <mml:mn>2</mml:mn>
         </mml:msub>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   lies on
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   In other words, the matrix <strong>A </strong>leaves the
   lines<strong> </strong><mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>L</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   invariant.
</p>
