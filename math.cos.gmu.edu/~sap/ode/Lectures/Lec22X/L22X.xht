      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 22.23: Direction field
            and Euler approximate solutions to
            <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mtext>,</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>7</mml:mn>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mn>4</mml:mn>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mn>4</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>,</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>y</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>6</mml:mn>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
            when
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>h</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0.2</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mn>0.1</mml:mn>
     </mml:mrow>
   </mml:math></span>
         </td>
      </tr></tbody>
</table></span><br/>
