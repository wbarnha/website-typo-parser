             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>y</mml:mi>
