           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:msub>
           <mml:mi>t</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msub>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>,
   where
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>t</mml:mi>
           <mml:mn>1</mml:mn>
         </mml:msub>
         <mml:mo form="infix">+</mml:mo>
         <mml:mi>h</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>:
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mi>x</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>x</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mi>h</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msub>
                   <mml:mi>y</mml:mi>
                   <mml:mn>1</mml:mn>
                 </mml:msub>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:msub>
               <mml:mi>y</mml:mi>
               <mml:mn>2</mml:mn>
             </mml:msub>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>y</mml:mi>
                 <mml:mn>1</mml:mn>
               </mml:msub>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi>h</mml:mi>
                 <mml:mo>&ApplyFunction;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>q</mml:mi>
                         <mml:mn>0</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msub>
                         <mml:mi>x</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>p</mml:mi>
                         <mml:mn>0</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msub>
                         <mml:mi>y</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   <mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(22.18)</mtext></mtd>

       <mml:mtable frame="solid" framespacing="0.8em 1.0ex">
         <mml:mtr>
           <mml:mtd>
             <mml:mtable align="axis" columnalign="left left left">
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mstyle fontweight="bold">
                     <mml:mtext mathcolor="black">EULER'S METHOD</mml:mtext>
                   </mml:mstyle>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>t</mml:mi>
                     <mml:mrow>
                       <mml:mi>k</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>t</mml:mi>
                       <mml:mi>k</mml:mi>
                     </mml:msub>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mi>h</mml:mi>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>x</mml:mi>
                     <mml:mrow>
                       <mml:mi>k</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mi>k</mml:mi>
                     </mml:msub>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi>h</mml:mi>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:msub>
                         <mml:mi>y</mml:mi>
                         <mml:mi>k</mml:mi>
                       </mml:msub>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:msub>
                     <mml:mi>y</mml:mi>
                     <mml:mrow>
                       <mml:mi>k</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mn>1</mml:mn>
                     </mml:mrow>
                   </mml:msub>
                   <mml:mo form="infix">=</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>y</mml:mi>
                       <mml:mi>k</mml:mi>
                     </mml:msub>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:mi>h</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:msub>
                               <mml:mi>q</mml:mi>
                               <mml:mn>0</mml:mn>
                             </mml:msub>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:msub>
                               <mml:mi>x</mml:mi>
                               <mml:mi>k</mml:mi>
                             </mml:msub>
                           </mml:mrow>
                           <mml:mo form="infix">+</mml:mo>
                           <mml:mrow>
                             <mml:msub>
                               <mml:mi>p</mml:mi>
                               <mml:mn>0</mml:mn>
                             </mml:msub>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:msub>
                               <mml:mi>y</mml:mi>
                               <mml:mi>k</mml:mi>
                             </mml:msub>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">for&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi>k</mml:mi>
                       <mml:mo form="infix">=</mml:mo>
                       <mml:mn>0</mml:mn>
                     </mml:mrow>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mn>1</mml:mn>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mn>2</mml:mn>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mi>&hellip;</mml:mi>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mspace width="veryverythinmathspace" />
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>

		  </mlabeledtr>
		</mtable>
		
   </mml:math>
</p>
<p>
   With another interact, <span class="QQQNote-BodyText"><a target="_blank" href="L22X__note6.xht"> <span class="QQQNote-BodyText">DFE</span></a></span>
    (DirectionFieldEuler) you can
   plot an Euler approximate solution for any ODE, create a corresponding
   direction field and for any choice of initial values within the direction
   field by a mouse click or keyboard input.
   <span class="QQQNote-BodyText"> <a target="_blank" href="L22X__note7.xht"><span class="QQQNote-BodyText">(See
   DFE instructions for use.)</span></a> </span> Like DF, the interact allows
   for a system of first-order ODEs more general than
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(22.15)"> <span class="QQQHyperLink-BodyText">Eqn.
   (22.15)</span></a></span>, namely,
</p>
<p>
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mover accent="true">
               <mml:mi>y</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi>g</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>x</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>y</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>where
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>g</mml:mi>
     </mml:mrow>
   </mml:math>
   are smooth (continuously differentiable functions of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>)
   throughout the plane. The next two examples use DFE to plot the direction
   fields and Euler approximations of the
   <span class="QQQHyperLink-BodyText"><a href="#Example22.09"> <span class="QQQHyperLink-BodyText">Example
   22.9</span></a></span> and
   <span class="QQQHyperLink-BodyText"><a href="#Example22.10"> <span class="QQQHyperLink-BodyText">Example
   22.10</span></a></span>.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      22.16
   </p>
</div>
<p class="QQQExampleText">
   Use <span class="QQQHyperLink-BodyText">DFE</span> to plot an Euler
   approximate solution to the
   IVP<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&die;</mml:mo>
           </mml:mover>
           <mml:mo form="infix">+</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mover accent="true">
             <mml:mi>x</mml:mi>
             <mml:mo form="postfix" accent="true">&dot;</mml:mo>
           </mml:mover>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>in
   the
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>-window
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         </mml:mrow>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mn>2</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         <mml:mi>y</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
       </mml:mrow>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
   on the interval
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Use stepsizes
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>h</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0.5</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0.25</mml:mn>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0.1</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0.01</mml:mn>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:<span style="width:1em"><span style="padding-left:1em"></span></span></span>Transform
   the ODE to the
   system<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mtable align="axis" columnalign="left left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
             <mml:mtext>,</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mover accent="true">
                 <mml:mi>y</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>x</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>,</mml:mtext>
           </mml:mtd>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>y</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>with the specified initial condition. We display the Euler approximate
   solutions in Figure 22.22. Note how as
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>h</mml:mi>
     </mml:mrow>
   </mml:math>
   decreases from
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0.01</mml:mn>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>the
   curves approach a circle through
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">,</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   This is to be expected as the actual orbit through
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">,</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   is defined by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:msup>
           <mml:mi>y</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>See
   <span class="QQQHyperLink-BodyText"><a href="#Example22.03"> <span class="QQQHyperLink-BodyText">Example
   22.3</span></a></span> and
   <span class="QQQHyperLink-BodyText"><a href="#Figure22.10"> <span class="QQQHyperLink-BodyText">Fig.
   22.10</span></a></span>.
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQCenteredExampleText">
   
<!-- MSISEMCOM /FRAME{ftbpFU}{5.278in}{7.1114in}{0pt}{}{}{fig.22.22x.png}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 5.278in;  height 7.1114in;  depth 0pt;  original-width 5.0004in;  original-height 6.7499in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "1H";  file-properties "XNPEU"; }} -->
