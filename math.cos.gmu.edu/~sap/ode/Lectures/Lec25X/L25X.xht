   </mml:math>has
   eigen-pairs<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>&lambda;</mml:mi>
