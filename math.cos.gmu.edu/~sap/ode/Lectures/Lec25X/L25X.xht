   </mml:math>
</p>
<p>
   <br/><a id="Figure25.03"></a>
</p>
<p class="QQQCenteredBodyText">
   
<!-- MSISEMCOM /FRAME{dtbpFU}{4.5169in}{4.4434in}{0pt}{}{}{figure25.03_v2.wmf}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 4.5169in;  height 4.4434in;  depth 0pt;  original-width 2.9931in;  original-height 2.9447in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "5";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L25X__3.png" alt="figure25.03_v2.wmf" height="427px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 25.3: Some orbits of
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mover accent="true">
         <mml:mi>y</mml:mi>
         <mml:mo form="postfix" accent="true">&dot;</mml:mo>
       </mml:mover>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math></span>
         </td>
      </tr></tbody>
</table></span><br/>
