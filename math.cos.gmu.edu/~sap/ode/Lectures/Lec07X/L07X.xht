           <mml:mi>t</mml:mi>
         </mml:msubsup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>3</mml:mn>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:msub>
               <mml:mi>t</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>&tau;</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>x</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                     <mml:mo>&ApplyFunction;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:mn>2</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>&tau;</mml:mi>
                     <mml:mn>2</mml:mn>
                   </mml:msup>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:msubsup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true"></mml:mo>
             <mml:mrow>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>&tau;</mml:mi>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>2</mml:mn>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>&tau;</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mn>1</mml:mn>
                           <mml:mn>6</mml:mn>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>&tau;</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
           </mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>2</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>6</mml:mn>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   By induction one can show
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>n</mml:mi>
         </mml:msub>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo form="postfix">!</mml:mo>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mrow>
                   <mml:mn>3</mml:mn>
                   <mml:mo form="postfix">!</mml:mo>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mi>&ctdot;</mml:mi>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mn>1</mml:mn>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
             </mml:mrow>
             <mml:mi>n</mml:mi>
           </mml:msup>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mi>n</mml:mi>
                 </mml:msup>
                 <mml:mrow>
                   <mml:mi>n</mml:mi>
                   <mml:mo form="postfix">!</mml:mo>
                 </mml:mrow>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   You should recognize this approximation as the Taylor polynomial of degree
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
   for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:msup>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Hence we must have
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:munder>
           <mml:mo mathcolor="gray" form="prefix" movablelimits="true">lim</mml:mo>
           <mml:mrow>
             <mml:mi>n</mml:mi>
             <mml:mo form="infix">&rarr;</mml:mo>
             <mml:mi>&infin;</mml:mi>
           </mml:mrow>
         </mml:munder>
         <mml:mrow>
           <mml:msub>
             <mml:mi>x</mml:mi>
             <mml:mi>n</mml:mi>
           </mml:msub>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>t</mml:mi>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 7.12
   </p>
</div>
