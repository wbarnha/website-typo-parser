         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
            with stepsize
            <mml:math display="inline">
     <mml:mrow>
       <mml:mi>h</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.5</mml:mn>
