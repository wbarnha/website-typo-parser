                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mn>0</mml:mn>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mrow>
                             <mml:mn>2</mml:mn>
                             <mml:mo form="postfix">!</mml:mo>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mrow>
                             <mml:mover accent="true">
                               <mml:mi>x</mml:mi>
                               <mml:mo form="postfix" accent="true">&tdot;</mml:mo>
                             </mml:mover>
                             <mml:mo>&ApplyFunction;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mn>0</mml:mn>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mrow>
                             <mml:mn>3</mml:mn>
                             <mml:mo form="postfix">!</mml:mo>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>3</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mstyle displaystyle="true" scriptlevel="0">
                         <mml:mfrac>
                           <mml:mrow>
                             <mml:msup>
                               <mml:mi>x</mml:mi>
                               <mml:mrow>
                                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                 <mml:mn>4</mml:mn>
                                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                               </mml:mrow>
                             </mml:msup>
                             <mml:mo>&ApplyFunction;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mn>0</mml:mn>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mrow>
                             <mml:mn>4</mml:mn>
                             <mml:mo form="postfix">!</mml:mo>
                           </mml:mrow>
                         </mml:mfrac>
                       </mml:mstyle>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>t</mml:mi>
                       <mml:mn>4</mml:mn>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">+</mml:mo>
                   <mml:mi>&ctdot;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mn>1</mml:mn>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>3</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:msup>
                   <mml:mi>t</mml:mi>
                   <mml:mn>4</mml:mn>
                 </mml:msup>
                 <mml:mo form="infix">+</mml:mo>
                 <mml:mi>&ctdot;</mml:mi>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Note that we can truncate the Taylor series after a finite number of terms to
   obtain an approximate solution. For instance we can approximate the solution
   with a degree 2 Taylor polynomial
   <mml:math mode="display" display="block">
 
    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(7.5)</mtext></mtd>

       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>

   </mml:math>or
   a degree 5 Taylor
   polynomial<mml:math mode="display" display="block">
    
    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(7.6)</mtext></mtd>

       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mn>1</mml:mn>
           <mml:mo form="infix">+</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>3</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>4</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>5</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>

   </mml:math>to
   obtain polynomial approximations to the solution.
</p>
<p class="center">
   <br/><span class="large2">It must be noted that these approximations are valid
   only when the value of
   </span><mml:math display="inline">
     <mml:mrow>
       <mml:mstyle mathsize="2.65ex">
         <mml:mi>t</mml:mi>
       </mml:mstyle>
     </mml:mrow>
   </mml:math><span class="large2"> is
   near zero.</span>
</p>
<p class="QQQExampleText">
   <br/>We recognize this as a geometric series that converges to the
   solution<mml:math mode="display" display="block">
 
    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(7.7)</mtext></mtd>

       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">=</mml:mo>
           <mml:mfrac>
             <mml:mn>1</mml:mn>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>t</mml:mi>
             </mml:mrow>
           </mml:mfrac>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mrow>
           <mml:mo form="infix">&lt;</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">&lt;</mml:mo>
           <mml:mn>1</mml:mn>
         </mml:mrow>
       </mml:mrow>

  	  </mlabeledtr>
	</mtable>

   </mml:math>
</p>
<p class="QQQExampleText">
   Figure 7.11 displays graphs of the approximations Eqn. 7.5
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mi>n</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>2</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
   and Eqn. (7.6)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
       <mml:mrow>
         <mml:mi>n</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>5</mml:mn>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
     </mml:mrow>
   </mml:math>
   along with the actual solution, Eqn. (7.7).
</p>
<p class="center">
   
<!-- MSISEMCOM \FRAME{dtbpFU}{4.5844in}{3.8614in}{0pt}{}{}{example07.09.png}{\special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 4.5844in;  height 3.8614in;  depth 0pt;  original-width 3.7602in;  original-height 4.1165in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "T";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><table>
      <tbody>
      <tr>
         <img src="Images/L07XX__15.png" alt="example07.09.png" height="450px" />
      </tr>
      <tr>
         <td class="graphicCaption">
            <span style="display: block; text-align: center">Figure 7.11: Taylor
            polynomial approximations to
            <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math></span>
         </td>
      </tr></tbody>
</table></span><br/>
<!-- MSISEMCOM ENDFRAME -->
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 7.9
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   The IVP
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   can be solved directly; the solution is indeed,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">/</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mrow>
             <mml:mn>1</mml:mn>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mi>t</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   with maximal interval of definition
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
   (Verify this by solving the IVP.) That the maximal interval of definition is
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   does not contradict the fact that the Taylor series representation of the
   solution just converges on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
   Indeed, a Taylor series expansion of a solution
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   typically has a smaller interval of convergence than the maximal interval of
   definition of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/>
</p>
<p>
   Not all solutions to IVPs yield such a nice Taylor series. <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      7.10<a id="Example07.10"></a>
   </p>
</div>
<p class="QQQExampleText">
   <br/>We demonstrate how to derive a Taylor series solution to the
   IVP<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>t</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">,</mml:mo>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>The
   initial time
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   implies the Taylor series for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   be centered at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math><mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mn>0</mml:mn>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
