                       </mml:msup>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:msup>
                     <mml:mi>e</mml:mi>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:msup>
                 </mml:mtd>
               </mml:mtr>
               <mml:mtr>
                 <mml:mtd>
                   <mml:mtext mathcolor="black">(iii)</mml:mtext>
                 </mml:mtd>
                 <mml:mtd columnalign="left">
                   <mml:mrow>
                     <mml:msub>
                       <mml:mi>&lambda;</mml:mi>
                       <mml:mn>1</mml:mn>
                     </mml:msub>
                     <mml:mo form="infix">=</mml:mo>
                     <mml:msub>
                       <mml:mover accent="true">
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mo form="postfix" accent="true">&macr;</mml:mo>
                       </mml:mover>
                       <mml:mn>2</mml:mn>
                     </mml:msub>
                     <mml:mo form="infix">=</mml:mo>
                     <mml:mrow>
                       <mml:mi>&alpha;</mml:mi>
                       <mml:mo form="infix">+</mml:mo>
                       <mml:mrow>
                         <mml:mi>i</mml:mi>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>&beta;</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mrow>
                     <mml:mi>&beta;</mml:mi>
                     <mml:mo form="infix">&ne;</mml:mo>
                     <mml:mn>0</mml:mn>
                   </mml:mrow>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                   <mml:mi>&alpha;</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mrow>
                     <mml:mi>&beta;</mml:mi>
                     <mml:mo form="infix">&Element;</mml:mo>
                     <mml:mi fontstyle="upright">&Ropf;</mml:mi>
                   </mml:mrow>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mi>&alpha;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mi mathcolor="gray">cos</mml:mi>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mi>&beta;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">,</mml:mo>
                     <mml:mtext mathcolor="black">&ensp;</mml:mtext>
                     <mml:mrow>
                       <mml:msup>
                         <mml:mi>e</mml:mi>
                         <mml:mrow>
                           <mml:mi>&alpha;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:msup>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mi mathcolor="gray">sin</mml:mi>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:mi>&beta;</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
                 </mml:mtd>
                 <mml:mtd>
                   <mml:mstyle displaystyle="true" scriptlevel="0">
                     <mml:mfrac>
                       <mml:mrow>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mi>&alpha;</mml:mi>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:msup>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mspace width="thinmathspace" />
                           <mml:mrow>
                             <mml:mi>&beta;</mml:mi>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mi>&beta;</mml:mi>
                     </mml:mfrac>
                   </mml:mstyle>
                 </mml:mtd>
               </mml:mtr>
             </mml:mtable>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>

<p>
   The value of using the Green's kernel to determine a particular solution is
   evident when
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(16.03)"> <span class="QQQHyperLink-BodyText">integrals that define the coefficients </span></a></span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>1</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>c</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   cannot be computed in terms of elementary functions.
</p>
<p>
   <br/><hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      16.17<a id="Example16.17"></a>
   </p>
</div>
<p class="QQQExampleText">
   <br/>Use Green's kernel to determine a particular solution for
   ODE<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mn>4</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mn>1</mml:mn>
             <mml:msqrt>
               <mml:mi>t</mml:mi>
             </mml:msqrt>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>We
   computed the Green's kernel for
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&die;</mml:mo>
         </mml:mover>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mn>4</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   in Example
   16.15:<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>G</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mi mathcolor="gray">sin</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mspace width="thinmathspace" />
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mn>2</mml:mn>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Observe
   that
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>1</mml:mn>
         <mml:mo form="infix">/</mml:mo>
         <mml:msqrt>
           <mml:mi>t</mml:mi>
         </mml:msqrt>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   is only defined and continuous on
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>I</mml:mi>
         <mml:mo form="infix">:</mml:mo>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mo form="infix">&lt;</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="infix">&lt;</mml:mo>
           <mml:mi>&infin;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Choose
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   (as we need positive value for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Then use
   <span class="QQQHyperLink-BodyText"><a href="#Eqn.(16.09)"> <span class="QQQHyperLink-BodyText">Eqn.
   (16.09)</span></a> </span>to
   obtain<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mstyle displaystyle="true">
         <mml:mtable>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>x</mml:mi>
                   <mml:mi>p</mml:mi>
                 </mml:msub>
                 <mml:maligngroup />
                 <mml:mo form="infix">=</mml:mo>
                 <mml:maligngroup />
                 <mml:mrow>
                   <mml:msubsup>
                     <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                     <mml:msub>
                       <mml:mi>t</mml:mi>
                       <mml:mn>0</mml:mn>
                     </mml:msub>
                     <mml:mi>t</mml:mi>
                   </mml:msubsup>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mi>G</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>t</mml:mi>
                           <mml:mo form="infix">,</mml:mo>
                           <mml:mi>&tau;</mml:mi>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mspace width="thinmathspace" />
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mi>f</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mi>&tau;</mml:mi>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>d</mml:mi>
                     <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                   <mml:mn>1</mml:mn>
                   <mml:mi>t</mml:mi>
                 </mml:msubsup>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mspace width="thinmathspace" />
                           <mml:mrow>
                             <mml:mn>2</mml:mn>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:mn>2</mml:mn>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mn>1</mml:mn>
                         <mml:msqrt>
                           <mml:mi>&tau;</mml:mi>
                         </mml:msqrt>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:maligngroup />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd groupalign="right center left">
               <mml:maligngroup />
               <mml:maligngroup />
               <mml:mo form="infix">=</mml:mo>
               <mml:maligngroup />
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mn>1</mml:mn>
                     <mml:mn>2</mml:mn>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:msubsup>
                   <mml:mo form="prefix" largeop="true">&int;</mml:mo>
                   <mml:mn>1</mml:mn>
                   <mml:mi>t</mml:mi>
                 </mml:msubsup>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mrow>
                           <mml:mi mathcolor="gray">sin</mml:mi>
                           <mml:mo>&ApplyFunction;</mml:mo>
                           <mml:mspace width="thinmathspace" />
                           <mml:mrow>
                             <mml:mn>2</mml:mn>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mi>&tau;</mml:mi>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:mrow>
                         <mml:msqrt>
                           <mml:mi>&tau;</mml:mi>
                         </mml:msqrt>
                       </mml:mfrac>
                     </mml:mstyle>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>d</mml:mi>
                   <mml:mtext>&ZeroWidthSpace;</mml:mtext>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
       </mml:mstyle>
     </mml:mrow>
   </mml:math>Since
   the last integral cannot be evaluated symbolically any further, we must stop
   at this point. Thus a particular solution
   is<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mstyle displaystyle="true" scriptlevel="0">
             <mml:mfrac>
               <mml:mn>1</mml:mn>
               <mml:mn>2</mml:mn>
             </mml:mfrac>
           </mml:mstyle>
         </mml:mrow>
       </mml:mrow>
       <mml:mo>&InvisibleTimes;</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>1</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mrow>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mspace width="thinmathspace" />
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mi>t</mml:mi>
                         <mml:mo form="infix">&minus;</mml:mo>
                         <mml:mi>&tau;</mml:mi>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:msqrt>
                   <mml:mi>&tau;</mml:mi>
                 </mml:msqrt>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>An appropriate numerical procedure may be applied to compute an
   approximate value of
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   for values of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<div class="conclusion">
   <span class="leadin">End</span><p class="conclusion-FirstPara">
      of Example 16.17
   </p>
</div>
<p class="center">
   <hr class="hrulefill" />
</p>
<p>
   <br/>
</p>
<p>
   <br/>
</p>
<p>
   The Green's kernel is useful to calculate a particular solution when the
   forcing function
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   is defined piecewise. Observe that the integral form of the particular
   solution<mml:math mode="display" display="block">

    <mtable side='left' displaystyle="true">
     <mlabeledtr>
      <mtd><mtext>(16.10)</mtext></mtd>

       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:msub>
               <mml:mi>t</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>resembles
   the integral formula solution
   <span class="QQQHyperLink-BodyText"><a target="_blank" href="../Lec06X/L06X.xht#IntegralFormulaSolution"> <span class="QQQHyperLink-BodyText">Eqn.
   (6.6)</span></a> f</span>or a first-order linear IVP.
</p>
<p>
   <br/>
</p>
<p>
   <hr class="hrulefill" />
</p>
<div class="example">
   <span class="leadin">Example</span><p class="example-FirstPara">
      16.18:
   </p>
</div>
<p class="QQQExampleText">
   <br/>Use Green's kernel to determine a particular solution for
   ODE<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&die;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>b</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mover accent="true">
                 <mml:mi>x</mml:mi>
                 <mml:mo form="postfix" accent="true">&dot;</mml:mo>
               </mml:mover>
             </mml:mrow>
             <mml:mo form="infix">+</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>b</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext>&emsp;</mml:mtext>
         <mml:mtext mathcolor="black">where&emsp;</mml:mtext>
       </mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>f</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
           <mml:mtable align="axis" columnalign="center left left">
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mtext>,</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mspace width="veryverythinmathspace" />
               </mml:mtd>
               <mml:mtd>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>&infin;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mn>0</mml:mn>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:msub>
                   <mml:mi>f</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mtext>,</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mspace width="veryverythinmathspace" />
               </mml:mtd>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>T</mml:mi>
               </mml:mtd>
             </mml:mtr>
             <mml:mtr>
               <mml:mtd>
                 <mml:mn>0</mml:mn>
                 <mml:mtext>,</mml:mtext>
               </mml:mtd>
               <mml:mtd>
                 <mml:mspace width="veryverythinmathspace" />
               </mml:mtd>
               <mml:mtd>
                 <mml:mi>T</mml:mi>
                 <mml:mo form="infix">&le;</mml:mo>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&lt;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mtd>
             </mml:mtr>
           </mml:mtable>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext>&emsp;</mml:mtext>
       <mml:mtext mathcolor="black">and&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mi>b</mml:mi>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext mathcolor="black">&ensp;</mml:mtext>
         <mml:msub>
           <mml:mi>f</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">,</mml:mo>
         <mml:mtext mathcolor="black">&ensp;and&ensp;</mml:mtext>
       </mml:mrow>
       <mml:mi>T</mml:mi>
       <mml:mtext mathcolor="black">&ensp;are positive constants&ensp;</mml:mtext>
     </mml:mrow>
   </mml:math>The
   forcing function
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
     </mml:mrow>
   </mml:math>
   is essentially a unit pulse of length
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>
   and scaled by
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>f</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   as depicted
   below
<!-- MSISEMCOM /FRAME{ftbpF}{4.5826in}{1.9631in}{0pt}{}{}{t-pulse.png}{/special{ language "Scientific Word";  type "GRAPHIC";  maintain-aspect-ratio TRUE;  display "USEDEF";  valid_file "F";  width 4.5826in;  height 1.9631in;  depth 0pt;  original-width 3.0364in;  original-height 1.2903in;  cropleft "0";  croptop "1";  cropright "1";  cropbottom "0";  file_alias "3";  file-properties "XNPEU"; }} -->
   <br/><span class="displayed"><img src="Images/L16X__2.png" alt="t-pulse.png" height="188px" /></span><br/>
<!-- MSISEMCOM ENDFRAME -->
Finally
   assume the system is at rest at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   so that we may choose the initial conditions to be
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>,
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
         </mml:mover>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution:</span><strong><span style="width:1em"><span style="padding-left:1em"></span></span></strong>We
   turn to
   <span class="QQQHyperLink-BodyText"><a href="#Table16.01"> <span class="QQQHyperLink-BodyText">Table
   16.1</span></a></span> to determine the Green's kernel for the ODE. This
   requires that we calculate the characteristic roots of the polynomial
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>&lambda;</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>b</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&lambda;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:msup>
           <mml:mi>b</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>The
   polynomial readily factors as
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>&lambda;</mml:mi>
             <mml:mo form="infix">+</mml:mo>
             <mml:mi>b</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   which gives rise to the real repeated root
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>&lambda;</mml:mi>
         <mml:mo form="infix">=</mml:mo>
         <mml:mi>b</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Consequently Case (ii) of
   <span class="QQQHyperLink-BodyText"><a href="#Table16.01"> <span class="QQQHyperLink-BodyText">Table
   16.1</span></a></span> applies so that
</p>
<p class="QQQExampleText">
   <br/>
</p>
<p class="QQQExampleText">
   <mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>G</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">,</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mi>b</mml:mi>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
         </mml:msup>
       </mml:mrow>
     </mml:mrow>
   </mml:math>where
   we have set
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   It follows
   that<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>G</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>Because of the piecewise nature of
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>f</mml:mi>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   in order to evaluate the integral for
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   we must consider three cases: (a)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
   (b)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
       <mml:mo form="infix">&le;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>
   and (c)
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>T</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   We can ignore
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">&lt;</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   because the structure is assumed at rest prior to
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0.</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Case (a) </span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mi>&infin;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mn>0</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math><span style="width:1em"><span style="padding-left:1em"></span></span>Accordingly
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>,
   we
   have<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mrow>
           <mml:mrow>
             <mml:mi>G</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">,</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mspace width="thinmathspace" />
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi>f</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mi>&tau;</mml:mi>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:msubsup>
           <mml:mo form="prefix" largeop="true">&int;</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mi>t</mml:mi>
         </mml:msubsup>
         <mml:mrow>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mi>&tau;</mml:mi>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:msup>
               <mml:mi>e</mml:mi>
               <mml:mrow>
                 <mml:mrow>
                   <mml:mo form="prefix">--</mml:mo>
                   <mml:mi>b</mml:mi>
                 </mml:mrow>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                   <mml:mrow>
                     <mml:mi>t</mml:mi>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mi>&tau;</mml:mi>
                   </mml:mrow>
                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:msup>
             <mml:mo form="infix">&sdot;</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mrow>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Case (b) </span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>0</mml:mn>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>T</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math><span style="width:1em"><span style="padding-left:1em"></span></span>Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>b</mml:mi>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>f</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mn>1</mml:mn>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mi>b</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>b</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:msup>
                   <mml:mi>b</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Case (c) </span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>T</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math><span style="width:1em"><span style="padding-left:1em"></span></span>Then<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:mrow>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mi>p</mml:mi>
         </mml:msub>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mrow>
               <mml:mi>G</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mrow>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="infix">,</mml:mo>
                   <mml:mi>&tau;</mml:mi>
                 </mml:mrow>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mspace width="thinmathspace" />
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:mi>f</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                 <mml:mi>&tau;</mml:mi>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
               </mml:mrow>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>d</mml:mi>
             <mml:mtext>&ZeroWidthSpace;</mml:mtext>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>&tau;</mml:mi>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mn>0</mml:mn>
             <mml:mi>T</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>b</mml:mi>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:msubsup>
             <mml:mo form="prefix" largeop="true">&int;</mml:mo>
             <mml:mi>T</mml:mi>
             <mml:mi>t</mml:mi>
           </mml:msubsup>
           <mml:mrow>
             <mml:mrow>
               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mi>&tau;</mml:mi>
               </mml:mrow>
               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
             </mml:mrow>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mrow>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>b</mml:mi>
                   </mml:mrow>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mrow>
                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                     <mml:mrow>
                       <mml:mi>t</mml:mi>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mi>&tau;</mml:mi>
                     </mml:mrow>
                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                   </mml:mrow>
                 </mml:mrow>
               </mml:msup>
               <mml:mo form="infix">&sdot;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mtext mathcolor="black">&ensp;</mml:mtext>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>d</mml:mi>
           <mml:mtext>&ZeroWidthSpace;</mml:mtext>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&tau;</mml:mi>
         </mml:mrow>
         <mml:mo form="infix">=</mml:mo>
         <mml:mrow>
           <mml:msub>
             <mml:mi>f</mml:mi>
             <mml:mn>0</mml:mn>
           </mml:msub>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mstyle displaystyle="true" scriptlevel="0">
               <mml:mfrac>
                 <mml:mrow>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mi>b</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                             <mml:mrow>
                               <mml:mi>t</mml:mi>
                               <mml:mo form="infix">&minus;</mml:mo>
                               <mml:mi>T</mml:mi>
                             </mml:mrow>
                             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                           </mml:mrow>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>b</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mi>t</mml:mi>
                             <mml:mo form="infix">&minus;</mml:mo>
                             <mml:mi>T</mml:mi>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                   <mml:mo form="infix">&minus;</mml:mo>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                       <mml:mrow>
                         <mml:mn>1</mml:mn>
                         <mml:mo form="infix">+</mml:mo>
                         <mml:mrow>
                           <mml:mi>b</mml:mi>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                     </mml:mrow>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:msup>
                       <mml:mi>e</mml:mi>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix">&minus;</mml:mo>
                           <mml:mi>b</mml:mi>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>t</mml:mi>
                       </mml:mrow>
                     </mml:msup>
                   </mml:mrow>
                 </mml:mrow>
                 <mml:msup>
                   <mml:mi>b</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mfrac>
             </mml:mstyle>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>where in cases (b) and (c) the evaluation of the integrals was done by
   Mathematica. Thus, the piecewise particular solution is given
   by<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
         <mml:mtable align="axis" columnalign="center center left">
           <mml:mtr>
             <mml:mtd>
               <mml:mn>0</mml:mn>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mrow>
                 <mml:mo form="prefix">&minus;</mml:mo>
                 <mml:mi>&infin;</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mn>0</mml:mn>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mrow>
                       <mml:mn>1</mml:mn>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:mrow>
                               <mml:mi>b</mml:mi>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>b</mml:mi>
                             </mml:mrow>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:msup>
                       <mml:mi>b</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mn>0</mml:mn>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>T</mml:mi>
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
             <mml:mtd>
               <mml:mspace width="veryverythinmathspace" />
             </mml:mtd>
           </mml:mtr>
           <mml:mtr>
             <mml:mtd>
               <mml:msub>
                 <mml:mi>f</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mstyle displaystyle="true" scriptlevel="0">
                   <mml:mfrac>
                     <mml:mrow>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:mrow>
                               <mml:mi>b</mml:mi>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mrow>
                                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                 <mml:mrow>
                                   <mml:mi>t</mml:mi>
                                   <mml:mo form="infix">&minus;</mml:mo>
                                   <mml:mi>T</mml:mi>
                                 </mml:mrow>
                                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                               </mml:mrow>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>b</mml:mi>
                             </mml:mrow>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mrow>
                               <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                               <mml:mrow>
                                 <mml:mi>t</mml:mi>
                                 <mml:mo form="infix">&minus;</mml:mo>
                                 <mml:mi>T</mml:mi>
                               </mml:mrow>
                               <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                             </mml:mrow>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                       <mml:mo form="infix">&minus;</mml:mo>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                           <mml:mrow>
                             <mml:mn>1</mml:mn>
                             <mml:mo form="infix">+</mml:mo>
                             <mml:mrow>
                               <mml:mi>b</mml:mi>
                               <mml:mo>&InvisibleTimes;</mml:mo>
                               <mml:mi>t</mml:mi>
                             </mml:mrow>
                           </mml:mrow>
                           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                         </mml:mrow>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:msup>
                           <mml:mi>e</mml:mi>
                           <mml:mrow>
                             <mml:mrow>
                               <mml:mo form="prefix">&minus;</mml:mo>
                               <mml:mi>b</mml:mi>
                             </mml:mrow>
                             <mml:mo>&InvisibleTimes;</mml:mo>
                             <mml:mi>t</mml:mi>
                           </mml:mrow>
                         </mml:msup>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:msup>
                       <mml:mi>b</mml:mi>
                       <mml:mn>2</mml:mn>
                     </mml:msup>
                   </mml:mfrac>
                 </mml:mstyle>
               </mml:mrow>
             </mml:mtd>
             <mml:mtd>
               <mml:mtext mathcolor="black">if</mml:mtext>
             </mml:mtd>
             <mml:mtd>
               <mml:mi>T</mml:mi>
               <mml:mo form="infix">&le;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&lt;</mml:mo>
               <mml:mi>&infin;</mml:mi>
             </mml:mtd>
           </mml:mtr>
         </mml:mtable>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true"></mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   <br/>The following interact of Fig. 16.2 shows how the output is affected by
   changes in the damping factor
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>b</mml:mi>
     </mml:mrow>
   </mml:math>
   and the duration of the pulse
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>The
   pulse height
   <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>f</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
   is set to have value
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1.</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
