                                   </mml:msub>
                                 </mml:mrow>
                               </mml:mfrac>
                             </mml:mstyle>
                           </mml:mrow>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mrow>
                             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                             <mml:mrow>
                               <mml:msup>
                                 <mml:mi>e</mml:mi>
                                 <mml:mrow>
                                   <mml:msub>
                                     <mml:mi>&lambda;</mml:mi>
                                     <mml:mn>1</mml:mn>
                                   </mml:msub>
                                   <mml:mo>&InvisibleTimes;</mml:mo>
                                   <mml:mrow>
                                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                     <mml:mrow>
                                       <mml:mi>t</mml:mi>
                                       <mml:mo form="infix">&minus;</mml:mo>
