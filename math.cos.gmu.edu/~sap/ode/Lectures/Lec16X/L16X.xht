                         </mml:msub>
                         <mml:mi>t</mml:mi>
                       </mml:msubsup>
                       <mml:mrow>
                         <mml:mrow>
                           <mml:mstyle displaystyle="true" scriptlevel="0">
                             <mml:mfrac>
                               <mml:mrow>
                                 <mml:mrow>
                                   <mml:msub>
                                     <mml:mi>x</mml:mi>
                                     <mml:mn>1</mml:mn>
                                   </mml:msub>
                                   <mml:mo>&ApplyFunction;</mml:mo>
                                   <mml:mrow>
                                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                     <mml:mi>&tau;</mml:mi>
                                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                   </mml:mrow>
                                 </mml:mrow>
                                 <mml:mspace width="thinmathspace" />
                                 <mml:mo>&InvisibleTimes;</mml:mo>
                                 <mml:mrow>
                                   <mml:mi>f</mml:mi>
                                   <mml:mo>&ApplyFunction;</mml:mo>
                                   <mml:mrow>
                                     <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                     <mml:mi>&tau;</mml:mi>
                                     <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                   </mml:mrow>
                                 </mml:mrow>
                               </mml:mrow>
                               <mml:mrow>
                                 <mml:mi>W</mml:mi>
                                 <mml:mo>&ApplyFunction;</mml:mo>
                                 <mml:mrow>
                                   <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                                   <mml:mi>&tau;</mml:mi>
                                   <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                                 </mml:mrow>
                               </mml:mrow>
                             </mml:mfrac>
                           </mml:mstyle>
