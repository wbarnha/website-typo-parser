           </mml:mrow>
           <mml:mo form="infix">/</mml:mo>
           <mml:mn>2</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mrow>
           <mml:mi>&pi;</mml:mi>
           <mml:mo form="infix">/</mml:mo>
           <mml:mn>2.</mml:mn>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
