                 <mml:mrow>
                   <mml:munder>
                     <mml:mo mathcolor="gray" form="prefix" movablelimits="true">lim</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mn>2</mml:mn>
                       </mml:msub>
                       <mml:mo form="infix">&rarr;</mml:mo>
                       <mml:msub>
                         <mml:mi>&lambda;</mml:mi>
                         <mml:mn>1</mml:mn>
                       </mml:msub>
                     </mml:mrow>
                   </mml:munder>
                   <mml:mrow>
                     <mml:mstyle displaystyle="true" scriptlevel="0">
                       <mml:mfrac>
                         <mml:mrow>
                           <mml:mrow>
                             <mml:mstyle displaystyle="true" scriptlevel="0">
                               <mml:mfrac>
                                 <mml:mi>d</mml:mi>
                                 <mml:mrow>
                                   <mml:mi>d</mml:mi>
                                   <mml:mo>&InvisibleTimes;</mml:mo>
                                   <mml:msub>
                                     <mml:mi>&lambda;</mml:mi>
