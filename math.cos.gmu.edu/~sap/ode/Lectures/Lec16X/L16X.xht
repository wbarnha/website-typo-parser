                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>

		  </mlabeledtr>
		</mtable>

   </mml:math>
</p>
<p class="QQQExampleText">
   <br/><span class="QQQSansSerif14">Solution for</span>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>T</mml:mi>
         <mml:mo form="infix">&le;</mml:mo>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>&infin;</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">:</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p class="QQQExampleText">
   Since
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>f</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mi>t</mml:mi>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   when
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&ge;</mml:mo>
         <mml:mi>T</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
   the upper limit of integration in Eqn. (16.13) can be replaced by
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>T</mml:mi>
       <mml:mtext>.</mml:mtext>
     </mml:mrow>
   </mml:math>
   Thus the solution can be computed from Eqn. (16.15) by setting
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>t</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>T</mml:mi>
     </mml:mrow>
   </mml:math>
   in all terms within the square brackets.
   Consequently,<mml:math mode="display" display="block">
     <mml:mrow>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mi>p</mml:mi>
       </mml:msub>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mfrac>
             <mml:msub>
               <mml:mi>f</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mrow>
               <mml:mi>m</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mfrac>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">sin</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>T</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mi>T</mml:mi>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mi mathcolor="gray">sin</mml:mi>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:msub>
                             <mml:mi>&omega;</mml:mi>
                             <mml:mn>0</mml:mn>
                           </mml:msub>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>T</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">cos</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>0</mml:mn>
                         </mml:msub>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>T</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mn>1</mml:mn>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>T</mml:mi>
                   </mml:mrow>
                 </mml:mfrac>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
         <mml:mo form="infix">+</mml:mo>
         <mml:mrow>
           <mml:mfrac>
             <mml:msub>
               <mml:mi>f</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mrow>
               <mml:mi>m</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msub>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
             </mml:mrow>
           </mml:mfrac>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mrow>
             <mml:mi mathcolor="gray">cos</mml:mi>
             <mml:mo>&ApplyFunction;</mml:mo>
             <mml:mrow>
               <mml:msub>
                 <mml:mi>&omega;</mml:mi>
                 <mml:mn>0</mml:mn>
               </mml:msub>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>t</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
                 <mml:mfrac>
                   <mml:mrow>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
                         <mml:mrow>
                           <mml:mi>T</mml:mi>
                           <mml:mo form="infix">&minus;</mml:mo>
                           <mml:mi>T</mml:mi>
                         </mml:mrow>
                         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
                       </mml:mrow>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mrow>
                         <mml:mi mathcolor="gray">cos</mml:mi>
                         <mml:mo>&ApplyFunction;</mml:mo>
                         <mml:mrow>
                           <mml:msub>
                             <mml:mi>&omega;</mml:mi>
                             <mml:mn>0</mml:mn>
                           </mml:msub>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
                         </mml:mrow>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">+</mml:mo>
                     <mml:mrow>
                       <mml:mi mathcolor="gray">sin</mml:mi>
                       <mml:mo>&ApplyFunction;</mml:mo>
                       <mml:mrow>
                         <mml:msub>
                           <mml:mi>&omega;</mml:mi>
                           <mml:mn>0</mml:mn>
                         </mml:msub>
                         <mml:mo>&InvisibleTimes;</mml:mo>
                         <mml:mi>T</mml:mi>
                       </mml:mrow>
                     </mml:mrow>
                     <mml:mo form="infix">&minus;</mml:mo>
                     <mml:mrow>
                       <mml:msub>
                         <mml:mi>&omega;</mml:mi>
                         <mml:mn>0</mml:mn>
                       </mml:msub>
                       <mml:mo>&InvisibleTimes;</mml:mo>
                       <mml:mi>T</mml:mi>
                     </mml:mrow>
                   </mml:mrow>
                   <mml:mrow>
                     <mml:msubsup>
                       <mml:mi>&omega;</mml:mi>
                       <mml:mn>0</mml:mn>
                       <mml:mn>2</mml:mn>
                     </mml:msubsup>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>T</mml:mi>
                   </mml:mrow>
                 </mml:mfrac>
                 <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>Upon
   simplification we get
</p>
<p class="QQQExampleText">
