                   <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
                   <mml:mi>t</mml:mi>
                   <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>.</mml:mtext>
           </mml:mrow>
         </mml:math>
         </p>
      </li>
   </ol>
</li>
<li>
   <p>
      Match the slope fields below to the following ODEs:<br/>(a)
      <mml:math display="inline">
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:math><br/>(b)
      <mml:math display="inline">
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mfrac>
                 <mml:mn>1</mml:mn>
                 <mml:mn>4</mml:mn>
               </mml:mfrac>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>t</mml:mi>
                 <mml:mn>2</mml:mn>
               </mml:msup>
             </mml:mrow>
           </mml:mrow>
         </mml:math><br/>(c)
      <mml:math display="inline">
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi mathcolor="gray">cos</mml:mi>
               <mml:mo>&ApplyFunction;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
           </mml:mrow>
         </mml:math><br/>(d)
      <mml:math display="inline">
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mn>2</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mrow>
                 <mml:mi>x</mml:mi>
                 <mml:mo form="infix">/</mml:mo>
                 <mml:mi>t</mml:mi>
               </mml:mrow>
             </mml:mrow>
           </mml:mrow>
         </mml:math><br/>(e)
      <mml:math display="inline">
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:msup>
                 <mml:mi>e</mml:mi>
                 <mml:mrow>
                   <mml:mo form="prefix">&minus;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:msup>
             </mml:mrow>
           </mml:mrow>
         </mml:math><br/>(f)
      <mml:math display="inline">
           <mml:mrow>
             <mml:mover accent="true">
               <mml:mi>x</mml:mi>
               <mml:mo form="postfix" accent="true">&dot;</mml:mo>
             </mml:mover>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mn>1</mml:mn>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mi>t</mml:mi>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>x</mml:mi>
               </mml:mrow>
