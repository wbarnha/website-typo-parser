                   <mml:mi mathcolor="gray">sin</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mi>t</mml:mi>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>.</mml:mtext>
           </mml:mrow>
         </mml:math>
         </p>
      </li>
      <li>
         <p>
            <mml:math display="inline">
           <mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mover accent="true">
                   <mml:mi>x</mml:mi>
                   <mml:mo form="postfix" accent="true">&dot;</mml:mo>
                 </mml:mover>
                 <mml:mo form="infix">&minus;</mml:mo>
                 <mml:mrow>
                   <mml:mn>2</mml:mn>
                   <mml:mo>&InvisibleTimes;</mml:mo>
                   <mml:mi>x</mml:mi>
                 </mml:mrow>
               </mml:mrow>
               <mml:mo form="infix">=</mml:mo>
               <mml:mrow>
                 <mml:msup>
                   <mml:mi>e</mml:mi>
                   <mml:mrow>
                     <mml:mo form="prefix">&minus;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:msup>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mrow>
                   <mml:mi mathcolor="gray">cos</mml:mi>
                   <mml:mo>&ApplyFunction;</mml:mo>
                   <mml:mrow>
                     <mml:mn>2</mml:mn>
                     <mml:mo>&InvisibleTimes;</mml:mo>
                     <mml:mi>t</mml:mi>
                   </mml:mrow>
                 </mml:mrow>
               </mml:mrow>
             </mml:mrow>
             <mml:mtext>.</mml:mtext>
           </mml:mrow>
         </mml:math>
         </p></li></ol></li></ol>
