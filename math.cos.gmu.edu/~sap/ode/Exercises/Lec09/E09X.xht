         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mrow>
           <mml:mn>0</mml:mn>
           <mml:mtext>,</mml:mtext>
         </mml:mrow>
       </mml:msub>
     </mml:mrow>
   </mml:math>
      <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>a</mml:mi>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:msub>
           <mml:mi>x</mml:mi>
           <mml:mn>0</mml:mn>
         </mml:msub>
         <mml:mo form="infix">&lt;</mml:mo>
         <mml:mi>b</mml:mi>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
      satisfies
      <mml:math display="inline">
