         </mml:mrow>
       </mml:mrow>
       <mml:mtext>,</mml:mtext>
     </mml:mrow>
   </mml:math>
      <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi>x</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
           <mml:mn>0</mml:mn>
           <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:msub>
         <mml:mi>x</mml:mi>
         <mml:mn>0</mml:mn>
       </mml:msub>
     </mml:mrow>
   </mml:math>
      has <strong>finite</strong> <strong>escape time</strong>; i.e., there is some
      time
      <mml:math display="inline">
     <mml:mrow>
       <mml:msub>
         <mml:mi>t</mml:mi>
         <mml:mo form="infix">&midast;</mml:mo>
       </mml:msub>
       <mml:mo form="infix">></mml:mo>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
      so that
      <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:munder>
           <mml:mo mathcolor="gray" form="prefix" movablelimits="true">lim</mml:mo>
           <mml:mrow>
             <mml:mi>t</mml:mi>
             <mml:mo form="infix">&rarr;</mml:mo>
             <mml:msub>
               <mml:mi>t</mml:mi>
               <mml:mo form="infix">&midast;</mml:mo>
             </mml:msub>
           </mml:mrow>
         </mml:munder>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
             <mml:mi>t</mml:mi>
             <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
           </mml:mrow>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>&infin;</mml:mi>
     </mml:mrow>
   </mml:math>.
      [Compare the given IVP to
      <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mover accent="true">
           <mml:mi>x</mml:mi>
           <mml:mo form="postfix" accent="true">&dot;</mml:mo>
