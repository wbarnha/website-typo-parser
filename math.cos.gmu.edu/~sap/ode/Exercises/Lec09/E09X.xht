               <mml:mn>0</mml:mn>
             </mml:msub>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:msub>
               <mml:mi>r</mml:mi>
               <mml:mn>0</mml:mn>
             </mml:msub>
           </mml:mrow>
           <mml:mo form="infix">,</mml:mo>
           <mml:mrow>
