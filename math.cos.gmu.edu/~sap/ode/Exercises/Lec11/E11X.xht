                         <mml:mrow>
                           <mml:msub>
                             <mml:mi>&lambda;</mml:mi>
                             <mml:mn>2</mml:mn>
                           </mml:msub>
                           <mml:mo>&InvisibleTimes;</mml:mo>
                           <mml:mi>t</mml:mi>
