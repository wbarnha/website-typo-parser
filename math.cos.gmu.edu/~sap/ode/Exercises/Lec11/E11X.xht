               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:msub>
                   <mml:mi>q</mml:mi>
                   <mml:mn>0</mml:mn>
                 </mml:msub>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>x</mml:mi>
